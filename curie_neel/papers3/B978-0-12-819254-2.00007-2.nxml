                                 <mml:mi>y</mml:mi>
                                 <mml:mo>=</mml:mo>
                                 <mml:mo>â</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>b</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>e</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mi mathvariant="normal">Î</mml:mi>
                                             <mml:mi>y</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>V</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>A</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mi>G</mml:mi>
                                    <mml:mo>Â¯</mml:mo>
                                 </mml:mover>
                                 <mml:mo>,</mml:mo>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0245" view="all">leading to,<ce:display>
                           <ce:formula id="eqn18">
                              <ce:label>(4.18)</ce:label>
                              <mml:math altimg="si119.svg">
