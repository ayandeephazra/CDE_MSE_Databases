                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>A</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                              </mml:mrow>
                           </mml:mfrac>
                        </mml:math>. This yields:<ce:display>
                           <ce:formula id="eqn17">
                              <ce:label>(4.17)</ce:label>
                              <mml:math altimg="si118.svg">
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>F</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>y</mml:mi>
                                                   <mml:mo>_</mml:mo>
                                                   <mml:mi>o</mml:mi>
                                                   <mml:mi>s</mml:mi>
                                                   <mml:mi>m</mml:mi>
                                                   <mml:mi>o</mml:mi>
