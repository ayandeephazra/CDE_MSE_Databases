                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi>k</mml:mi>
                                       <mml:mi>T</mml:mi>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>b</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
