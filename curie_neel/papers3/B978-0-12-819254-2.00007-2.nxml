                                 <mml:mi>l</mml:mi>
                                 <mml:mi>o</mml:mi>
                                 <mml:mi>g</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mi>c</mml:mi>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>c</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
