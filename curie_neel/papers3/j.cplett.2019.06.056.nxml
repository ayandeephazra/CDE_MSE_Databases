                     <ce:display>
                        <ce:figure id="f0030">
                           <ce:link locator="ga1" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" id="lk005" xlink:href="pii:S0009261419305305/ga1"/>
                        </ce:figure>
