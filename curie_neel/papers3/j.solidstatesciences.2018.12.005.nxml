                           <mml:msub>
                              <mml:mrow>
                                 <mml:mi>k</mml:mi>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mi>x</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo>=</mml:mo>
                           <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mo>−</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mo>/</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>x</mml:mi>
                                 </mml:mrow>
