                                 <mml:mi>c</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo>=</mml:mo>
                           <mml:mn>2.448</mml:mn>
                           <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>4</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                           </mml:mrow>
