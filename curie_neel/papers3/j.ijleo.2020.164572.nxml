                           <mml:mn>2</mml:mn>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mn>2.009</mml:mn>
                     </mml:math> and <mml:math altimg="si31.svg">
                        <mml:msub>
                           <mml:mi>C</mml:mi>
                           <mml:mn>3</mml:mn>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mo>−</mml:mo>
                        <mml:mn>4.194</mml:mn>
                        <mml:mo>×</mml:mo>
