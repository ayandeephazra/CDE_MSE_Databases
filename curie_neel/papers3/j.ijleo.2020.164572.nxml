                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:msup>
                                 <mml:mi>e</mml:mi>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>α</mml:mi>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo>,</mml:mo>
