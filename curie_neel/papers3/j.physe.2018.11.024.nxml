                        <mml:mfrac>
                           <mml:mrow>
                              <mml:mi mathvariant="normal">Δ</mml:mi>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
