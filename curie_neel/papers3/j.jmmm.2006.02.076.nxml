            </ce:figure>
         </ce:floats>
         <head>
            <ce:title>Temperature dependence of the Faraday rotation in diluted magnetic semiconductors Cd<ce:inf loc="post">1−</ce:inf>
               <ce:italic>
                  <ce:inf loc="post">x</ce:inf>
               </ce:italic>
               <ce:inf loc="post">−</ce:inf>
               <ce:italic>
                  <ce:inf loc="post">y</ce:inf>
               </ce:italic>Mn<ce:italic>
                  <ce:inf loc="post">x</ce:inf>
               </ce:italic>Zn<ce:italic>
                  <ce:inf loc="post">y</ce:inf>
               </ce:italic>Te crystals</ce:title>
            <ce:author-group>
