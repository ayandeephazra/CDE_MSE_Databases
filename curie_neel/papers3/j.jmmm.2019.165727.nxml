                                 </mml:mrow>
                              </mml:msub>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi mathvariant="normal">NO</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> monolayer, the <mml:math altimg="si118.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>E</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
