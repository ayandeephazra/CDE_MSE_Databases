                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> and <mml:math altimg="si147.svg">
