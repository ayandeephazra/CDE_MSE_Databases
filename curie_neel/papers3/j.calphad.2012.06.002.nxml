                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi>e</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:mi>Δ</mml:mi>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>V</mml:mi>
                                                      <mml:mi>a</mml:mi>
                                                      <mml:mi>c</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo>/</mml:mo>
                                                <mml:mi>R</mml:mi>
                                                <mml:mi>T</mml:mi>
                                             </mml:mrow>
                                          </mml:msup>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">︸</mml:mo>
                                       </mml:mrow>
                                    </mml:munder>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>h</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>t</mml:mi>
                                    <mml:mi>t</mml:mi>
                                    <mml:mi>y</mml:mi>
                                    <mml:mspace width=".5em"/>
                                    <mml:mi>d</mml:mi>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>f</mml:mi>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>t</mml:mi>
                                    <mml:mi>s</mml:mi>
                                    <mml:mspace width=".5em"/>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>t</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>b</mml:mi>
                                    <mml:mi>u</mml:mi>
                                    <mml:mi>t</mml:mi>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>s</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si0046.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>x</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>D</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>θ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                        <mml:mo>/</mml:mo>
                        <mml:mi>T</mml:mi>
                     </mml:math>, <ce:italic>N</ce:italic> is the number of atoms per molecular formula, <ce:italic>R</ce:italic> is the gas constant, <ce:italic>σ</ce:italic> is the thermal pressure parameter<ce:cross-out>s</ce:cross-out> defined as <mml:math altimg="si0047.gif" overflow="scroll">
                        <mml:mi>σ</mml:mi>
                        <mml:mo>=</mml:mo>
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>α</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>V</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>T</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                     </mml:math> which is approximately the average value of <mml:math altimg="si0048.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>α</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> for <ce:italic>T</ce:italic>&gt;<mml:math altimg="si0049.gif" overflow="scroll">
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>θ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                     </mml:math> and <mml:math altimg="si0050.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mo>−</mml:mo>
                        <mml:mi>V</mml:mi>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>∂</mml:mi>
                              <mml:mi>P</mml:mi>
                              <mml:mo>/</mml:mo>
                              <mml:mi>∂</mml:mi>
                              <mml:mi>V</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> is the isothermal bulk modulus which is linked to the adiabatic bulk modulus according to <mml:math altimg="si0051.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>S</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mn>1</mml:mn>
                        <mml:mo>+</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>α</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mi>γ</mml:mi>
                        <mml:mi>T</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                     </mml:math>. <ce:italic>e</ce:italic> corresponds to the anharmonic contributions to the heat capacity. Then let <mml:math altimg="si0052.gif" overflow="scroll">
                        <mml:msup>
                           <mml:mrow>
                              <mml:mi>σ</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msup>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>m</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>θ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                        <mml:mo stretchy="false">)</mml:mo>
                        <mml:mo>/</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>θ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                        <mml:mo stretchy="false">)</mml:mo>
                        <mml:mo>+</mml:mo>
                        <mml:mi>e</mml:mi>
                        <mml:mo>=</mml:mo>
                        <mml:mi>g</mml:mi>
                     </mml:math>, <mml:math altimg="si0053.gif" overflow="scroll">
                        <mml:mi mathvariant="normal">Δ</mml:mi>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>c</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mi mathvariant="normal">Δ</mml:mi>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>E</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>c</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>−</mml:mo>
                        <mml:mi>T</mml:mi>
                        <mml:mi mathvariant="normal">Δ</mml:mi>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>S</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>c</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> is the Gibbs energy of formation of vacancies, where Δ<ce:italic>E</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>Vac</ce:italic>
                     </ce:inf> is the formation energy of a mole of vacancy and Δ<ce:italic>S</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>Vac</ce:italic>
                     </ce:inf> is an entropy related to changes in the vibrational spectrum of the lattice. For instance, we fixed the value of Δ<ce:italic>S</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>Vac</ce:italic>
                     </ce:inf> to the standard value 3<ce:italic>NR</ce:italic> 
                     <ce:cross-ref refid="bib28">[28]</ce:cross-ref>.</ce:para>
                  <ce:para id="p0080" view="all">The thermal expansion, <mml:math altimg="si0054.gif" overflow="scroll">
                        <mml:msubsup>
                           <mml:mrow>
                              <mml:mi>α</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>v</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>n</mml:mi>
                              <mml:mi>m</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>g</mml:mi>
                           </mml:mrow>
                        </mml:msubsup>
                     </mml:math>, and the heat capacity are strongly correlated, and they can be described by the same set of parameters <ce:cross-refs refid="bib29 bib30">[29,30]</ce:cross-refs>:<ce:display>
                        <ce:formula id="eq0060">
                           <ce:label>(12)</ce:label>
                           <mml:math altimg="si0055.gif" overflow="scroll">
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>α</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>v</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>m</mml:mi>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>g</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>T</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:mi>A</mml:mi>
                              <mml:mn>9</mml:mn>
                              <mml:mi>N</mml:mi>
                              <mml:mi>R</mml:mi>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mi>T</mml:mi>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>θ</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>D</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>3</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:msubsup>
                                 <mml:mo>∫</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>x</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>D</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mrow>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>x</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>4</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>e</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>x</mml:mi>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mi>d</mml:mi>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:msup>
                                                <mml:mrow>
                                                   <mml:mi>e</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>x</mml:mi>
                                                </mml:mrow>
                                             </mml:msup>
                                             <mml:mo>−</mml:mo>
                                             <mml:mn>1</mml:mn>
                                             <mml:mo stretchy="false">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo>+</mml:mo>
                                 <mml:mi>B</mml:mi>
                                 <mml:mi>T</mml:mi>
                                 <mml:mo>+</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>V</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>a</mml:mi>
                                             <mml:mi>c</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>a</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>3</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi mathvariant="normal">Δ</mml:mi>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>E</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>a</mml:mi>
                                             <mml:mi>c</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>R</mml:mi>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mi>e</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi mathvariant="normal">Δ</mml:mi>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>G</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>V</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>c</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mo>/</mml:mo>
                                       <mml:mrow>
                                          <mml:mi>R</mml:mi>
                                          <mml:mi>T</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:msup>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si0056.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>c</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> is the formation volume of a vacancy, and <mml:math altimg="si0057.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>V</mml:mi>
                              <mml:mi>a</mml:mi>
                              <mml:mi>c</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>/</mml:mo>
                        <mml:msup>
                           <mml:mrow>
                              <mml:mi>a</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>3</mml:mn>
                           </mml:mrow>
                        </mml:msup>
                        <mml:mo>≈</mml:mo>
                        <mml:mn>1</mml:mn>
                        <mml:mo>/</mml:mo>
                        <mml:mn>2</mml:mn>
                     </mml:math>. <ce:italic>A</ce:italic> and <ce:italic>B</ce:italic> are related to the cohesive energy and bulk modulus.</ce:para>
                  <ce:para id="p0085" view="all">Assuming that the main change of adiabatic bulk modulus with temperature arises from the associated volume change we can write:<ce:display>
                        <ce:formula id="eq0065">
                           <ce:label>(13)</ce:label>
                           <mml:math altimg="si0058.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>T</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>+</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mi>∂</mml:mi>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>B</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>S</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>∂</mml:mi>
                                             <mml:mi>V</mml:mi>
                                          </mml:mrow>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>T</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mi mathvariant="normal">Δ</mml:mi>
                              <mml:mi>V</mml:mi>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0090" view="all">According to the Anderson–Grüneisen theory of quasi-harmonic solids <ce:cross-refs refid="bib31 bib32 bib33">[31–33]</ce:cross-refs>, the quantities <mml:math altimg="si0059.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>∂</mml:mi>
                              <mml:mspace width=".25em"/>
                              <mml:mi>ln</mml:mi>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>/</mml:mo>
                              <mml:mi>∂</mml:mi>
                              <mml:mspace width=".25em"/>
                              <mml:mi>ln</mml:mi>
                              <mml:mi>V</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> are constants of the materials. These last quantities, often denoted <ce:italic>δ</ce:italic>, are represented by the so-called Anderson Grüneisen parameter and can be approximated as:<mml:math altimg="si0060.gif" overflow="scroll">
                        <mml:mi>δ</mml:mi>
                        <mml:mo>≈</mml:mo>
                        <mml:mn>2</mml:mn>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>γ</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>0</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>−</mml:mo>
                        <mml:mi>z</mml:mi>
                     </mml:math>, where <ce:italic>a</ce:italic> is a constant which can take the values 0 or −2/3. Consequently, Eq. <ce:cross-ref refid="eq0065">(13)</ce:cross-ref> can be approximated as:<ce:display>
                        <ce:formula id="eq0070">
                           <ce:label>(14)</ce:label>
                           <mml:math altimg="si0061.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>T</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>[</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mo>+</mml:mo>
                              <mml:mi>δ</mml:mi>
                              <mml:mi mathvariant="normal">Δ</mml:mi>
                              <mml:mi>V</mml:mi>
                              <mml:mo>/</mml:mo>
                              <mml:mi>V</mml:mi>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>]</mml:mo>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0095" view="all">The Grüneisen equation of state leads to a volume change <mml:math altimg="si0062.gif" overflow="scroll">
                        <mml:mi mathvariant="normal">Δ</mml:mi>
                        <mml:mi>V</mml:mi>
                        <mml:mo>=</mml:mo>
                        <mml:mo>−</mml:mo>
                        <mml:mo>[</mml:mo>
                        <mml:mi>γ</mml:mi>
                        <mml:mo>/</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>B</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>S</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mn>0</mml:mn>
                        <mml:mo stretchy="false">)</mml:mo>
                        <mml:mo>]</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>E</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                              <mml:mi>h</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math>. <mml:math altimg="si0063.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>E</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                              <mml:mi>h</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:msubsup>
                           <mml:mo>∫</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mi>T</mml:mi>
                        </mml:msubsup>
                        <mml:mrow>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mi>C</mml:mi>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mi>V</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mi>T</mml:mi>
                           <mml:mo>′</mml:mo>
                           <mml:mo stretchy="false">)</mml:mo>
                           <mml:mi>d</mml:mi>
                           <mml:mi>T</mml:mi>
                           <mml:mo>′</mml:mo>
                        </mml:mrow>
                     </mml:math>. Thus the temperature dependence of the adiabatic bulk modulus is formulated as:<ce:display>
                        <ce:formula id="eq0075">
                           <ce:label>(15)</ce:label>
                           <mml:math altimg="si0064.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>T</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>−</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>γ</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>γ</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>z</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>V</mml:mi>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mn>0</mml:mn>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:msubsup>
                                 <mml:mo>∫</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mi>T</mml:mi>
                              </mml:msubsup>
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mi>C</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>V</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mi>T</mml:mi>
                                 <mml:mo>′</mml:mo>
                                 <mml:mo stretchy="false">)</mml:mo>
                                 <mml:mi>d</mml:mi>
                                 <mml:mi>T</mml:mi>
                                 <mml:mo>′</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0100" view="all">According to Eqs. <ce:cross-refs refid="eq0025 eq0030 eq0035 eq0040 eq0045">(5)–(9)</ce:cross-refs>, the thermal conductivity of the insulators can be correlated with the equilibrium volume at standard temperature, the thermal expansion, heat capacity and bulk modulus as a function of temperature.</ce:para>
               </ce:section>
               <ce:section id="s0020" view="all">
                  <ce:label>4</ce:label>
                  <ce:section-title>A simple statistical model for the prediction of <mml:math altimg="si0065.gif" overflow="scroll">
                        <mml:mover accent="true">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>θ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo stretchy="true">¯</mml:mo>
                           </mml:mrow>
                        </mml:mover>
                        <mml:mo stretchy="false">(</mml:mo>
                        <mml:mi>∞</mml:mi>
                        <mml:mo stretchy="false">)</mml:mo>
                     </mml:math> and <ce:italic>γ</ce:italic>(<ce:italic>∞</ce:italic>)</ce:section-title>
                  <ce:para id="p0105" view="all">For many compounds, due to the lack of experimental data, some physical properties need to be estimated in order to be able to predict the thermal conductivity as a function of temperature. Moreover, according to the CALPHAD method, the thermal conductivity model for solutions must be consistent with the Compound Energy Formalism <ce:cross-ref refid="bib34">[34]</ce:cross-ref> (in this formalism, ions or atoms are distributed over various sublattices, and all combinations of these ions or atoms – including the hypothetical compounds – must be considered as end-members of the solution; hence their thermal conductivity should be estimated). In particular, all physical properties of the hypothetical compounds will have to be assessed in order to provide a reasonable estimation of the thermal conductivity of the corresponding solid solution. It is possible to base our estimation only on first principle calculations. However, the latter are very time-consuming and, moreover, their reliability in the case of complex hypothetical compounds is not obvious (in particular, for thermal properties). This is due to the numerous approximations made to calculate the phonon spectrum.</ce:para>
                  <ce:para id="p0110" view="all">The so-called “in silico” approach allows to determination of the thermodynamic properties of phases without any experimental information. This is achieved by coupling energy calculations from ab initio approaches to entropy calculations from statistical physics, for example. In our cases, we will consider the <ce:italic>classical statistical thermodynamic</ce:italic> results for calculating <ce:italic>Θ</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>D</ce:italic>
                     </ce:inf>(<ce:italic>∞</ce:italic>) and <ce:italic>γ</ce:italic>(<ce:italic>∞</ce:italic>) from the methodology detailed below. This method is based on the evaluation of the partition function of the system. The data necessary to formulate the partition function are only the equilibrium volume and the cohesive energy.</ce:para>
                  <ce:para id="p0115" view="all">The thermal properties of solids can be derived from the canonical partition function of a harmonic oscillator <mml:math altimg="si0066.gif" overflow="scroll">
                        <mml:msubsup>
                           <mml:mrow>
                              <mml:mi mathvariant="bold">Σ</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>m</mml:mi>
                              <mml:mo>=</mml:mo>
                              <mml:mn>0</mml:mn>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>∞</mml:mi>
                           </mml:mrow>
                        </mml:msubsup>
                        <mml:msup>
                           <mml:mrow>
                              <mml:mi>e</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mo>−</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>E</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>m</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>/</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mi>T</mml:mi>
                           </mml:mrow>
                        </mml:msup>
                     </mml:math>. The vibrational energy <ce:italic>E</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>n</ce:italic>
                     </ce:inf>, with the principal quantum number <ce:italic>n</ce:italic> of this oscillator, can be expressed as a sum of a harmonic and an anharmonic contributions:<ce:display>
                        <ce:formula id="eq0080">
                           <ce:label>(16)</ce:label>
                           <mml:math altimg="si0067.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>E</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>n</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>=</mml:mo>
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:munder accentunder="true">
                                       <mml:mrow>
                                          <mml:mi>h</mml:mi>
                                          <mml:mi>υ</mml:mi>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:mi>n</mml:mi>
                                          <mml:mo>+</mml:mo>
                                          <mml:mn>1</mml:mn>
                                          <mml:mo>/</mml:mo>
                                          <mml:mn>2</mml:mn>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">︸</mml:mo>
                                       </mml:mrow>
                                    </mml:munder>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>H</mml:mi>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>m</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>c</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                              <mml:mo>−</mml:mo>
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:munder accentunder="true">
                                       <mml:mrow>
                                          <mml:mo>{</mml:mo>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mo stretchy="false">(</mml:mo>
                                                <mml:mi>h</mml:mi>
                                                <mml:mi>υ</mml:mi>
                                                <mml:mo stretchy="false">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:mo>/</mml:mo>
                                          <mml:mn>4</mml:mn>
                                          <mml:mi>E</mml:mi>
                                          <mml:mi>c</mml:mi>
                                          <mml:mo>}</mml:mo>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mo stretchy="false">(</mml:mo>
                                                <mml:mi>n</mml:mi>
                                                <mml:mo>+</mml:mo>
                                                <mml:mn>1</mml:mn>
                                                <mml:mo>/</mml:mo>
                                                <mml:mn>2</mml:mn>
                                                <mml:mo stretchy="false">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msup>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">︸</mml:mo>
                                       </mml:mrow>
                                    </mml:munder>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>A</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>h</mml:mi>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>m</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>c</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <ce:italic>Ec</ce:italic> is the cohesive energy. The material is assumed to be composed of independent oscillators that are vibrating with a frequency distribution given by the Debye approximation. Under these conditions, Ruffa <ce:cross-refs refid="bib35 bib36 bib37 bib38 bib39">[35–39]</ce:cross-refs> has shown that the Grüneisen parameter as a function of the temperature can be approximated as follows:<ce:display>
                        <ce:formula id="eq0085">
                           <ce:label>(17)</ce:label>
                           <mml:math altimg="si0068.gif" overflow="scroll">
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>γ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>*</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>∞</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:mi>lim</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>x</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>D</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mspace width="0.25em"/>
                                    <mml:mo>→</mml:mo>
                                    <mml:mspace width="0.25em"/>
                                    <mml:mn>0</mml:mn>
                                 </mml:mrow>
                              </mml:munder>
                              <mml:mspace width=".5em"/>
                              <mml:mo>[</mml:mo>
                              <mml:mn>25</mml:mn>
                              <mml:mi>m</mml:mi>
                              <mml:mo>/</mml:mo>
                              <mml:mn>9</mml:mn>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>m</mml:mi>
                              <mml:mo>+</mml:mo>
                              <mml:mn>4</mml:mn>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>]</mml:mo>
                              <mml:mo>/</mml:mo>
                              <mml:mo>[</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mo>+</mml:mo>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mi>T</mml:mi>
