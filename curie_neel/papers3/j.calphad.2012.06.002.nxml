                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>x</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>D</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>−</mml:mo>
