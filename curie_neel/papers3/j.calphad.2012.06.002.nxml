                                    <mml:munder accentunder="true">
                                       <mml:mrow>
                                          <mml:mi>N</mml:mi>
                                          <mml:mi>R</mml:mi>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:mi>Δ</mml:mi>
