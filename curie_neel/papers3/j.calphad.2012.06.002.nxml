                              </mml:msub>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>]</mml:mo>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <ce:sup loc="post">*</ce:sup> denotes the estimated properties and <ce:italic>F</ce:italic>(<ce:italic>x</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>D</ce:italic>
                     </ce:inf>) is defined as:<ce:display>
                        <ce:formula id="eq0090">
