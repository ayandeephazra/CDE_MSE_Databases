                                 <mml:mi>x</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>ω</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si18.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>ε</mml:mi>
                              <mml:mrow>
                                 <mml:mi>z</mml:mi>
                                 <mml:mi>z</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>ω</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> along the <mml:math altimg="si19.svg">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mi>a</mml:mi>
                              <mml:mo linebreak="badbreak" stretchy="true">→</mml:mo>
