                              <mml:mi>l</mml:mi>
                              <mml:mo>=</mml:mo>
                              <mml:mi>A</mml:mi>
                              <mml:mo>,</mml:mo>
                              <mml:mi>B</mml:mi>
                              <mml:mo>,</mml:mo>
                              <mml:mi>C</mml:mi>
                           </mml:math>
                        </ce:formula>
                     </ce:display>In Eq. <ce:cross-ref id="crf0065" refid="eq0005">(1)</ce:cross-ref>, <mml:math altimg="si18.svg">
                        <mml:msub>
                           <mml:mi mathvariant="fraktur">T</mml:mi>
