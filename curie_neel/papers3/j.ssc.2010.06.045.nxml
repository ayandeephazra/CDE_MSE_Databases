                                    <mml:mi>q</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mrow>
                                 <mml:mo>(</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mi>S</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mi>S</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>y</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
