                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>z</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>μ</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mstyle mathvariant="bold-italic">
                                 <mml:mi>B</mml:mi>
                              </mml:mstyle>
                              <mml:mo>⋅</mml:mo>
                              <mml:mstyle mathvariant="bold-italic">
                                 <mml:mi>g</mml:mi>
                              </mml:mstyle>
