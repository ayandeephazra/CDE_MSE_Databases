                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>̄</mml:mo>
                                 </mml:mrow>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>4</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mrow>
