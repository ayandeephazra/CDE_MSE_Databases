                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>O</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mo>+</mml:mo>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>O</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mo>+</mml:mo>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>B</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>O</mml:mi>
