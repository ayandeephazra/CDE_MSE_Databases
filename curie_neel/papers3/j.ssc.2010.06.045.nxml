                                 <mml:mi>S</mml:mi>
                              </mml:mstyle>
                              <mml:mo>+</mml:mo>
                              <mml:mo>∑</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>f</mml:mi>
