               </ce:keyword>
               <ce:keyword id="k0010">
                  <ce:text>Photonic quasicrystals</ce:text>
               </ce:keyword>
               <ce:keyword id="k0015">
                  <ce:text>Photonic band gap</ce:text>
               </ce:keyword>
               <ce:keyword id="k0020">
