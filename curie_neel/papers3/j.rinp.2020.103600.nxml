                                          </mml:msubsup>
                                          <mml:msup>
                                             <mml:mo>cos</mml:mo>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mi>ϕ</mml:mi>
                                       </mml:mrow>
