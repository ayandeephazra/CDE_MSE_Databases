                                             <mml:mi>n</mml:mi>
                                             <mml:mi>o</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msubsup>
                                          <mml:msup>
                                             <mml:mo>sin</mml:mo>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
