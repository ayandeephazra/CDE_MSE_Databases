                           <mml:math altimg="si10.svg">
                              <mml:mrow>
                                 <mml:mi mathvariant="normal">Δ</mml:mi>
                                 <mml:msub>
                                    <mml:mi>H</mml:mi>
                                    <mml:mi>f</mml:mi>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mi>E</mml:mi>
                                 <mml:mrow>
                                    <mml:mfenced close=")" open="(">
                                       <mml:mrow>
                                          <mml:msub>
