                                                         <mml:mrow>
                                                            <mml:mi>E</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mi mathvariant="normal">ln</mml:mi>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>n</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>f</mml:mi>
                                                            </mml:mrow>
