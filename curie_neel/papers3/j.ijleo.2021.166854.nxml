                                    <mml:mi>f</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mi mathvariant="normal">exp</mml:mi>
                              <mml:mrow>
                                 <mml:mfenced open="(" close=")">
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mi>x</mml:mi>
