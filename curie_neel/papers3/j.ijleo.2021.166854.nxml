                                       <mml:mi mathvariant="normal">ln</mml:mi>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>n</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
