                                    </mml:mfrac>
                                    <mml:mo class="qopname">sin</mml:mo>
                                    <mml:mfenced open="(" close=")">
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mi>π</mml:mi>
