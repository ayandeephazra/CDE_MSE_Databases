                                 </mml:mover>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mi>c</mml:mi>
                                 <mml:mo>,</mml:mo>
                                 <mml:mi>v</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo>(</mml:mo>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>→</mml:mo>
                              </mml:mover>
                              <mml:mo>)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> is determined as follows <ce:display>
                        <ce:formula id="fd2">
                           <ce:label>(2)</ce:label>
                           <mml:math display="block" id="d1e1462" altimg="si47.svg">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>p</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>→</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>c</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>v</mml:mi>
                                    </mml:mrow>
