                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>c</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo linebreak="badbreak">∗</mml:mo>
                                    </mml:mrow>
                                 </mml:msubsup>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
