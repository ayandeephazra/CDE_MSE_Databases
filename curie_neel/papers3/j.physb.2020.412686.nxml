                                             </mml:mrow>
                                             <mml:mo>→</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>v</mml:mi>
                                          <mml:mo>,</mml:mo>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>0</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
