                                 </mml:mrow>
                                 <mml:mo>→</mml:mo>
                              </mml:mover>
                              <mml:mo>)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">&gt;</mml:mo>
                           <mml:mn>0</mml:mn>
