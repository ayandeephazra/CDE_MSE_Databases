                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:mi>V</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mi>e</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>i</mml:mi>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>k</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>→</mml:mo>
                                       </mml:mover>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>r</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>→</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:mi>V</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mi>e</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>i</mml:mi>
