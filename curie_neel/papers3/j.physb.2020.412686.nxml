                                       <mml:mn>2</mml:mn>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:munder>
                                    <mml:mrow>
                                       <mml:mo linebreak="badbreak">∑</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mrow>
