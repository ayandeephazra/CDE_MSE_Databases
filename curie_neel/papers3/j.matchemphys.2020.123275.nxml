                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mtext>V</mml:mtext>
                                       <mml:mtext>i</mml:mtext>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mrow>
                                 <mml:mo stretchy="true">[</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mtext>m</mml:mtext>
                                       <mml:mtext>i</mml:mtext>
                                    </mml:msub>
                                    <mml:msubsup>
                                       <mml:mtext>v</mml:mtext>
                                       <mml:mi mathvariant="normal">σ</mml:mi>
                                       <mml:mtext>i</mml:mtext>
                                    </mml:msubsup>
                                    <mml:msubsup>
                                       <mml:mtext>v</mml:mtext>
                                       <mml:mi mathvariant="normal">β</mml:mi>
