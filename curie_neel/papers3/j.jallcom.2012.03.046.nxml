                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>N.</ce:given-name>
                  <ce:surname>Korozlu</ce:surname>
                  <ce:cross-ref refid="aff0010">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
