                                       <mml:msub>
                                          <mml:mi>t</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msub>
                                       <mml:mn>...</mml:mn>
                                       <mml:msub>
                                          <mml:mi>t</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>m</mml:mi>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mi>a</mml:mi>
