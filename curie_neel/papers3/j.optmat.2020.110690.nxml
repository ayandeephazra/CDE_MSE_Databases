                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd19">
                           <ce:label>(19)</ce:label>
                           <mml:math altimg="si56.svg" alttext="Equation 19.">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>p</mml:mi>
                                 </mml:msub>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
