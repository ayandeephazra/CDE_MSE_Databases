                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mi>c</mml:mi>
                                          </mml:mtd>
                                          <mml:mtd columnalign="left">
                                             <mml:mi>d</mml:mi>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
