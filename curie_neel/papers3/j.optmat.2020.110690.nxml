                                 <mml:mspace width="0.25em"/>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>n</mml:mi>
                                                <mml:mo>ˆ</mml:mo>
                                             </mml:mover>
                                             <mml:mrow>
                                                <mml:mi>m</mml:mi>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:mn>1</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>cos</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:msub>
                                             <mml:mi>θ</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>m</mml:mi>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:mn>1</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>n</mml:mi>
                                                <mml:mo>ˆ</mml:mo>
                                             </mml:mover>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                          <mml:mspace width="0.25em"/>
