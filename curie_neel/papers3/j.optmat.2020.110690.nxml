                                 <mml:mfrac>
                                    <mml:msubsup>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>m</mml:mi>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mn>1</mml:mn>
                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                    </mml:msubsup>
                                    <mml:msubsup>
                                       <mml:mi>E</mml:mi>
                                       <mml:mn>0</mml:mn>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
