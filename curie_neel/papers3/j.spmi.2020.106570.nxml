                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where<mml:math altimg="si7.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>E</mml:mi>
                              <mml:mrow>
                                 <mml:mi>M</mml:mi>
