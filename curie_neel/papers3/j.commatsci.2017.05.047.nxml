                                    <mml:mo>=</mml:mo>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>Q</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">first</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>p</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo>+</mml:mo>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>Q</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>sec</mml:mtext>
                                          <mml:mi mathvariant="italic">ond</mml:mi>
                                       </mml:mrow>
