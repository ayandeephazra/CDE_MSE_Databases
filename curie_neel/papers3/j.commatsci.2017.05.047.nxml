                                                         <mml:mi mathvariant="italic">ond</mml:mi>
                                                         <mml:mo stretchy="false">)</mml:mo>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mo>-</mml:mo>
                                                         <mml:mn>1</mml:mn>
                                                      </mml:mrow>
                                                   </mml:msup>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>E</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="italic">is</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo stretchy="false">)</mml:mo>
