                                 <ce:hsp sp="0.25"/>=<ce:hsp sp="0.25"/>maximum number of iteration</ce:para>
                           </ce:list-item>
                           <ce:list-item id="o0050">
                              <ce:para id="p0175" view="all">
                                 <mml:math altimg="si71.gif" overflow="scroll">
                                    <mml:mrow>
