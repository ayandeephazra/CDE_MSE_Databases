                                 <mml:mrow>
                                    <mml:mn>0</mml:mn>
                                    <mml:mi>m</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>=</mml:mo>
                              <mml:mn>1</mml:mn>
                              <mml:mtext>,</mml:mtext>
                              <mml:mo>â</mml:mo>
                              <mml:mtext>m</mml:mtext>
                           </mml:mrow>
                        </mml:math>.</ce:para>
                     <ce:para id="p0085" view="all">Eq. <ce:cross-ref refid="e0060" id="c0040">(12a)</ce:cross-ref> gives the modified intermediate output using Taylor expansion which leads to increments presented in Eq. <ce:cross-ref refid="e0065" id="c0045">(12b)</ce:cross-ref> where <mml:math altimg="si31.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:mi>Î±</mml:mi>
                           </mml:mrow>
                        </mml:math> represents a relaxation factor<ce:display>
                           <ce:formula id="e0060">
                              <ce:label>(12a)</ce:label>
                              <mml:math altimg="si32.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mtext>Q</mml:mtext>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mtext>p</mml:mtext>
                                    <mml:mo>+</mml:mo>
                                    <mml:mi mathvariant="normal">Î</mml:mi>
                                    <mml:mtext>p</mml:mtext>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo>=</mml:mo>
                                    <mml:mtext>Q</mml:mtext>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mtext>p</mml:mtext>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo>+</mml:mo>
                                    <mml:mstyle displaystyle="true">
                                       <mml:munderover>
                                          <mml:mrow>
                                             <mml:mo>â</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>s</mml:mi>
                                             <mml:mo>=</mml:mo>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>S</mml:mi>
                                          </mml:mrow>
                                       </mml:munderover>
                                    </mml:mstyle>
                                    <mml:mstyle displaystyle="true">
                                       <mml:munderover>
                                          <mml:mrow>
                                             <mml:mo>â</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>m</mml:mi>
                                             <mml:mo>=</mml:mo>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>M</mml:mi>
                                          </mml:mrow>
                                       </mml:munderover>
                                    </mml:mstyle>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mi>â</mml:mi>
                                          <mml:mi>Q</mml:mi>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:mi>p</mml:mi>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>â</mml:mi>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>p</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi mathvariant="italic">sm</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mi mathvariant="normal">Î</mml:mi>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>p</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">sm</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>â</mml:mo>
                                    <mml:mn>0</mml:mn>
