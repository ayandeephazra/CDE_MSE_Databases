                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msubsup>
                                             <mml:mrow>
                                                <mml:mi>f</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>z</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>â²</mml:mo>
                                                <mml:mo stretchy="false">(</mml:mo>
                                                <mml:mi mathvariant="italic">first</mml:mi>
                                                <mml:mo stretchy="false">)</mml:mo>
                                             </mml:mrow>
                                          </mml:msubsup>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>+</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:mstyle displaystyle="true">
                                       <mml:munderover>
                                          <mml:mrow>
                                             <mml:mo>â</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>i</mml:mi>
                                             <mml:mo>=</mml:mo>
                                             <mml:mn>1</mml:mn>
