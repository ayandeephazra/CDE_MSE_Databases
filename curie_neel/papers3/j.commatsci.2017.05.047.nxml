                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>i</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mo>-</mml:mo>
                                                            <mml:mn>1</mml:mn>
                                                         </mml:mrow>
                                                      </mml:msubsup>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>E</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi mathvariant="italic">im</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi mathvariant="italic">rm</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:mo>=</mml:mo>
                                                      <mml:mstyle displaystyle="true">
                                                         <mml:munderover>
                                                            <mml:mrow>
                                                               <mml:mo>â</mml:mo>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>k</mml:mi>
                                                               <mml:mo>=</mml:mo>
                                                               <mml:mn>0</mml:mn>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>K</mml:mi>
                                                            </mml:mrow>
                                                         </mml:munderover>
                                                      </mml:mstyle>
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>w</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi mathvariant="italic">ik</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:mstyle displaystyle="true">
                                                         <mml:munderover>
                                                            <mml:mrow>
                                                               <mml:mo>â</mml:mo>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>m</mml:mi>
                                                               <mml:mo>=</mml:mo>
                                                               <mml:mn>1</mml:mn>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>M</mml:mi>
                                                            </mml:mrow>
                                                         </mml:munderover>
                                                      </mml:mstyle>
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi mathvariant="italic">km</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi mathvariant="italic">rm</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:mtext>,</mml:mtext>
                                                      <mml:mi>r</mml:mi>
                                                      <mml:mo>=</mml:mo>
                                                      <mml:mn>1</mml:mn>
                                                      <mml:mtext>,</mml:mtext>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mo>.</mml:mo>
                                                      <mml:mo>.</mml:mo>
                                                      <mml:mo>.</mml:mo>
                                                      <mml:mo>.</mml:mo>
                                                      <mml:mo>.</mml:mo>
                                                      <mml:mi>K</mml:mi>
                                                      <mml:mtext>,</mml:mtext>
                                                      <mml:mo>â</mml:mo>
                                                      <mml:mi>m</mml:mi>
                                                   </mml:mrow>
                                                </mml:math>
                                             </ce:formula>
                                          </ce:display>
                                          <ce:display>
                                             <ce:formula id="e0140">
                                                <mml:math altimg="si82.gif" overflow="scroll">
                                                   <mml:mrow>
                                                      <mml:mi>Q</mml:mi>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mi>p</mml:mi>
