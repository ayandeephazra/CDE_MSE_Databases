                                       <mml:mrow>
                                          <mml:mi>m</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">xv</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>m</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>d</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>i</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo>+</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi>a</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>m</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>d</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>i</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
