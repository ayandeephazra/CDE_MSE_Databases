                                                <mml:mrow>
                                                   <mml:mi mathvariant="italic">is</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo stretchy="false">(</mml:mo>
                                                   <mml:mtext>sec</mml:mtext>
                                                   <mml:mi mathvariant="italic">ond</mml:mi>
                                                   <mml:mo stretchy="false">)</mml:mo>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:msub>
                                                <mml:mrow>
