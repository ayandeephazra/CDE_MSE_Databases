                                    <mml:msub>
                                       <mml:mi mathvariant="normal">C</mml:mi>
                                       <mml:mn>13</mml:mn>
                                    </mml:msub>
                                    <mml:mo>+</mml:mo>
                                    <mml:mfrac>
                                       <mml:msub>
