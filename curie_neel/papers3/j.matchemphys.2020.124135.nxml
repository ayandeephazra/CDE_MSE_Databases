                                       <mml:mo>,</mml:mo>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msub>
                                          <mml:mi>C</mml:mi>
                                          <mml:mi>p</mml:mi>
                                       </mml:msub>
                                       <mml:mspace width="0.25em"/>
