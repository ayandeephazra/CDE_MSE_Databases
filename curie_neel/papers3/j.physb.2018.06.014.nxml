                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi mathvariant="italic">GaAs</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mrow>
                                 <mml:mo>(</mml:mo>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:mi>T</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mn>12.74</mml:mn>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>â</mml:mo>
                                    <mml:mn>1.73</mml:mn>
                                    <mml:mo>Ã</mml:mo>
                                    <mml:mn>1</mml:mn>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                       <mml:mrow>
