                                                            <mml:mi>y</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>E</mml:mi>
                                                            <mml:mi>X</mml:mi>
                                                            <mml:mi>P</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msup>
                                                      <mml:mo>=</mml:mo>
                                                      <mml:mn>2.36948</mml:mn>
                                                      <mml:mo>−</mml:mo>
                                                      <mml:mn>0.89572</mml:mn>
                                                      <mml:mo>×</mml:mo>
                                                      <mml:mi>x</mml:mi>
                                                      <mml:mo>+</mml:mo>
                                                      <mml:mn>1.3255</mml:mn>
                                                      <mml:mo>×</mml:mo>
                                                      <mml:msup>
                                                         <mml:mrow>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mn>2</mml:mn>
                                                         </mml:mrow>
                                                      </mml:msup>
                                                   </mml:mrow>
                                                </mml:mtd>
                                                <mml:mtd/>
                                             </mml:mtr>
                                          </mml:mtable>
