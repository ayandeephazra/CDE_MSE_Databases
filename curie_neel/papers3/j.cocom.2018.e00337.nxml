                                                         </mml:mrow>
                                                      </mml:msup>
                                                      <mml:mo>=</mml:mo>
                                                      <mml:mn>2.35787</mml:mn>
                                                      <mml:mo>−</mml:mo>
                                                      <mml:mn>0.91249</mml:mn>
                                                      <mml:mo>×</mml:mo>
                                                      <mml:mi>x</mml:mi>
                                                      <mml:mo>+</mml:mo>
                                                      <mml:mn>1.20691</mml:mn>
                                                      <mml:mo>×</mml:mo>
