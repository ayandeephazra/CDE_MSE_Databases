                                                         <mml:mrow>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mn>2</mml:mn>
                                                         </mml:mrow>
                                                      </mml:msup>
                                                   </mml:mrow>
                                                </mml:mtd>
                                                <mml:mtd/>
                                             </mml:mtr>
                                             <mml:mtr>
                                                <mml:mtd>
