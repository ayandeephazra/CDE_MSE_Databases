                                                            <mml:mi>m</mml:mi>
                                                            <mml:mi>B</mml:mi>
                                                            <mml:mi>J</mml:mi>
                                                            <mml:mo>−</mml:mo>
                                                            <mml:mi>W</mml:mi>
