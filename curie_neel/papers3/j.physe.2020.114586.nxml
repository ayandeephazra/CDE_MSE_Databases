                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mi>δ</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>ε</mml:mi>
                                       </mml:mrow>
