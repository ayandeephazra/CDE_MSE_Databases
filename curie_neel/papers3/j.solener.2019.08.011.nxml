                        <ce:display>
                           <ce:formula id="e0035">
                              <ce:label>(7)</ce:label>
                              <mml:math altimg="si18.svg">
                                 <mml:mrow>
                                    <mml:mi>α</mml:mi>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
