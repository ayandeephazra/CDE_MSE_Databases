                                                      <mml:mo stretchy="false">]</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mo stretchy="false">/</mml:mo>
