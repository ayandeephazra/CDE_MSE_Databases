                        <ce:figure id="f0070">
                           <ce:alt-text id="at0085" role="short">fx1</ce:alt-text>
                           <ce:link id="lk0070" locator="fx1" xlink:type="simple" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:href="pii:S0022459617305029/fx1"/>
                        </ce:figure>
                     </ce:display>
                  </ce:simple-para>
               </ce:abstract-sec>
            </ce:abstract>
