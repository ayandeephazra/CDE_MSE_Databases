               <ce:keyword id="k0005">
                  <ce:text>Photocatalytic water splitting</ce:text>
               </ce:keyword>
               <ce:keyword id="k0010">
                  <ce:text>Two dimensional materials</ce:text>
               </ce:keyword>
               <ce:keyword id="k0015">
                  <ce:text>Density functional theory computations</ce:text>
               </ce:keyword>
               <ce:keyword id="k0020">
                  <ce:text>Niobium chalcogenide halide</ce:text>
               </ce:keyword>
            </ce:keywords>
