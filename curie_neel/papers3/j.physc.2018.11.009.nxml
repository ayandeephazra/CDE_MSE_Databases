                                       <mml:msub>
                                          <mml:mi>P</mml:mi>
                                          <mml:mi>B</mml:mi>
                                       </mml:msub>
                                       <mml:msubsup>
                                          <mml:mi mathvariant="fraktur">D</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>B</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
