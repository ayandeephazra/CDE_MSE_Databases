                                          <mml:mi mathvariant="fraktur">D</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>A</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>−</mml:mo>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:msub>
                                          <mml:mi mathvariant="fraktur">D</mml:mi>
