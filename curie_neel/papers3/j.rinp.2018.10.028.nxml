               <ce:section-title id="st005">Graphical abstract</ce:section-title>
               <ce:abstract-sec id="as005" view="all">
                  <ce:simple-para id="sp0005" view="all">
                     <ce:display>
                        <ce:figure id="f0030">
                           <ce:link locator="ga1" xlink:type="simple" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:href="pii:S2211379718319417/ga1" id="lk00030"/>
                        </ce:figure>
