                        </ce:italic>
                        <ce:inf loc="post">1</ce:inf>, [0<ce:hsp sp="0.12"/>1<ce:hsp sp="0.12"/>0]<ce:italic>v<ce:inf loc="post">t</ce:inf>
                        </ce:italic>
                        <ce:inf loc="post">2</ce:inf>, [1<ce:hsp sp="0.12"/>1<ce:hsp sp="0.12"/>0]<ce:italic>v<ce:inf loc="post">l</ce:inf>
