                        <entry xmlns="http://www.elsevier.com/xml/common/dtd">NdCoO<inf loc="post">3</inf>
                        </entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char=".">3.777</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char=".">168.7</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char=".">212.4</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd">BiScO<inf loc="post">3</inf>
                        </entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char=".">3.974</entry>
