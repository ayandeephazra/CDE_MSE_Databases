               <ce:keyword id="kwrd0010">
                  <ce:text>Metal/n-4<ce:italic>H</ce:italic>–SiC structures with zinc-doped polyvinyl alcohol</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0015">
