                                    <mml:mi>i</mml:mi>
                                    <mml:mover accent="true">
                                       <mml:mi>G</mml:mi>
                                       <mml:mo>â</mml:mo>
                                    </mml:mover>
                                    <mml:mo>Â·</mml:mo>
                                    <mml:mover accent="true">
