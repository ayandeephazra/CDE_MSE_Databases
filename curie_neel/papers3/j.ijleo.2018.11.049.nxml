                                                </mml:mrow>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mspace width="1em"/>
                                                <mml:mover accent="true">
                                                   <mml:mi>G</mml:mi>
                                                   <mml:mo>â</mml:mo>
                                                </mml:mover>
                                                <mml:mo>=</mml:mo>
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                          </mml:mtr>
                                          <mml:mtr>
                                             <mml:mtd columnalign="left">
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>Ï</mml:mi>
                                                      <mml:mi>R</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:msup>
                                                         <mml:mi>a</mml:mi>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msup>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mfenced open="(" close=")">
                                                      <mml:mrow>
                                                         <mml:mfrac>
                                                            <mml:mn>1</mml:mn>
                                                            <mml:mrow>
                                                               <mml:msub>
                                                                  <mml:mi>Ïµ</mml:mi>
                                                                  <mml:mi>GaAs</mml:mi>
                                                               </mml:msub>
                                                            </mml:mrow>
                                                         </mml:mfrac>
                                                         <mml:mo>â</mml:mo>
                                                         <mml:mn>1</mml:mn>
                                                      </mml:mrow>
                                                   </mml:mfenced>
                                                </mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>J</mml:mi>
