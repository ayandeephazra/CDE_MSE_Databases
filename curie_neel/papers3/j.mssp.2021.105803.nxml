                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>, <mml:math altimg="si72.svg">
                              <mml:mrow>
                                 <mml:mi>n</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mn>0</mml:mn>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
