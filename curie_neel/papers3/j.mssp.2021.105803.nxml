                                                         <mml:mrow>
                                                            <mml:mo stretchy="true">(</mml:mo>
                                                            <mml:mi>ω</mml:mi>
                                                            <mml:mo stretchy="true">)</mml:mo>
                                                         </mml:mrow>
                                                      </mml:mrow>
