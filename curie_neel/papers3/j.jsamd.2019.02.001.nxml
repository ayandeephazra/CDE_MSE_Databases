                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>V</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mn>0</mml:mn>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>V</mml:mi>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>B</mml:mi>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mo>+</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>V</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mn>0</mml:mn>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>V</mml:mi>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>−</mml:mo>
