            <ce:title id="title0010">Role of electronegativity on the bulk modulus, magnetic moment and band gap of Co<ce:inf loc="post">2</ce:inf>MnAl based Heusler alloys</ce:title>
            <ce:author-group id="augrp0010">
               <ce:author id="au1" author-id="S2468217919300048-634afac10e8046a8ee44176ddd61d1b7">
