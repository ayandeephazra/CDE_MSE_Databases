                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>ω</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>H</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd4">
                           <ce:label>(4)</ce:label>
                           <mml:math altimg="si7.gif" overflow="scroll">
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>ε</mml:mi>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mrow>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                             <mml:mo>⃗</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mo>∇</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mo>×</mml:mo>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mo>∇</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mo>×</mml:mo>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>ω</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <ce:italic>ω</ce:italic> is the angular frequency of the harmonic modes. Eq. <ce:cross-ref refid="fd3" id="crosref0205">(3)</ce:cross-ref> requires <mml:math altimg="si8.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mo>∇</mml:mo>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mo>⋅</mml:mo>
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>H</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mo>=</mml:mo>
                        <mml:mn>0</mml:mn>
                     </mml:math>, whereas Eq. <ce:cross-ref refid="fd4" id="crosref0210">(4)</ce:cross-ref> must be combined with <mml:math altimg="si9.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mo>∇</mml:mo>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mo>⋅</mml:mo>
                        <mml:mfenced open="(" close=")">
                           <mml:mrow>
                              <mml:mi>ε</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:mrow>
                        </mml:mfenced>
                        <mml:mo>=</mml:mo>
                        <mml:mn>0</mml:mn>
                     </mml:math>.</ce:para>
                  <ce:para id="p0050" view="all">A periodic dielectric medium is invariant under translations by vectors <mml:math altimg="si10.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>R</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mo>=</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>l</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>+</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>l</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>+</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>l</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>3</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>3</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> (<ce:italic>l</ce:italic>
                     <ce:inf loc="post">1</ce:inf>, <ce:italic>l</ce:italic>
                     <ce:inf loc="post">2</ce:inf>, <ce:italic>l</ce:italic>
                     <ce:inf loc="post">3</ce:inf> ∈ <ce:italic>Z</ce:italic>), where <mml:math altimg="si11.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>,</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>,</mml:mo>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>3</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                     </mml:math> are primitive lattice vectors, and the group of all <mml:math altimg="si12.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>R</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                     </mml:math> forms a Bravais lattice. In a PC, the dielectric constant satisfies <mml:math altimg="si13.gif" overflow="scroll">
                        <mml:mi>ε</mml:mi>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                        <mml:mo>=</mml:mo>
                        <mml:mi>ε</mml:mi>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>r</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mo>+</mml:mo>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>R</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                     </mml:math>.</ce:para>
                  <ce:para id="p0055" view="all">In this work, we studied a 2DPC with periodic patterns in the <ce:italic>xy</ce:italic> plane and infinite length in <ce:italic>z</ce:italic>. PC modes are divided into two polarization types: transverse-magnetic (TM) polarization, wherein the magnetic field is contained in the <ce:italic>xy</ce:italic> plane and the electric field is contained in the <ce:italic>z</ce:italic> direction; and transverse-electric (TE) polarization, in which, contrary to TM polarization, the electric field is in the <ce:italic>xy</ce:italic> plane and the magnetic field is in the <ce:italic>z</ce:italic> direction.</ce:para>
                  <ce:para id="p0060" view="all">For TM polarization, the <ce:italic>E</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>z</ce:italic>
                     </ce:inf> component is obtained from Eq. <ce:cross-ref refid="fd4" id="crosref0215">(4)</ce:cross-ref>, and using Bloch-Floquet theorem, the electric field can be expanded <mml:math altimg="si14.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>E</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                     </mml:math> as a sum of plane waves [<ce:cross-ref refid="bib2" id="crosref0220">2</ce:cross-ref>,<ce:cross-ref refid="bib28" id="crosref0225">28</ce:cross-ref>]:<ce:display>
                        <ce:formula id="fd5">
                           <ce:label>(5)</ce:label>
                           <mml:math altimg="si15.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>z</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:mo>∑</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>M</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mover accent="true">
                                                <mml:mrow>
                                                   <mml:mi>G</mml:mi>
                                                </mml:mrow>
                                                <mml:mo>⃗</mml:mo>
                                             </mml:mover>
                                          </mml:mrow>
                                          <mml:mo>+</mml:mo>
                                          <mml:mrow>
                                             <mml:mover accent="true">
                                                <mml:mrow>
                                                   <mml:mi>k</mml:mi>
                                                </mml:mrow>
                                                <mml:mo>⃗</mml:mo>
                                             </mml:mover>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>⋅</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>r</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si16.gif" overflow="scroll">
                        <mml:msubsup>
                           <mml:mrow>
                              <mml:mi>C</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                              <mml:mi>M</mml:mi>
                           </mml:mrow>
                        </mml:msubsup>
                     </mml:math> are the expansion coefficients, <mml:math altimg="si17.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>k</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                     </mml:math> is Bloch wave vector, and <mml:math altimg="si18.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>G</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mo>=</mml:mo>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>x</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>,</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>y</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                     </mml:math> is a two-dimensional (2D) reciprocal lattice vector with components <ce:italic>G</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>x</ce:italic>
                     </ce:inf> and <ce:italic>G</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>y</ce:italic>
                     </ce:inf>. The PWE method is often used to calculate the PBS. This method takes advantage of the periodicity of the dielectric system to expand the inverse of the dielectric function in a plane wave base, as follows:<ce:display>
                        <ce:formula id="fd6">
                           <ce:label>(6)</ce:label>
                           <mml:math altimg="si19.gif" overflow="scroll">
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>ε</mml:mi>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mrow>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                             <mml:mo>⃗</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mo>=</mml:mo>
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:mo>∑</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>η</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:mo>⋅</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>r</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where the Fourier transform de <mml:math altimg="si20.gif" overflow="scroll">
                        <mml:mfrac>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>ε</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                           </mml:mrow>
                        </mml:mfrac>
                     </mml:math> is given by<ce:display>
                        <ce:formula id="fd7">
                           <ce:label>(7)</ce:label>
                           <mml:math altimg="si21.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>η</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>=</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>A</mml:mi>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:mo>∫</mml:mo>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>ε</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>i</mml:mi>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:mo>⋅</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>r</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mi>d</mml:mi>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>r</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>Here we designate the surface of unit cell by <ce:italic>A</ce:italic>. Substituting Eqs. <ce:cross-refs refid="fd5 fd6" id="crosrefs0015">(5) and (6)</ce:cross-refs> into Eq. <ce:cross-ref refid="fd4" id="crosref0230">(4)</ce:cross-ref>, we obtain the eigenvalue equation for the TM polarization [<ce:cross-ref refid="bib28" id="crosref0235">28</ce:cross-ref>,<ce:cross-ref refid="bib31" id="crosref0240">31</ce:cross-ref>]:<ce:display>
                        <ce:formula id="fd8">
                           <ce:label>(8)</ce:label>
                           <mml:math altimg="si22.gif" overflow="scroll">
                              <mml:munder>
                                 <mml:mrow>
                                    <mml:mo>∑</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:munder>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>η</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>G</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>′</mml:mo>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mfenced open="[" close="]">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mfenced open="(" close=")">
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo>+</mml:mo>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>k</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfenced>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>+</mml:mo>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mfenced open="(" close=")">
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>y</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo>+</mml:mo>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>k</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>y</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfenced>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                              </mml:mfenced>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>M</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                              <mml:mo>=</mml:mo>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mfenced open="(" close=")">
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mi>ω</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                    </mml:mfenced>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>G</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>′</mml:mo>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>T</mml:mi>
                                    <mml:mi>M</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:math>
                        </ce:formula>
                     </ce:display>For TE polarization, the magnetic field is in the <ce:italic>z</ce:italic> direction, and it is expressed as an expansion in a plane wave base <mml:math altimg="si23.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>H</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                        <mml:mo>=</mml:mo>
                        <mml:mrow>
                           <mml:mover accent="true">
                              <mml:mrow>
                                 <mml:mi>z</mml:mi>
                              </mml:mrow>
                              <mml:mo>⃗</mml:mo>
                           </mml:mover>
                        </mml:mrow>
                        <mml:msub>
                           <mml:mrow>
                              <mml:mo>∑</mml:mo>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:msubsup>
                           <mml:mrow>
                              <mml:mi>C</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                              <mml:mi>E</mml:mi>
                           </mml:mrow>
                        </mml:msubsup>
                        <mml:msup>
                           <mml:mrow>
                              <mml:mi>e</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>i</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:mo>+</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mrow>
                                             <mml:mi>k</mml:mi>
                                          </mml:mrow>
                                          <mml:mo>⃗</mml:mo>
                                       </mml:mover>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>⋅</mml:mo>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>r</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:mrow>
                        </mml:msup>
                     </mml:math>, with <mml:math altimg="si24.gif" overflow="scroll">
                        <mml:msubsup>
                           <mml:mrow>
                              <mml:mi>C</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>T</mml:mi>
                              <mml:mi>E</mml:mi>
                           </mml:mrow>
                        </mml:msubsup>
                     </mml:math> the expansion coefficients. The eigenvalue equation for TE polarization is given by<ce:display>
                        <ce:formula id="fd9">
                           <ce:label>(9)</ce:label>
                           <mml:math altimg="si25.gif" overflow="scroll">
                              <mml:mtable class="align">
                                 <mml:mtr>
                                    <mml:mtd/>
                                    <mml:mtd>
                                       <mml:munder>
                                          <mml:mrow>
                                             <mml:mo>∑</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                       </mml:munder>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>η</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mrow>
                                                   <mml:mi>G</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>′</mml:mo>
                                                </mml:mrow>
                                             </mml:msup>
                                             <mml:mo>−</mml:mo>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mfenced open="[" close="]">
                                          <mml:mrow>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:msubsup>
                                                      <mml:mrow>
                                                         <mml:mi>G</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>x</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mo>′</mml:mo>
                                                      </mml:mrow>
                                                   </mml:msubsup>
                                                   <mml:mo>+</mml:mo>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>k</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>x</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>G</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>x</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                   <mml:mo>+</mml:mo>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>k</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>x</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mo>+</mml:mo>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:msubsup>
                                                      <mml:mrow>
                                                         <mml:mi>G</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>y</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mo>′</mml:mo>
                                                      </mml:mrow>
                                                   </mml:msubsup>
                                                   <mml:mo>+</mml:mo>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>k</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>y</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>G</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>y</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                   <mml:mo>+</mml:mo>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>k</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>y</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                             </mml:mfenced>
                                          </mml:mrow>
                                       </mml:mfenced>
                                    </mml:mtd>
                                 </mml:mtr>
                                 <mml:mtr>
                                    <mml:mtd/>
                                    <mml:mtd>
                                       <mml:mspace width="1em"/>
                                       <mml:msubsup>
                                          <mml:mrow>
                                             <mml:mi>C</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>G</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mi>E</mml:mi>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo>=</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:mi>ω</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>c</mml:mi>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                             </mml:mfenced>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:msubsup>
                                          <mml:mrow>
                                             <mml:mi>C</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mrow>
                                                   <mml:mi>G</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>′</mml:mo>
                                                </mml:mrow>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mi>E</mml:mi>
                                          </mml:mrow>
                                       </mml:msubsup>
                                    </mml:mtd>
                                 </mml:mtr>
                              </mml:mtable>
                           </mml:math>
                        </ce:formula>
                     </ce:display>The Fourier transform <mml:math altimg="si26.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>η</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                     </mml:math> plays a key role in determining the PBS for both polarizations. The particular structure studied in this paper is a 2DPC of hexagonal lattice composed air holes with cross sections in the form of isosceles triangle embedded in a GaAs background, as shown in <ce:cross-ref refid="fig1" id="crosref0245">Fig. 1</ce:cross-ref>
                     <ce:float-anchor refid="fig1"/>. The primitive vectors of the reciprocal lattice are <mml:math altimg="si27.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>1</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mfrac>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mi>π</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>a</mml:mi>
                           </mml:mrow>
                        </mml:mfrac>
                        <mml:mfenced open="(" close=")">
                           <mml:mrow>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                              <mml:mo>−</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:mi>y</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>⃗</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:mn>3</mml:mn>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mrow>
                              </mml:mfrac>
                           </mml:mrow>
                        </mml:mfenced>
                     </mml:math> and <mml:math altimg="si28.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mo>=</mml:mo>
                        <mml:mfrac>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mi>π</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>a</mml:mi>
                           </mml:mrow>
                        </mml:mfrac>
                        <mml:mfrac>
                           <mml:mrow>
                              <mml:mn>2</mml:mn>
                              <mml:mrow>
                                 <mml:mover accent="true">
                                    <mml:mrow>
                                       <mml:mi>y</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>⃗</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:msqrt>
                                 <mml:mrow>
                                    <mml:mn>3</mml:mn>
                                 </mml:mrow>
                              </mml:msqrt>
                           </mml:mrow>
                        </mml:mfrac>
                     </mml:math>, where <ce:italic>a</ce:italic> is the lattice constant.</ce:para>
                  <ce:para id="p0065" view="all">The GaAs dielectric constant depends on the hydrostatic pressure (P) and temperature (T) [<ce:cross-ref refid="bib29" id="crosref0250">29</ce:cross-ref>],<ce:display>
                        <ce:formula id="fd10">
                           <ce:label>(10)</ce:label>
                           <mml:math altimg="si29.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>ε</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>A</mml:mi>
                                    <mml:mi>s</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:mi>T</mml:mi>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mn>12.74</mml:mn>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>1.73</mml:mn>
                                    <mml:mo>×</mml:mo>
                                    <mml:mn>1</mml:mn>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mn>3</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>9.4</mml:mn>
                                    <mml:mo>×</mml:mo>
                                    <mml:mn>1</mml:mn>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mn>5</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mi>T</mml:mi>
                                          <mml:mo>−</mml:mo>
                                          <mml:mn>75.6</mml:mn>
                                       </mml:mrow>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mspace width="1em"/>
                              <mml:mtext>for</mml:mtext>
                              <mml:mspace width="1em"/>
                              <mml:mi>T</mml:mi>
                              <mml:mo>≤</mml:mo>
                              <mml:mn>200</mml:mn>
                              <mml:mi>K</mml:mi>
                           </mml:math>
                        </ce:formula>
                     </ce:display>In the present work we assume that the applied hydrostatic pressure does not modify the structure of the 2DPC. The Fourier coefficients <mml:math altimg="si26.gif" overflow="scroll">
                        <mml:msub>
                           <mml:mrow>
                              <mml:mi>η</mml:mi>
                           </mml:mrow>
                           <mml:mrow>
                              <mml:mi>G</mml:mi>
                           </mml:mrow>
                        </mml:msub>
                        <mml:mrow>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mrow>
                              <mml:mover accent="true">
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>⃗</mml:mo>
                              </mml:mover>
                           </mml:mrow>
                           <mml:mo stretchy="false">)</mml:mo>
                        </mml:mrow>
                     </mml:math> are given by Ref. [<ce:cross-ref refid="bib32" id="crosref0255">32</ce:cross-ref>]<ce:display>
                        <ce:formula id="fd11">
                           <ce:label>(11)</ce:label>
                           <mml:math altimg="si30.gif" overflow="scroll">
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>η</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>G</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo>=</mml:mo>
                              <mml:mfenced open="{" close="">
                                 <mml:mrow>
                                    <mml:mtable class="cases">
                                       <mml:mtr>
                                          <mml:mtd>
                                             <mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi>ε</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mi>G</mml:mi>
                                                         <mml:mi>a</mml:mi>
                                                         <mml:mi>A</mml:mi>
                                                         <mml:mi>s</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mi>P</mml:mi>
                                                         <mml:mo>,</mml:mo>
                                                         <mml:mi>T</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:mfrac>
                                             <mml:mo>+</mml:mo>
                                             <mml:mfrac>
                                                <mml:mrow>
                                                   <mml:msup>
                                                      <mml:mrow>
                                                         <mml:mi>A</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mo>′</mml:mo>
                                                      </mml:mrow>
                                                   </mml:msup>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>A</mml:mi>
                                                </mml:mrow>
                                             </mml:mfrac>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:mn>1</mml:mn>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>ε</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                               <mml:mi>a</mml:mi>
                                                               <mml:mi>A</mml:mi>
                                                               <mml:mi>s</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                         <mml:mrow>
                                                            <mml:mo stretchy="false">(</mml:mo>
                                                            <mml:mrow>
                                                               <mml:mi>P</mml:mi>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mi>T</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mo stretchy="false">)</mml:mo>
                                                         </mml:mrow>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mspace width="1em"/>
                                          </mml:mtd>
                                          <mml:mtd>
                                             <mml:mspace width="1em"/>
                                             <mml:mrow>
                                                <mml:mover accent="true">
                                                   <mml:mrow>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mo>⃗</mml:mo>
                                                </mml:mover>
                                             </mml:mrow>
                                             <mml:mo>=</mml:mo>
                                             <mml:mn>0</mml:mn>
                                          </mml:mtd>
                                       </mml:mtr>
                                       <mml:mtr>
                                          <mml:mtd>
                                             <mml:mfrac>
                                                <mml:mrow>
                                                   <mml:msup>
                                                      <mml:mrow>
                                                         <mml:mi>A</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mo>′</mml:mo>
                                                      </mml:mrow>
                                                   </mml:msup>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi>A</mml:mi>
                                                </mml:mrow>
                                             </mml:mfrac>
                                             <mml:mfenced open="(" close=")">
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:mn>1</mml:mn>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>ε</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                               <mml:mi>a</mml:mi>
                                                               <mml:mi>A</mml:mi>
                                                               <mml:mi>s</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                         <mml:mrow>
                                                            <mml:mo stretchy="false">(</mml:mo>
                                                            <mml:mrow>
                                                               <mml:mi>P</mml:mi>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mi>T</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mo stretchy="false">)</mml:mo>
                                                         </mml:mrow>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mfenced open="[" close="]">
                                                <mml:mrow>
                                                   <mml:mi>I</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>x</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                         <mml:mo>,</mml:mo>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>y</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                   <mml:mo>+</mml:mo>
                                                   <mml:mi>I</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mo>−</mml:mo>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>x</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                         <mml:mo>,</mml:mo>
                                                         <mml:msub>
                                                            <mml:mrow>
                                                               <mml:mi>G</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mi>y</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:mfenced>
                                             <mml:mspace width="1em"/>
                                          </mml:mtd>
                                          <mml:mtd>
                                             <mml:mspace width="1em"/>
                                             <mml:mrow>
                                                <mml:mover accent="true">
                                                   <mml:mrow>
                                                      <mml:mi>G</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mo>⃗</mml:mo>
                                                </mml:mover>
                                             </mml:mrow>
                                             <mml:mo>≠</mml:mo>
                                             <mml:mn>0</mml:mn>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:mfenced>
                           </mml:math>
                        </ce:formula>
                     </ce:display>with <ce:italic>A</ce:italic>′ being the area occupied by the isosceles triangle; the <ce:italic>I</ce:italic>(<ce:italic>G</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>x</ce:italic>
                     </ce:inf>, <ce:italic>G</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>y</ce:italic>
                     </ce:inf>) function is given by<ce:display>
                        <ce:formula id="fd12">
                           <ce:label>(12)</ce:label>
                           <mml:math altimg="si31.gif" overflow="scroll">
                              <mml:mi>I</mml:mi>
                              <mml:mrow>
                                 <mml:mo stretchy="false">(</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>G</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>x</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>G</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>y</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                                 <mml:mo stretchy="false">)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>g</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>y</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:mfrac>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mfenced open="(" close=")">
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>g</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>y</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>3</mml:mn>
                                             </mml:mrow>
                                          </mml:mfrac>
                                          <mml:mo>−</mml:mo>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>g</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                    </mml:mfenced>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mfenced open="[" close="]">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>g</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>y</mml:mi>
                                                   </mml:mrow>
