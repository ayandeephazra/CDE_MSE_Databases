         <head>
            <ce:dochead id="dh005">
               <ce:textfn>Research paper</ce:textfn>
            </ce:dochead>
            <ce:title id="tm005">SiP<ce:inf loc="post">2</ce:inf>S monolayer: A two dimensional semiconductor with a moderate band gap</ce:title>
            <ce:author-group id="ag005">
               <ce:author orcid="0000-0003-0786-2175" id="au005" author-id="S0009261418309448-f90525ffd6c30e2ac7fb622829d7aab0">
                  <ce:given-name>Mosayeb</ce:given-name>
                  <ce:surname>Naseri</ce:surname>
                  
               </ce:author>
               <ce:affiliation id="af005" affiliation-id="S0009261418309448-f76e35f209bb2abed1d44f3cb1fb9f17">
