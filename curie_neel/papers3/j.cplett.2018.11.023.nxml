                                 </mml:mfrac>
                                 <mml:munder>
                                    <mml:mo movablelimits="false">lim</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>q</mml:mi>
                                       <mml:mo stretchy="false">→</mml:mo>
                                       <mml:mn>0</mml:mn>
                                    </mml:mrow>
                                 </mml:munder>
                                 <mml:mfrac>
                                    <mml:mn>1</mml:mn>
                                    <mml:msup>
                                       <mml:mi>q</mml:mi>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
                                 </mml:mfrac>
