                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>v</mml:mi>
                                             <mml:mover accent="true">
                                                <mml:mi>k</mml:mi>
                                                <mml:mo stretchy="false">→</mml:mo>
                                             </mml:mover>
                                          </mml:mrow>
