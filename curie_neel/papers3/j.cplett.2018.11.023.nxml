                                    </mml:msub>
                                 </mml:mrow>
                                 <mml:mi>δ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:msub>
                                       <mml:mi>ε</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mover accent="true">
                                             <mml:mi>k</mml:mi>
                                             <mml:mo stretchy="false">→</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>-</mml:mo>
                                    <mml:msub>
                                       <mml:mi>ε</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>v</mml:mi>
