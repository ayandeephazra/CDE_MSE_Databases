                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mn>2</mml:mn>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:msubsup>
