                                    <mml:mi>ω</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mn>4</mml:mn>
                                       <mml:msup>
                                          <mml:mi>π</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:msup>
