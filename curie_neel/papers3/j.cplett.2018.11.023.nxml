                                             <mml:mi>k</mml:mi>
                                             <mml:mo stretchy="false">→</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo stretchy="false">〉</mml:mo>
                                 </mml:mrow>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">〈</mml:mo>
                                       <mml:msub>
                                          <mml:mi>u</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>c</mml:mi>
                                             <mml:mover accent="true">
                                                <mml:mi>k</mml:mi>
                                                <mml:mo stretchy="false">→</mml:mo>
                                             </mml:mover>
                                             <mml:mo>+</mml:mo>
                                             <mml:msub>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>β</mml:mi>
                                             </mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>q</mml:mi>
                                                <mml:mo stretchy="false">→</mml:mo>
                                             </mml:mover>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo stretchy="false">|</mml:mo>
