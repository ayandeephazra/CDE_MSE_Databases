                                          <mml:mi>P</mml:mi>
                                          <mml:mn>2</mml:mn>
                                          <mml:mi>S</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">)</mml:mo>
                                       <mml:mo stretchy="false">/</mml:mo>
                                       <mml:mn>4</mml:mn>
                                       <mml:mo>,</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where, <mml:math altimg="si15.gif" overflow="scroll">
                           <mml:msub>
                              <mml:mi>E</mml:mi>
