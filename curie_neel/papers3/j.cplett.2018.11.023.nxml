                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">Si</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>+</mml:mo>
                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
