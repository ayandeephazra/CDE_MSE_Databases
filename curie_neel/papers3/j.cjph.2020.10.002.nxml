                                    <mml:msubsup>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>g</mml:mi>
                                          <mml:mspace width="1em"/>
                                          <mml:mi>j</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>x</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
