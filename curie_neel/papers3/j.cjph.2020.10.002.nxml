                                    <mml:mi>R</mml:mi>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mi>C</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:mfrac>
                                    <mml:munderover>
                                       <mml:mo>â</mml:mo>
                                       <mml:mrow>
                                          <mml:mi>j</mml:mi>
                                          <mml:mo>=</mml:mo>
                                          <mml:mn>1</mml:mn>
                                       </mml:mrow>
                                       <mml:mi>k</mml:mi>
                                    </mml:munderover>
                                    <mml:mrow>
                                       <mml:mi>Ï</mml:mi>
