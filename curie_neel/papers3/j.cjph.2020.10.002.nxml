                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:msub>
                                       <mml:mi>L</mml:mi>
                                       <mml:mi>j</mml:mi>
                                    </mml:msub>
                                    <mml:mo>,</mml:mo>
                                    <mml:mi>Ï</mml:mi>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                              <mml:mo>]</mml:mo>
                           </mml:mrow>
                        </mml:math>represents <mml:math altimg="si13.svg">
                           <mml:mrow>
                              <mml:mrow>
                                 <mml:mi>Îµ</mml:mi>
                              </mml:mrow>
                              <mml:mo linebreak="goodbreak">â</mml:mo>
                              <mml:mi>i</mml:mi>
                              <mml:mi>n</mml:mi>
                              <mml:mi>s</mml:mi>
                              <mml:mi>e</mml:mi>
                              <mml:mi>n</mml:mi>
                              <mml:mi>s</mml:mi>
                              <mml:mi>t</mml:mi>
                              <mml:mi>i</mml:mi>
                              <mml:mi>v</mml:mi>
                              <mml:mi>e</mml:mi>
                           </mml:mrow>
                        </mml:math>lost function</ce:para>
                     <ce:para id="para0014" view="all">The presented risk function in <ce:cross-ref id="crf0044" refid="eqn0002">Eq.Â (2)</ce:cross-ref> consists of the empirical error part <mml:math altimg="si14.svg">
                           <mml:mrow>
                              <mml:mfrac>
                                 <mml:mi>C</mml:mi>
                                 <mml:mi>k</mml:mi>
                              </mml:mfrac>
                              <mml:munderover>
                                 <mml:mo>â</mml:mo>
                                 <mml:mrow>
                                    <mml:mi>j</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mn>1</mml:mn>
                                 </mml:mrow>
                                 <mml:mi>k</mml:mi>
                              </mml:munderover>
                              <mml:mrow>
                                 <mml:mi>Ï</mml:mi>
