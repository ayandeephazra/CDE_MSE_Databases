                                             <mml:msub>
                                                <mml:mi>E</mml:mi>
                                                <mml:mi>g</mml:mi>
                                             </mml:msub>
                                             <mml:mrow>
                                                <mml:mo>(</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>L</mml:mi>
                                                   <mml:mi>j</mml:mi>
                                                </mml:msub>
                                                <mml:mo>,</mml:mo>
                                                <mml:mi>Ï</mml:mi>
                                                <mml:mo>)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
