            <ce:title id="tm005">Wide band gap semiconductor technology: State-of-the-art</ce:title>
            <ce:author-group id="ag005">
               <ce:author orcid="0000-0003-0976-6232" id="au005" biographyid="bg005" author-id="S0038110118305616-fee7abac4e384d1002fbd9e68ac0a487">
                  <ce:given-name>Michael</ce:given-name>
                  <ce:surname>Shur</ce:surname>
                  
               </ce:author>
               <ce:affiliation id="af005" affiliation-id="S0038110118305616-495a4431045edcb8f7065855672acfac">
