            <ce:appendices view="all">
               <ce:section id="s0065" view="extended">
                  <ce:label>Appendix A</ce:label>
                  <ce:section-title id="st0095">Supplementary data</ce:section-title>
                  <ce:para id="p0225" view="all">
                     <ce:display>
