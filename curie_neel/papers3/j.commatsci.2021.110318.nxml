               <ce:abstract-sec id="as005" view="all">
                  <ce:simple-para id="sp005" view="all">
                     <ce:display>
                        <ce:figure id="f0040">
                           <ce:link id="lk005" locator="ga1" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:href="pii:S0927025621000434/ga1"/>
