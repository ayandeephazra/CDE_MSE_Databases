                                 </mml:math>
                              </ce:formula>
                              <ce:formula id="fd2">
                                 <ce:label>(2)</ce:label>
                                 <mml:math display="block" id="d1e2855" altimg="si56.svg">
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mrow>
                                             <mml:mi>H</mml:mi>
                                          </mml:mrow>
