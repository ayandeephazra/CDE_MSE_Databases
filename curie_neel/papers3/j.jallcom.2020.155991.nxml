                              </ce:formula>
                           </ce:display>
                           <ce:display>
                              <ce:formula id="fd5">
                                 <ce:label>(5)</ce:label>
                                 <mml:math display="block" altimg="si38.svg" alttext="Equation 5.">
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>V</mml:mi>
                                          <mml:mi>L</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>010</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>22</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mo>;</mml:mo>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msubsup>
                                          <mml:mi>V</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>010</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>66</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mo>;</mml:mo>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msubsup>
                                          <mml:mi>V</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>010</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>44</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                    </mml:mrow>
                                 </mml:math>
                              </ce:formula>
                           </ce:display>
                           <ce:display>
                              <ce:formula id="fd6">
                                 <ce:label>(6)</ce:label>
                                 <mml:math display="block" altimg="si39.svg" alttext="Equation 6.">
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>V</mml:mi>
                                          <mml:mi>L</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>001</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>33</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mo>;</mml:mo>
                                       <mml:mspace width="0.25em"/>
                                       <mml:mi>V</mml:mi>
                                       <mml:msubsup>
                                          <mml:mn>1</mml:mn>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mn>1</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>001</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>55</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mo>;</mml:mo>
                                       <mml:mspace width="0.25em"/>
                                       <mml:msubsup>
                                          <mml:mi>V</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">[</mml:mo>
                                             <mml:mn>001</mml:mn>
                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>44</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                    </mml:mrow>
                                 </mml:math>
                              </ce:formula>
                           </ce:display>
                        </ce:para>
                        <ce:para id="p0145" view="all">The subscript <ce:italic>L</ce:italic> (<ce:italic>T</ce:italic>) notes that the sound wave is polarized longitudinally (transversally), T1 and T2 refer to the first and second transverse modes. From <ce:cross-ref refid="tbl4" id="crosref0225">Table 4</ce:cross-ref>, it is noted that the elastic wave velocities of the Li2HgMS4 (M = Si, Ge, Sn) compounds decrease slightly when passing from Li2HgSiS4 to Li2HgGeS4 to Li2HgSnS4, reflecting the slight decrease of the elastic constants (stiffness) when one moves following the same sequence. Moreover, the longitudinal sound wave velocity is larger than the transverse one. This is attributed to the fact that the elastic constants characterising the resistance to unidirectional-stress are larger than those characterizing the resistance to shear deformation. From Eqs. <ce:cross-refs refid="fd4 fd5 fd6" id="crosrefs0060">(4)–(6</ce:cross-refs>), one notes that <ce:italic>C</ce:italic>
                           <ce:inf loc="post">11</ce:inf>, <ce:italic>C</ce:italic>
                           <ce:inf loc="post">22</ce:inf>, and <ce:italic>C</ce:italic>
                           <ce:inf loc="post">33</ce:inf> determine the longitudinal elastic wave velocities along the [100], [010] and [001] crystallographic directions, respectively, and <ce:italic>C</ce:italic>
                           <ce:inf loc="post">44</ce:inf>, <ce:italic>C</ce:italic>
                           <ce:inf loc="post">55</ce:inf>, and <ce:italic>C</ce:italic>
                           <ce:inf loc="post">66</ce:inf> determine the transverse modes. Therefore, the anisotropy of the acoustic velocities suggests the elastic anisotropy of the title compounds.</ce:para>
                     </ce:section>
                     <ce:section id="sec3.2.2" view="all">
                        <ce:label>3.2.2</ce:label>
                        <ce:section-title id="sectitle0055">Elastic moduli of the polycrystalline aggregates</ce:section-title>
                        <ce:para id="p0150" view="all">The isotropic elastic moduli characterizing the elastic properties of a randomly oriented polycrystals (polycrystalline aggregates), such as the isotropic bulk modulus <ce:italic>B,</ce:italic> shear modulus <ce:italic>G</ce:italic>, Young's modulus <ce:italic>E</ce:italic> and Poison's ratio <mml:math altimg="si40.svg">
                              <mml:mi>σ</mml:mi>
                           </mml:math>, can be numerically estimated from the single-crystal elastic constants based on the Voigt-Reuss-Hill approximation [<ce:cross-refs refid="bib63 bib64 bib65 bib66" id="crosrefs0065">63–66</ce:cross-refs>] (subscripted, respectively, V, R and H in the following). Numerical estimates of the main polycrystalline elastic moduli, viz., <ce:italic>B</ce:italic>, <ce:italic>G</ce:italic>, <ce:italic>E</ce:italic> and <mml:math altimg="si40.svg">
                              <mml:mi>σ</mml:mi>
                           </mml:math>, of the explored compounds are gathered in <ce:cross-ref refid="tbl4" id="crosref0230">Table 4</ce:cross-ref>. From the listed data in this table, one can note:<ce:list id="olist0015">
                              <ce:list-item id="o0035">
                                 <ce:label>(i)</ce:label>
                                 <ce:para id="p0155" view="all">The values of the bulk modulus <ce:italic>B</ce:italic>, which characterizes the resistance of a material against volume change, shear modulus <ce:italic>G</ce:italic>, which measures the stiffness of a material against shear deformation, and Young's modulus <ce:italic>E</ce:italic>, which is defined as the ratio of the tensile stress in a material to the corresponding tensile strain, are low, indicating that the considered compounds exhibit a weak stiffness against compression, attraction and shearing deformations, i.e., they are soft materials.</ce:para>
                              </ce:list-item>
                              <ce:list-item id="o0040">
                                 <ce:label>(ii)</ce:label>
                                 <ce:para id="p0160" view="all">The numerical values of the isotropic bulk modulus <ce:italic>B</ce:italic>, shear modulus <ce:italic>G</ce:italic> and Young's modulus <ce:italic>E</ce:italic> decrease slightly when one moves from Li<ce:inf loc="post">2</ce:inf>HgSiS<ce:inf loc="post">4</ce:inf> to Li<ce:inf loc="post">2</ce:inf>HgGeS<ce:inf loc="post">4</ce:inf> to Li<ce:inf loc="post">2</ce:inf>HgSnS<ce:inf loc="post">4</ce:inf>. This trend indicates that the elastic moduli <ce:italic>B</ce:italic>, <ce:italic>G</ce:italic> and <ce:italic>E</ce:italic> of the Li<ce:inf loc="post">2</ce:inf>HgMS<ce:inf loc="post">4</ce:inf> (M = Si, Ge, Sn) compounds decrease slightly with the increasing atomic number of the M atom, which can be explained by the fact that the unit-cell volume increases slightly with the increasing atomic radius and the bulk modulus value is inversely proportional to the unit-cell volume; <mml:math altimg="si41.svg">
                                       <mml:mrow>
                                          <mml:mi>B</mml:mi>
                                          <mml:mo linebreak="goodbreak" linebreakstyle="after">∝</mml:mo>
                                          <mml:msup>
                                             <mml:mi>V</mml:mi>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:mn>1</mml:mn>
                                             </mml:mrow>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:math>.</ce:para>
                              </ce:list-item>
                              <ce:list-item id="o0045">
                                 <ce:label>(iii)</ce:label>
                                 <ce:para id="p0165" view="all">The Poisson's ratio <mml:math altimg="si40.svg">
                                       <mml:mi>σ</mml:mi>
                                    </mml:math>, which is defined as the ratio of transverse strain (normal to the applied stress) to the longitudinal strain (in the direction of the applied stress), is generally linked to the change in volume of the solid during a uniaxial deformation [<ce:cross-refs refid="bib67 bib68 bib69" id="crosrefs0070">67–69</ce:cross-refs>]. If <mml:math altimg="si40.svg">
                                       <mml:mi>σ</mml:mi>
                                    </mml:math> is equal to 0.5, no volume change occurs, while if it is lower than 0.5, a large volume change is expected for any elastic deformation. In our case, the value of <mml:math altimg="si40.svg">
                                       <mml:mi>σ</mml:mi>
                                    </mml:math> is ∼0.33 for all considered materials, which suggests that a considerable change in volume accompanies the elastic deformation.</ce:para>
                              </ce:list-item>
                              <ce:list-item id="o0050">
                                 <ce:label>(iv)</ce:label>
                                 <ce:para id="p0170" view="all">Ductile behavior of materials is closely associated with their machinability and resistance to thermal shock, so it is important to determine whether the material is brittle or ductile. For this, Pugh [<ce:cross-ref refid="bib70" id="crosref0235">70</ce:cross-ref>] proposed the ratio of the isotropic bulk modulus <ce:italic>B</ce:italic> to the isotropic shear modulus <ce:italic>G</ce:italic> as a criterion for determining whether the solid is of ductile or brittle nature. This criterion suggests that if the <ce:italic>B</ce:italic>/<ce:italic>G</ce:italic> ratio is greater than 1.75, the solid tends to ductility, while a <ce:italic>B</ce:italic>/<ce:italic>G</ce:italic> ratio less than 1.75 indicates a tendency to brittleness. Based on Pugh's empirical criterion and in the light of the obtained result (<ce:italic>B</ce:italic>/<ce:italic>G</ce:italic> ∼ 2.6), the three studied DLSs would behave as ductile materials.</ce:para>
                              </ce:list-item>
                              <ce:list-item id="o0055">
                                 <ce:label>(v)</ce:label>
                                 <ce:para id="p0175" view="all">In Debye model, Debye temperature <ce:italic>θ</ce:italic>
                                    <ce:inf loc="post">
                                       <ce:italic>D</ce:italic>
                                    </ce:inf> is an important parameter that specifies the boundary between high and low temperature areas for a solid. Therefore, <ce:italic>θ</ce:italic>
                                    <ce:inf loc="post">
                                       <ce:italic>D</ce:italic>
                                    </ce:inf> is linked to a number of thermo-physical properties, such as thermal expansion, melting point and Grüneisen parameter. <ce:italic>θ</ce:italic>
                                    <ce:inf loc="post">
                                       <ce:italic>D</ce:italic>
                                    </ce:inf> can be numerically estimated from the specific heat or from the elastic moduli. Here, we calculated <ce:italic>θ</ce:italic>
                                    <ce:inf loc="post">
                                       <ce:italic>D</ce:italic>
                                    </ce:inf> from the predicted elastic moduli through a well-known relationship [<ce:cross-ref refid="bib71" id="crosref0240">71</ce:cross-ref>,<ce:cross-ref refid="bib72" id="crosref0245">72</ce:cross-ref>]. The calculated mass density (<ce:italic>ρ</ce:italic>), isotropic transverse, longitudinal and average sound velocities (<mml:math altimg="si42.svg">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>t</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:math>, <mml:math altimg="si43.svg">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>l</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:math> and <mml:math altimg="si44.svg">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>m</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:math>) and Debye temperature (<mml:math altimg="si45.svg">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>θ</mml:mi>
                                             <mml:mi>D</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:math>) for the Li<ce:inf loc="post">2</ce:inf>HgMS<ce:inf loc="post">4</ce:inf> (M = Si, Ge, Sn) polycrystalline compounds are listed in <ce:cross-ref refid="tbl5" id="crosref0250">Table 5</ce:cross-ref>
                                    <ce:float-anchor refid="tbl5"/>. The calculated values of Debye temperature and isotropic average sound velocity decrease when one moves from Li<ce:inf loc="post">2</ce:inf>HgSiS<ce:inf loc="post">4</ce:inf> to Li<ce:inf loc="post">2</ce:inf>HgGeS<ce:inf loc="post">4</ce:inf> to Li<ce:inf loc="post">2</ce:inf>HgSnS<ce:inf loc="post">4</ce:inf>. This trend can be explained by the fact that large elastic moduli and low mass density lead to the relatively large elastic wave velocities and consequently a large Debye temperature. Li<ce:inf loc="post">2</ce:inf>HgSiS<ce:inf loc="post">4</ce:inf> has the largest Debye temperature and acoustic velocities among the studied compounds because it has the largest bulk and shear moduli and lowest mass density.</ce:para>
                              </ce:list-item>
                           </ce:list>
                        </ce:para>
                     </ce:section>
                     <ce:section id="sec3.2.3" view="all">
                        <ce:label>3.2.3</ce:label>
                        <ce:section-title id="sectitle0060">Elastic anisotropy</ce:section-title>
                        <ce:para id="p0180" view="all">It is known that microcracks and mechanical failure can appear in materials having a significant anisotropy of elastic properties as well as thermal expansion [<ce:cross-ref refid="bib73" id="crosref0255">73</ce:cross-ref>]. Elastic anisotropy reflects the anisotropy of the interatomic bonding in different planes of the crystal. Therefore, knowledge of the extent of elastic anisotropy in crystals gives an idea of a possible mechanical failure. Furthermore, it is demonstrated that elastic anisotropy has a significant influence on nanoscale precursor textures in alloys [<ce:cross-ref refid="bib74" id="crosref0260">74</ce:cross-ref>]. Thus, in order to find mechanisms that will improve the resistance of solids to microcracks and mechanical failure, predicting and correctly describing the degree of elastic anisotropy of solids is of great importance as far as applications of these solids are concerned. Many different anisotropic factors or indexes have been developed to characterize the anisotropy of elastic properties for predicting and understanding the possible mechanical failure modes in materials. In this work, four different elastic anisotropy metrics, including shear anisotropic factors, universal anisotropic index, percent anisotropy and visualization of crystallographic direction dependence of the linear compressibility and Young's modulus, were used to quantify the elastic anisotropy extent in the Li<ce:inf loc="post">2</ce:inf>HgSiS<ce:inf loc="post">4</ce:inf>, Li<ce:inf loc="post">2</ce:inf>HgGeS<ce:inf loc="post">4</ce:inf> and Li<ce:inf loc="post">2</ce:inf>HgSnS<ce:inf loc="post">4</ce:inf> compounds.<ce:list id="olist0020">
                              <ce:list-item id="o0060">
                                 <ce:label>1.</ce:label>
                                 <ce:para id="p0185" view="all">To characterize the degree of anisotropy in the interatomic bonding in different planes in the considered orthorhombic crystals, one uses the shear anisotropic factors <ce:italic>A</ce:italic>
                                    <ce:inf loc="post">i</ce:inf> given by the following expressions [<ce:cross-ref refid="bib75" id="crosref0265">75</ce:cross-ref>,<ce:cross-ref refid="bib76" id="crosref0270">76</ce:cross-ref>]:</ce:para>
                              </ce:list-item>
                           </ce:list>
                           <ce:display>
                              <ce:formula id="fd10">
                                 <ce:label>(10)</ce:label>
                                 <mml:math display="block" altimg="si46.svg" alttext="Equation 10.">
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>A</mml:mi>
                                          <mml:mn>1</mml:mn>
                                       </mml:msub>
                                       <mml:mo linebreak="badbreak">=</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mn>4</mml:mn>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>44</mml:mn>
                                             </mml:msub>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:msub>
