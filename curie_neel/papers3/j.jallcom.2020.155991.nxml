                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>55</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
