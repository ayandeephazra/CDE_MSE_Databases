                                             <mml:mo stretchy="true">]</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mn>66</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:mi>ρ</mml:mi>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mo>;</mml:mo>
                                       <mml:mspace width="0.25em"/>
