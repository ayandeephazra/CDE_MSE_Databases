                                                                           <mml:mrow>
                                                                              <mml:mo stretchy="true">(</mml:mo>
                                                                              <mml:mrow>
                                                                                 <mml:mtable columnalign="left">
                                                                                    <mml:mtr>
                                                                                       <mml:mtd>
                                                                                          <mml:mn>2</mml:mn>
                                                                                          <mml:msup>
                                                                                             <mml:mrow>
                                                                                                <mml:mi>u</mml:mi>
                                                                                             </mml:mrow>
                                                                                             <mml:mn>6</mml:mn>
                                                                                          </mml:msup>
                                                                                          <mml:mo>−</mml:mo>
                                                                                          <mml:mn>4</mml:mn>
                                                                                          <mml:msup>
                                                                                             <mml:mrow>
                                                                                                <mml:mi>u</mml:mi>
                                                                                             </mml:mrow>
                                                                                             <mml:mn>4</mml:mn>
                                                                                          </mml:msup>
                                                                                       </mml:mtd>
                                                                                    </mml:mtr>
                                                                                    <mml:mtr>
                                                                                       <mml:mtd>
                                                                                          <mml:mo>−</mml:mo>
