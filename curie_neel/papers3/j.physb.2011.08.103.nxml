                                                            <mml:msup>
                                                               <mml:mrow>
                                                                  <mml:mi>u</mml:mi>
                                                               </mml:mrow>
                                                               <mml:mn>6</mml:mn>
                                                            </mml:msup>
                                                            <mml:mo stretchy="false">)</mml:mo>
                                                         </mml:mtd>
                                                      </mml:mtr>
                                                      <mml:mtr>
