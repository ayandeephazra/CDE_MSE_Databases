                                          <mml:mtd>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">{</mml:mo>
                                                <mml:mrow>
                                                   <mml:mtable columnalign="left">
                                                      <mml:mtr>
                                                         <mml:mtd>
                                                            <mml:mo stretchy="false">(</mml:mo>
                                                            <mml:mn>26</mml:mn>
                                                            <mml:msup>
                                                               <mml:mrow>
                                                                  <mml:mi>u</mml:mi>
                                                               </mml:mrow>
                                                               <mml:mn>2</mml:mn>
                                                            </mml:msup>
                                                            <mml:mo>−</mml:mo>
