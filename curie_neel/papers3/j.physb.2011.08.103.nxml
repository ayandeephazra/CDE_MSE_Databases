                                                      <mml:mi>R</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi mathvariant="normal">c</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:mrow>
                                                </mml:msubsup>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mi mathvariant="normal">exp</mml:mi>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>R</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi mathvariant="normal">c</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mo stretchy="true">]</mml:mo>
                              </mml:mrow>
                              <mml:mspace width=".25em"/>
                              <mml:mi mathvariant="normal">for</mml:mi>
                              <mml:mspace width=".25em"/>
                              <mml:mi>r</mml:mi>
                              <mml:mo>≤</mml:mo>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>R</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi mathvariant="normal">c</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>W</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi mathvariant="normal">ion</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>r</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:mo>−</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mi>Z</mml:mi>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                       </mml:mrow>
