                                 </mml:mrow>
                              </mml:msub>
                              <mml:mo stretchy="false">(</mml:mo>
                              <mml:mi>q</mml:mi>
                              <mml:mo stretchy="false">)</mml:mo>
                              <mml:mo>=</mml:mo>
                              <mml:mfrac>
                                 <mml:mrow>
