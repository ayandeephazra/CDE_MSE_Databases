                                                                                          <mml:mn>17</mml:mn>
                                                                                          <mml:mi>u</mml:mi>
                                                                                       </mml:mtd>
                                                                                    </mml:mtr>
                                                                                 </mml:mtable>
                                                                              </mml:mrow>
                                                                              <mml:mo stretchy="true">)</mml:mo>
                                                                           </mml:mrow>
                                                                           <mml:mi>sin</mml:mi>
                                                                           <mml:mspace width=".1em"/>
                                                                           <mml:mi>u</mml:mi>
                                                                        </mml:mtd>
                                                                     </mml:mtr>
