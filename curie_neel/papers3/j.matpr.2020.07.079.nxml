                                                         <mml:mi>H</mml:mi>
                                                         <mml:mo stretchy="true">/</mml:mo>
                                                         <mml:mi>L</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msubsup>
                                                </mml:mrow>
                                             </mml:mrow>
                                          </mml:mfenced>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">/</mml:mo>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
