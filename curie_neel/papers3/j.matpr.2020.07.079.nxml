                                          <mml:mrow>
                                             <mml:mo>-</mml:mo>
                                             <mml:mi>λ</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>4</mml:mn>
                                             <mml:msub>
                                                <mml:mi>k</mml:mi>
                                                <mml:mi>B</mml:mi>
                                             </mml:msub>
