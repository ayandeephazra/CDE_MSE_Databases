                           </mml:mrow>
                        </mml:mrow>
                     </mml:math>, <mml:math altimg="si11.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi mathvariant="normal">G</mml:mi>
                              <mml:mrow>
                                 <mml:mi mathvariant="normal">m</mml:mi>
                                 <mml:mi mathvariant="normal">a</mml:mi>
                                 <mml:mi mathvariant="normal">x</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>14</mml:mn>
                           <mml:msubsup>
                              <mml:mi>R</mml:mi>
                              <mml:mrow>
                                 <mml:mi>y</mml:mi>
                              </mml:mrow>
                              <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mn>2</mml:mn>
                              </mml:mfrac>
