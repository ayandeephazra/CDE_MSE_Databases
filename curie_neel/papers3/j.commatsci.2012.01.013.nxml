                                          <mml:mrow>
                                             <mml:mn>3</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:mfrac>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi>B</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>′</mml:mo>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mo>-</mml:mo>
                                       <mml:mn>4</mml:mn>
                                       <mml:mo stretchy="false">)</mml:mo>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>f</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
