                           <mml:mn>0.32</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.68</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
