                           <mml:mtext>Z</mml:mtext>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
                           <mml:mo stretchy="false">â</mml:mo>
                           <mml:mtext>G</mml:mtext>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
                           <mml:mo stretchy="false">â</mml:mo>
                           <mml:mtext>Y</mml:mtext>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mo linebreak="badbreak" linebreakstyle="after">â</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
                           <mml:mo stretchy="false">â</mml:mo>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>X</mml:mtext>
