                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
                           <mml:mo stretchy="false">â</mml:mo>
                           <mml:mtext>A</mml:mtext>
                           <mml:mo stretchy="false">(</mml:mo>
                           <mml:mn>0.32</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.32</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>0.5</mml:mn>
                           <mml:mo stretchy="false">)</mml:mo>
