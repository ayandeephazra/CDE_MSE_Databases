                                    <mml:mi>e</mml:mi>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msqrt>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msubsup>
                                       <mml:mo>+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msubsup>
                                                <mml:mi>N</mml:mi>
                                                <mml:mi>d</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msubsup>
                                          </mml:mrow>
                                          <mml:mn>4</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                 </mml:msqrt>
                                 <mml:mo>+</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>N</mml:mi>
                                          <mml:mi>d</mml:mi>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:mfrac>
                                 <mml:mo>,</mml:mo>
                                 <mml:mspace width="1em"/>
                                 <mml:mspace width="1em"/>
                                 <mml:msub>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>h</mml:mi>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msqrt>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msubsup>
