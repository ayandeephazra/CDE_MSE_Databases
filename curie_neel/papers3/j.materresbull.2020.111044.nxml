            <ce:abstract id="abs0005" class="graphical" view="all">
               <ce:section-title id="sect0005">Graphical abstract</ce:section-title>
               <ce:abstract-sec id="abst0005" view="all">
                  <ce:simple-para id="spar0025" view="all">
                     <ce:display>
                        <ce:figure id="fig0025">
                           <ce:link xlink:href="pii:S0025540820315257/ga1" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" locator="ga1"/>
