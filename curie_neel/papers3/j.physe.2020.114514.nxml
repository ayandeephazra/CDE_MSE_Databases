                                                <mml:mrow>
                                                   <mml:mtext>atom</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mtext>O</mml:mtext>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:msubsup>
                                          </mml:mrow>
