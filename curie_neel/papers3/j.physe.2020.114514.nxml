                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si9.svg" alttext="Equation 8.">
                                 <mml:msubsup>
                                    <mml:mrow>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mtext>form</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mo>-</mml:mo>
                                                   <mml:mtext>PtO</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>2</mml:mn>
                                                </mml:mrow>
                                             </mml:msub>
