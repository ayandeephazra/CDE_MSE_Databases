                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mo>-</mml:mo>
                                                   <mml:mtext>PtO</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>2</mml:mn>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:msubsup>
                                 <mml:mo linebreak="goodbreak">−</mml:mo>
                                 <mml:mfenced close="]" open="[">
                                    <mml:mrow>
