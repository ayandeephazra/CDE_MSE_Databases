                                          <mml:mrow>
                                             <mml:mi>x</mml:mi>
                                             <mml:msubsup>
                                                <mml:mrow>
                                                   <mml:mi>E</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mtext>coh</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mtext>Pt</mml:mtext>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:mi>y</mml:mi>
                                             <mml:msubsup>
                                                <mml:mrow>
                                                   <mml:mi>E</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mtext>coh</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="false">(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mtext>O</mml:mtext>
                                                      </mml:mrow>
                                                      <mml:mo stretchy="false">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:msubsup>
