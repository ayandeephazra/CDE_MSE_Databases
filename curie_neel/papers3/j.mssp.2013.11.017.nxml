               </ce:keyword>
               <ce:keyword id="key0020">
                  <ce:text>Room temperature ferromagnetism</ce:text>
               </ce:keyword>
               <ce:keyword id="key0025">
                  <ce:text>Cd<ce:inf loc="post">1−<ce:italic>x</ce:italic>
                     </ce:inf>Cr<ce:inf loc="post">
