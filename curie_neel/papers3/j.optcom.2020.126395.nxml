                                          <mml:mi>p</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>T</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>e</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
