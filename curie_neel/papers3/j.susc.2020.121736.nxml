                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>≈</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi>e</mml:mi>
                                       <mml:msup>
                                          <mml:mi>ℏ</mml:mi>
                                          <mml:mn>3</mml:mn>
                                       </mml:msup>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mn>5</mml:mn>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo>_</mml:mo>
                                                   <mml:mn>2</mml:mn>
                                                   <mml:mi>D</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo>+</mml:mo>
                                             <mml:mn>3</mml:mn>
                                             <mml:msub>
                                                <mml:mi>C</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>y</mml:mi>
                                                   <mml:mo>_</mml:mo>
                                                   <mml:mn>2</mml:mn>
                                                   <mml:mi>D</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
