                                             <mml:mi>δ</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>α</mml:mi>
                                                <mml:mo>,</mml:mo>
                                                <mml:msup>
                                                   <mml:mi>α</mml:mi>
                                                   <mml:mo>′</mml:mo>
