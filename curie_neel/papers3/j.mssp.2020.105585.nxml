                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="goodbreak">−</mml:mo>
                                       <mml:msub>
                                          <mml:mrow>
