                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>11</mml:mn>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                       <mml:mn>0</mml:mn>
                                       <mml:mo>,</mml:mo>
                                    </mml:mrow>
                                 </mml:math>
                              </ce:formula>
