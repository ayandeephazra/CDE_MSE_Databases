                  <ce:text>Coupled resonator</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0015">
                  <ce:text>Optical waveguide</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0020">
                  <ce:text>Directional coupler</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0025">
