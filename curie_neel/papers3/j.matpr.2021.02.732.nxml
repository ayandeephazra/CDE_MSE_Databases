                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>14.2</mml:mn>
                        </mml:mrow>
                     </mml:math>T, both <mml:math altimg="si6.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mo stretchy="false">[</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
