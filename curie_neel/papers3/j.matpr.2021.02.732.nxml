                                    <mml:mfenced close="]" open="[">
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>1</mml:mn>
                                             <mml:mo>-</mml:mo>
                                             <mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mi>ε</mml:mi>
                                                   <mml:mi>N</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>2</mml:mn>
                                                   <mml:mi>M</mml:mi>
