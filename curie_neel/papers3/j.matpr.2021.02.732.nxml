                              </mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:msubsup>
                        </mml:mrow>
                     </mml:math>.</ce:para>
                  <ce:para id="p0210" view="all">The nonlinear induced polarization of weakly-polar magnetized doped semiconductor crystal (which is time integral of induced current density) may be expressed as<ce:display>
                        <ce:formula id="e0070">
                           <ce:label>(11a)</ce:label>
                           <mml:math altimg="si81.svg">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>P</mml:mi>
                                    <mml:mrow>
                                       <mml:mi mathvariant="italic">cd</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:msub>
                                       <mml:mi>ω</mml:mi>
                                       <mml:mi>s</mml:mi>
                                    </mml:msub>
                                    <mml:mo stretchy="false">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mo>∫</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>J</mml:mi>
                                       <mml:mrow>
                                          <mml:mi mathvariant="italic">cd</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:msub>
                                          <mml:mi>ω</mml:mi>
                                          <mml:mi>s</mml:mi>
                                       </mml:msub>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mspace width="0.166667em"/>
                                    <mml:mi>d</mml:mi>
                                    <mml:mi>t</mml:mi>
                                 </mml:mrow>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo>-</mml:mo>
                                       <mml:msub>
                                          <mml:mi>J</mml:mi>
                                          <mml:mrow>
                                             <mml:mi mathvariant="italic">cd</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mrow>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>s</mml:mi>
                                          </mml:msub>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>i</mml:mi>
                                       <mml:msub>
                                          <mml:mi>ω</mml:mi>
                                          <mml:mi>s</mml:mi>
                                       </mml:msub>
                                    </mml:mrow>
                                 </mml:mfrac>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0215" view="all">which yields<ce:display>
