                           </mml:msub>
                        </mml:mrow>
                     </mml:math> as well as <mml:math altimg="si8.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mo stretchy="false">[</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:msubsup>
                                          <mml:mi>χ</mml:mi>
                                          <mml:mi>R</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:mn>3</mml:mn>
                                             <mml:mo stretchy="false">)</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
