                                                         <mml:mfrac>
                                                            <mml:mrow>
                                                               <mml:mn>2</mml:mn>
                                                               <mml:msub>
                                                                  <mml:mi>q</mml:mi>
                                                                  <mml:mi>s</mml:mi>
                                                               </mml:msub>
