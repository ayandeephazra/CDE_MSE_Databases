                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>s</mml:mi>
                                          </mml:msub>
                                          <mml:mo stretchy="false">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
