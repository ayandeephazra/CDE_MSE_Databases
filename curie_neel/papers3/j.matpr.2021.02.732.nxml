                                       <mml:mrow>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:msubsup>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mrow>
                                                <mml:mi mathvariant="italic">rs</mml:mi>
                                             </mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:msubsup>
                                          <mml:mo>+</mml:mo>
