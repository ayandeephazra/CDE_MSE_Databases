                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo stretchy="false">]</mml:mo>
                              </mml:mrow>
                              <mml:mi>r</mml:mi>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si8.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mo stretchy="false">[</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:msubsup>
                                          <mml:mi>χ</mml:mi>
                                          <mml:mi>R</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:mn>3</mml:mn>
                                             <mml:mo stretchy="false">)</mml:mo>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi mathvariant="italic">cd</mml:mi>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>I</mml:mi>
                                       <mml:mi>d</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo stretchy="false">]</mml:mo>
                              </mml:mrow>
                              <mml:mi>i</mml:mi>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math> on <mml:math altimg="si2.svg">
                        <mml:mrow>
                           <mml:msub>
