                                       <mml:mo>,</mml:mo>
                                       <mml:mi>I</mml:mi>
                                       <mml:mi>d</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo stretchy="false">]</mml:mo>
