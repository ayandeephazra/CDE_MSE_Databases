                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>ε</mml:mi>
                                          <mml:mi>∞</mml:mi>
                                       </mml:msub>
                                       <mml:msup>
                                          <mml:mi>e</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:msub>
                                          <mml:mi>k</mml:mi>
                                          <mml:mrow>
                                             <mml:mi mathvariant="italic">op</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mrow>
                                          <mml:mo stretchy="false">(</mml:mo>
                                          <mml:msub>
                                             <mml:mi>k</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                          <mml:mo>-</mml:mo>
                                          <mml:msub>
                                             <mml:mi>k</mml:mi>
