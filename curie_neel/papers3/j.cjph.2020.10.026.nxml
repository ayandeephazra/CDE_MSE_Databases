                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>C</mml:mi>
                                             <mml:mn>12</mml:mn>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:msub>
                                             <mml:mi>C</mml:mi>
                                             <mml:mn>13</mml:mn>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:msub>
                                             <mml:mi>C</mml:mi>
                                             <mml:mn>23</mml:mn>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">]</mml:mo>
                                 </mml:mrow>
