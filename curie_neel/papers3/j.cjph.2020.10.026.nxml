                                       <mml:mn>22</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">−</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mn>12</mml:mn>
                                    </mml:msub>
                                    <mml:mo>)</mml:mo>
