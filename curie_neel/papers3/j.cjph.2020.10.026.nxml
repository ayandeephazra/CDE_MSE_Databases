                     </ce:display>
                     <ce:display>
                        <ce:formula id="eqn0011">
                           <ce:label>(11)</ce:label>
                           <mml:math altimg="si11.svg">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mn>11</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">+</mml:mo>
                                    <mml:msub>
