                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mn>22</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mn>33</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mn>44</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mn>55</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mn>66</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">&gt;</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="eqn0010">
                           <ce:label>(10)</ce:label>
                           <mml:math altimg="si3.svg">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">[</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mn>11</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">+</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mn>22</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">+</mml:mo>
                                    <mml:msub>
                                       <mml:mi>C</mml:mi>
                                       <mml:mn>33</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">+</mml:mo>
                                    <mml:mn>2</mml:mn>
