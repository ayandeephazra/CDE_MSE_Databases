                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mi>K</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>cos</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mi>K</mml:mi>
                                                      <mml:mo linebreak="badbreak">+</mml:mo>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>k</mml:mi>
