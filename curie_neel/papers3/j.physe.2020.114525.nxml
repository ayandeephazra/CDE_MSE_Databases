                                          <mml:mo>−</mml:mo>
                                          <mml:mi>i</mml:mi>
                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>k</mml:mi>
                                          </mml:msub>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0105" view="all">It satisfies the following modified effective Schrödinger equation:<ce:display>
                        <ce:formula id="fd19">
                           <ce:label>(19)</ce:label>
                           <mml:math altimg="si84.svg" alttext="Equation 19.">
                              <mml:mrow>
                                 <mml:mi>i</mml:mi>
                                 <mml:mi>ℏ</mml:mi>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo>∂</mml:mo>
                                       <mml:msub>
                                          <mml:mi>ψ</mml:mi>
                                          <mml:mi>e</mml:mi>
                                       </mml:msub>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mover accent="true">
                                                <mml:mi>x</mml:mi>
                                                <mml:mo linebreak="badbreak">→</mml:mo>
                                             </mml:mover>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>t</mml:mi>
                                          </mml:mrow>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo>∂</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:munder>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mi>k</mml:mi>
                                 </mml:munder>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>c</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mover accent="true">
                                             <mml:mi>k</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">⋅</mml:mo>
                                          <mml:mover accent="true">
                                             <mml:mi>x</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mi>i</mml:mi>
                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>k</mml:mi>
                                          </mml:msub>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:msub>
                                             <mml:mi>m</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                       </mml:mfrac>
                                       <mml:mi>Δ</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>x</mml:mi>
                                          </mml:mrow>
                                          <mml:msub>
                                             <mml:mi>m</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                       </mml:mfrac>
                                       <mml:mi>V</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>x</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:munder>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mi>k</mml:mi>
                                 </mml:munder>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>c</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mover accent="true">
                                             <mml:mi>k</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">⋅</mml:mo>
                                          <mml:mover accent="true">
                                             <mml:mi>x</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mi>i</mml:mi>
                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>k</mml:mi>
                                          </mml:msub>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>or<ce:display>
                        <ce:formula id="fd20">
                           <ce:label>(20)</ce:label>
                           <mml:math altimg="si85.svg" alttext="Equation 20.">
                              <mml:mrow>
                                 <mml:mi>i</mml:mi>
                                 <mml:mi>ℏ</mml:mi>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo>∂</mml:mo>
                                       <mml:msub>
                                          <mml:mi>ψ</mml:mi>
                                          <mml:mi>e</mml:mi>
                                       </mml:msub>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mover accent="true">
                                                <mml:mi>x</mml:mi>
                                                <mml:mo linebreak="badbreak">→</mml:mo>
                                             </mml:mover>
                                             <mml:mo>,</mml:mo>
                                             <mml:mi>t</mml:mi>
                                          </mml:mrow>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo>∂</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:msub>
                                             <mml:mi>m</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                       </mml:mfrac>
                                       <mml:mi>Δ</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>x</mml:mi>
                                          </mml:mrow>
                                          <mml:msub>
                                             <mml:mi>m</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                       </mml:mfrac>
                                       <mml:mi>V</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>x</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ψ</mml:mi>
                                    <mml:mi>e</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mi>x</mml:mi>
                                          <mml:mo linebreak="badbreak">→</mml:mo>
                                       </mml:mover>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>which is valid for small <mml:math altimg="si86.svg">
                        <mml:mi>k</mml:mi>
                     </mml:math> or <mml:math altimg="si87.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mi>i</mml:mi>
                                 <mml:mover accent="true">
                                    <mml:mi>k</mml:mi>
                                    <mml:mo>→</mml:mo>
                                 </mml:mover>
                                 <mml:mo>⋅</mml:mo>
                                 <mml:mover accent="true">
                                    <mml:mi>x</mml:mi>
                                    <mml:mo>→</mml:mo>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> is slowly varying with respect to the lattice spacing [<ce:cross-ref id="crosref0235" refid="bib57">57</ce:cross-ref>]. Here <mml:math altimg="si88.svg">
                        <mml:mi>Δ</mml:mi>
                     </mml:math> is the Laplacian operator. We consider the Kronig–Penney model that consists of an infinite periodic array of rectangular potential barriers as shown in <ce:cross-ref id="crosref0240" refid="fig5">Fig. 5</ce:cross-ref>
                     <ce:float-anchor refid="fig5"/> [<ce:cross-ref id="crosref0245" refid="bib58">58</ce:cross-ref>]. If <mml:math altimg="si89.svg">
                        <mml:mi>L</mml:mi>
                     </mml:math> is the length of the lattice, then <mml:math altimg="si90.svg">
                        <mml:mrow>
                           <mml:mi>ψ</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>ψ</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>L</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> and hence we find <mml:math altimg="si91.svg">
                        <mml:mrow>
                           <mml:mi>k</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:mi>π</mml:mi>
                              </mml:mrow>
                              <mml:mi>L</mml:mi>
                           </mml:mfrac>
                           <mml:mi>n</mml:mi>
                           <mml:mo>,</mml:mo>
                           <mml:mi>n</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">±</mml:mo>
                           <mml:mn>1</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">±</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>...</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">±</mml:mo>
                           <mml:mfrac>
                              <mml:mi>N</mml:mi>
                              <mml:mn>2</mml:mn>
                           </mml:mfrac>
                        </mml:mrow>
                     </mml:math>.</ce:para>
                  <ce:para id="p0110" view="all">We set in what follows <mml:math altimg="si92.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>ψ</mml:mi>
                              <mml:mi>e</mml:mi>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo>,</mml:mo>
                              <mml:mi>t</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>i</mml:mi>
                                       <mml:mi>E</mml:mi>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>/</mml:mo>
                                    <mml:mi>ℏ</mml:mi>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mi>φ</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> which reduces equation [<ce:cross-ref id="crosref0250" refid="bib20">20</ce:cross-ref>] to:<ce:display>
                        <ce:formula id="fd21">
                           <ce:label>(21)</ce:label>
                           <mml:math altimg="si93.svg" alttext="Equation 21.">
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>″</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>m</mml:mi>
                                                <mml:mn>0</mml:mn>
                                             </mml:msub>
                                             <mml:mi>E</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                       </mml:mfrac>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>x</mml:mi>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                       <mml:mi>V</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>x</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si21.svg">
                        <mml:mi>E</mml:mi>
                     </mml:math> is the energy value above the well. The following solutions hold:<ce:display>
                        <ce:formula id="fd22">
                           <ce:label>(22)</ce:label>
                           <mml:math altimg="si94.svg" alttext="Equation 22.">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">{</mml:mo>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr>
                                          <mml:mtd>
                                             <mml:mrow>
                                                <mml:mn>0</mml:mn>
                                                <mml:mo linebreak="badbreak">&lt;</mml:mo>
                                                <mml:mi>x</mml:mi>
                                                <mml:mo linebreak="badbreak">&lt;</mml:mo>
                                                <mml:mi>a</mml:mi>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mi>b</mml:mi>
                                                <mml:mo>:</mml:mo>
                                                <mml:mi>φ</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mi>A</mml:mi>
                                                <mml:msup>
                                                   <mml:mi>e</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mi>K</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:msup>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:msup>
                                                   <mml:mi>A</mml:mi>
                                                   <mml:mo>′</mml:mo>
                                                </mml:msup>
                                                <mml:msup>
                                                   <mml:mi>e</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mo>−</mml:mo>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mi>K</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:msup>
                                                <mml:mo>,</mml:mo>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mn>0</mml:mn>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                       <mml:mtr>
                                          <mml:mtd>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:mi>b</mml:mi>
                                                <mml:mo linebreak="badbreak">&lt;</mml:mo>
                                                <mml:mi>x</mml:mi>
                                                <mml:mo linebreak="badbreak">&lt;</mml:mo>
                                                <mml:mn>0</mml:mn>
                                                <mml:mo>:</mml:mo>
                                                <mml:mtext> </mml:mtext>
                                                <mml:mi>φ</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mi>B</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mtext>Ai</mml:mtext>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mo>−</mml:mo>
                                                            <mml:mi>R</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mo stretchy="true">(</mml:mo>
                                                               <mml:mrow>
                                                                  <mml:msup>
                                                                     <mml:mi>K</mml:mi>
                                                                     <mml:mn>2</mml:mn>
                                                                  </mml:msup>
                                                                  <mml:mo linebreak="badbreak">+</mml:mo>
                                                                  <mml:mi>P</mml:mi>
                                                                  <mml:mi>x</mml:mi>
                                                               </mml:mrow>
                                                               <mml:mo stretchy="true">)</mml:mo>
                                                            </mml:mrow>
                                                         </mml:mrow>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:msup>
                                                   <mml:mi>B</mml:mi>
                                                   <mml:mo>′</mml:mo>
                                                </mml:msup>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mtext>Bi</mml:mtext>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mo>−</mml:mo>
                                                            <mml:mi>R</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mo stretchy="true">(</mml:mo>
                                                               <mml:mrow>
                                                                  <mml:msup>
                                                                     <mml:mi>K</mml:mi>
                                                                     <mml:mn>2</mml:mn>
                                                                  </mml:msup>
                                                                  <mml:mo linebreak="badbreak">+</mml:mo>
                                                                  <mml:mi>P</mml:mi>
                                                                  <mml:mi>x</mml:mi>
                                                               </mml:mrow>
                                                               <mml:mo stretchy="true">)</mml:mo>
                                                            </mml:mrow>
                                                         </mml:mrow>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo>,</mml:mo>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>V</mml:mi>
                                                   <mml:mn>0</mml:mn>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0115" view="all">
                     <mml:math altimg="si95.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>K</mml:mi>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>m</mml:mi>
                                    <mml:mn>0</mml:mn>
                                 </mml:msub>
                                 <mml:mi>E</mml:mi>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:msup>
                                    <mml:mi>ℏ</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                              </mml:mrow>
                           </mml:mfrac>
                           <mml:mo>,</mml:mo>
                           <mml:mspace width="0.25em"/>
                           <mml:mi>P</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:mi>α</mml:mi>
                                 <mml:msub>
                                    <mml:mi>V</mml:mi>
                                    <mml:mn>0</mml:mn>
                                 </mml:msub>
                              </mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:mfrac>
                           <mml:mo>,</mml:mo>
                           <mml:mspace width="0.25em"/>
                           <mml:mi>R</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">(</mml:mo>
                                 <mml:mo linebreak="badbreak">−</mml:mo>
                                 <mml:mfrac>
                                    <mml:mn>2</mml:mn>
                                    <mml:mi>α</mml:mi>
                                 </mml:mfrac>
                                 <mml:mo stretchy="true">)</mml:mo>
                              </mml:mrow>
                              <mml:mrow>
