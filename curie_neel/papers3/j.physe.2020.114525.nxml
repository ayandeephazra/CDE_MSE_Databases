                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mover accent="true">
                                       <mml:mi>x</mml:mi>
                                       <mml:mo linebreak="badbreak">→</mml:mo>
                                    </mml:mover>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:munder>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mi>k</mml:mi>
                                 </mml:munder>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>c</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mover accent="true">
                                             <mml:mi>k</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
