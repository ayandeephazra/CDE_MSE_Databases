                                          </mml:msup>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>x</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                          <mml:msub>
                                             <mml:mi>u</mml:mi>
                                             <mml:mi>k</mml:mi>
