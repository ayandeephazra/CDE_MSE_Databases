                                             <mml:mi>k</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">⋅</mml:mo>
                                          <mml:mover accent="true">
                                             <mml:mi>x</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi>i</mml:mi>
                                          <mml:msub>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mi>k</mml:mi>
                                          </mml:msub>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
