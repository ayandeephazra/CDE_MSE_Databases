                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mover accent="true">
                                       <mml:mi>x</mml:mi>
                                       <mml:mo linebreak="badbreak">→</mml:mo>
                                    </mml:mover>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ψ</mml:mi>
                                    <mml:mi>e</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mover accent="true">
                                          <mml:mi>x</mml:mi>
                                          <mml:mo linebreak="badbreak">→</mml:mo>
                                       </mml:mover>
                                       <mml:mo>,</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>since for <mml:math altimg="si80.svg">
                        <mml:mrow>
                           <mml:mi>k</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
