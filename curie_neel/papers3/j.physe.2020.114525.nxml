                                          </mml:mrow>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>′</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo linebreak="newline">+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mn>1</mml:mn>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:mi>μ</mml:mi>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mi>x</mml:mi>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:msup>
                                             <mml:mi>μ</mml:mi>
                                             <mml:mo>″</mml:mo>
                                          </mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>x</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mfrac>
                                             <mml:mn>1</mml:mn>
                                             <mml:mn>4</mml:mn>
                                          </mml:mfrac>
                                          <mml:msup>
                                             <mml:msup>
                                                <mml:mi>μ</mml:mi>
                                                <mml:mo>′</mml:mo>
                                             </mml:msup>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>x</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mfrac>
                                             <mml:mn>1</mml:mn>
                                             <mml:mn>2</mml:mn>
                                          </mml:mfrac>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:msup>
                                                   <mml:mi>m</mml:mi>
                                                   <mml:mo>′</mml:mo>
                                                </mml:msup>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>m</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                             </mml:mrow>
                                          </mml:mfrac>
                                          <mml:msup>
                                             <mml:mi>μ</mml:mi>
                                             <mml:mo>′</mml:mo>
                                          </mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>x</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                                <mml:mi>m</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                             </mml:mrow>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mfrac>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mi>E</mml:mi>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0055" view="all">The constraint <mml:math altimg="si24.svg">
                        <mml:mrow>
                           <mml:mn>2</mml:mn>
                           <mml:msup>
                              <mml:mi>μ</mml:mi>
                              <mml:mo>′</mml:mo>
                           </mml:msup>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>m</mml:mi>
                                    <mml:mo>′</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mi>m</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:mfrac>
                        </mml:mrow>
                     </mml:math>, i.e. <mml:math altimg="si25.svg">
                        <mml:mrow>
                           <mml:mi>m</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>C</mml:mi>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:mi>μ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> (<mml:math altimg="si26.svg">
                        <mml:mi>C</mml:mi>
                     </mml:math> is an integration constant) simplifies considerably equation [<ce:cross-ref id="crosref0160" refid="bib3">3</ce:cross-ref>] to:<ce:display>
                        <ce:formula id="fd4">
                           <ce:label>(4)</ce:label>
                           <mml:math altimg="si27.svg" alttext="Equation 4.">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msup>
                                          <mml:mi>μ</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>x</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>″</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mn>1</mml:mn>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:mi>μ</mml:mi>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mi>x</mml:mi>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:msup>
                                             <mml:mi>μ</mml:mi>
                                             <mml:mo>″</mml:mo>
                                          </mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>x</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mfrac>
                                             <mml:mn>3</mml:mn>
                                             <mml:mn>4</mml:mn>
                                          </mml:mfrac>
                                          <mml:msup>
                                             <mml:msup>
                                                <mml:mi>μ</mml:mi>
                                                <mml:mo>′</mml:mo>
                                             </mml:msup>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>x</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                                <mml:mi>C</mml:mi>
                                                <mml:msup>
                                                   <mml:mi>e</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>μ</mml:mi>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mi>x</mml:mi>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                   </mml:mrow>
                                                </mml:msup>
                                             </mml:mrow>
                                             <mml:msup>
                                                <mml:mi>ℏ</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mfrac>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mi>E</mml:mi>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0060" view="all">By selecting a weak <mml:math altimg="si28.svg">
                        <mml:mrow>
                           <mml:mi>m</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msub>
                              <mml:mi>m</mml:mi>
                              <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mi>α</mml:mi>
                                 <mml:msup>
                                    <mml:mi>x</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> where <mml:math altimg="si29.svg">
                        <mml:mi>α</mml:mi>
                     </mml:math> is a real tiny parameter assumed to be positive all through this section and <mml:math altimg="si30.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>m</mml:mi>
                              <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>m</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo linebreak="badbreak">=</mml:mo>
                              <mml:mn>0</mml:mn>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>C</mml:mi>
                        </mml:mrow>
                     </mml:math>, it is easy to verify that equation [<ce:cross-ref id="crosref0165" refid="bib4">4</ce:cross-ref>] is reduced for <mml:math altimg="si31.svg">
                        <mml:mrow>
                           <mml:mi>V</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0</mml:mn>
                        </mml:mrow>
                     </mml:math> to the following equation:<ce:display>
                        <ce:formula id="fd5">
                           <ce:label>(5)</ce:label>
                           <mml:math altimg="si32.svg" alttext="Equation 5.">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>μ</mml:mi>
                                                   <mml:mn>0</mml:mn>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mi>α</mml:mi>
                                                <mml:msup>
                                                   <mml:mi>x</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msup>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>″</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>α</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mn>1</mml:mn>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:msub>
                                                <mml:mi>μ</mml:mi>
                                                <mml:mn>0</mml:mn>
                                             </mml:msub>
                                             <mml:mo linebreak="badbreak">−</mml:mo>
                                             <mml:mi>α</mml:mi>
                                             <mml:msup>
                                                <mml:mi>x</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mn>3</mml:mn>
                                       <mml:msup>
                                          <mml:mi>α</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:msup>
                                          <mml:mi>x</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:mi>C</mml:mi>
                                             <mml:msup>
                                                <mml:mi>e</mml:mi>
                                                <mml:mrow>
                                                   <mml:mn>2</mml:mn>
                                                   <mml:mrow>
                                                      <mml:mo stretchy="true">(</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>μ</mml:mi>
                                                         <mml:mn>0</mml:mn>
                                                      </mml:msub>
                                                      <mml:mo linebreak="badbreak">−</mml:mo>
                                                      <mml:mi>α</mml:mi>
                                                      <mml:msup>
                                                         <mml:mi>x</mml:mi>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msup>
                                                      <mml:mo stretchy="true">)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:msup>
                                             <mml:mi>ℏ</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mrow>
                                             <mml:mi>E</mml:mi>
                                             <mml:mo linebreak="badbreak">−</mml:mo>
                                             <mml:mi>V</mml:mi>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mi>x</mml:mi>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0065" view="all">Since <mml:math altimg="si29.svg">
                        <mml:mi>α</mml:mi>
                     </mml:math> is assumed to be a too small real positive parameter using the series expansion <mml:math altimg="si33.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>2</mml:mn>
                                 <mml:mi>α</mml:mi>
                                 <mml:msup>
                                    <mml:mi>x</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">≈</mml:mo>
                           <mml:mn>1</mml:mn>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">−</mml:mo>
                           <mml:mn>2</mml:mn>
                           <mml:mi>α</mml:mi>
                           <mml:msup>
                              <mml:mi>x</mml:mi>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">+</mml:mo>
                           <mml:mtext>O</mml:mtext>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:msup>
                                 <mml:mi>α</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si34.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">(</mml:mo>
                                 <mml:mn>1</mml:mn>
                                 <mml:mo linebreak="badbreak">−</mml:mo>
                                 <mml:mi>β</mml:mi>
                                 <mml:msup>
                                    <mml:mi>x</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msup>
                                 <mml:mo stretchy="true">)</mml:mo>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>1</mml:mn>
                              </mml:mrow>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">≈</mml:mo>
                           <mml:mn>1</mml:mn>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">+</mml:mo>
                           <mml:mi>β</mml:mi>
                           <mml:msup>
                              <mml:mi>x</mml:mi>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">+</mml:mo>
                           <mml:mtext>O</mml:mtext>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:msup>
                                 <mml:mi>β</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> for any tiny real parameter <mml:math altimg="si35.svg">
                        <mml:mi>β</mml:mi>
                     </mml:math>, this equation is reduced after setting <mml:math altimg="si36.svg">
                        <mml:mrow>
                           <mml:mfrac>
                              <mml:mn>1</mml:mn>
                              <mml:mn>2</mml:mn>
                           </mml:mfrac>
                           <mml:mi>ln</mml:mi>
                           <mml:mspace width="0.25em"/>
                           <mml:msub>
                              <mml:mi>m</mml:mi>
                              <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msub>
                              <mml:mi>μ</mml:mi>
                              <mml:mn>0</mml:mn>
                           </mml:msub>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>1</mml:mn>
                        </mml:mrow>
                     </mml:math> for convenience and taking <mml:math altimg="si31.svg">
                        <mml:mrow>
                           <mml:mi>V</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0</mml:mn>
                        </mml:mrow>
                     </mml:math> (free particles) to:<ce:display>
                        <ce:formula id="fd6">
                           <ce:label>(6)</ce:label>
                           <mml:math altimg="si37.svg" alttext="Equation 6.">
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>″</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mi>e</mml:mi>
                                                <mml:mn>4</mml:mn>
                                             </mml:msup>
                                             <mml:mi>E</mml:mi>
                                          </mml:mrow>
                                          <mml:msup>
                                             <mml:mi>ℏ</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mfrac>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>α</mml:mi>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mi>α</mml:mi>
                                             <mml:mi>E</mml:mi>
                                             <mml:msup>
                                                <mml:mi>e</mml:mi>
                                                <mml:mn>4</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:msup>
                                             <mml:mi>ℏ</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mfrac>
                                       <mml:msup>
                                          <mml:mi>x</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0070" view="all">This equation resembles surprisingly the quantum harmonic oscillator where its energy level is quantized and is given by:<ce:display>
                        <ce:formula id="fd7">
                           <ce:label>(7)</ce:label>
                           <mml:math altimg="si38.svg" alttext="Equation 7.">
                              <mml:mrow>
                                 <mml:mi>E</mml:mi>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mn>4</mml:mn>
                                       <mml:mi>α</mml:mi>
                                       <mml:msup>
                                          <mml:mi>ℏ</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mn>4</mml:mn>
                                    </mml:msup>
                                 </mml:mfrac>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mi>n</mml:mi>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:mfrac>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mn>2</mml:mn>
                                                </mml:mfrac>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>with <mml:math altimg="si39.svg">
                        <mml:mrow>
                           <mml:mi>n</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0,1,2</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>...</mml:mn>
                        </mml:mrow>
                     </mml:math> and its associated wave function is given by:<ce:display>
                        <ce:formula id="fd8">
                           <ce:label>(8)</ce:label>
                           <mml:math altimg="si40.svg" alttext="Equation 8.">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mi>n</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:mi>α</mml:mi>
                                             <mml:msqrt>
                                                <mml:mrow>
                                                   <mml:msup>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mi>n</mml:mi>
                                                            <mml:mo linebreak="badbreak">+</mml:mo>
                                                            <mml:mfrac>
                                                               <mml:mn>1</mml:mn>
                                                               <mml:mn>2</mml:mn>
                                                            </mml:mfrac>
                                                         </mml:mrow>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:msup>
                                                   <mml:mo linebreak="badbreak">+</mml:mo>
                                                   <mml:mfrac>
                                                      <mml:mn>1</mml:mn>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                             </mml:msqrt>
                                          </mml:mrow>
                                          <mml:mi>π</mml:mi>
                                       </mml:mfrac>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>4</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mfrac>
                                    <mml:mn>1</mml:mn>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mn>2</mml:mn>
                                             <mml:mi>n</mml:mi>
                                          </mml:msup>
                                          <mml:mi>n</mml:mi>
                                          <mml:mo>!</mml:mo>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mfrac>
                                 <mml:msub>
                                    <mml:mi>H</mml:mi>
                                    <mml:mi>n</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:mn>2</mml:mn>
                                             <mml:mi>α</mml:mi>
                                             <mml:msqrt>
                                                <mml:mrow>
                                                   <mml:msup>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mi>n</mml:mi>
                                                            <mml:mo linebreak="badbreak">+</mml:mo>
                                                            <mml:mfrac>
                                                               <mml:mn>1</mml:mn>
                                                               <mml:mn>2</mml:mn>
                                                            </mml:mfrac>
                                                         </mml:mrow>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:msup>
                                                   <mml:mo linebreak="badbreak">+</mml:mo>
                                                   <mml:mfrac>
                                                      <mml:mn>1</mml:mn>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                             </mml:msqrt>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:msup>
                                    <mml:mi>e</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mi>α</mml:mi>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mi>n</mml:mi>
                                                      <mml:mo linebreak="badbreak">+</mml:mo>
                                                      <mml:mfrac>
                                                         <mml:mn>1</mml:mn>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:mfrac>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                             <mml:mo linebreak="badbreak">+</mml:mo>
                                             <mml:mfrac>
                                                <mml:mn>1</mml:mn>
                                                <mml:mn>2</mml:mn>
                                             </mml:mfrac>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:msup>
                                          <mml:mi>x</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si41.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>H</mml:mi>
                              <mml:mi>n</mml:mi>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>z</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">(</mml:mo>
                                 <mml:mo linebreak="badbreak">−</mml:mo>
                                 <mml:mn>1</mml:mn>
                                 <mml:mo stretchy="true">)</mml:mo>
                              </mml:mrow>
                              <mml:mi>n</mml:mi>
                           </mml:msup>
                           <mml:mspace width="0.25em"/>
                           <mml:mi>exp</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:msup>
                                 <mml:mi>z</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:msup>
                              <mml:mi>D</mml:mi>
                              <mml:mi>n</mml:mi>
                           </mml:msup>
                           <mml:mspace width="0.25em"/>
                           <mml:mi>exp</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mo linebreak="badbreak">−</mml:mo>
                              <mml:msup>
                                 <mml:mi>z</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> are Hermite polynomials where <mml:math altimg="si42.svg">
                        <mml:mrow>
                           <mml:mi>D</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mi>d</mml:mi>
                              <mml:mrow>
                                 <mml:mi>d</mml:mi>
                                 <mml:mi>z</mml:mi>
                              </mml:mrow>
                           </mml:mfrac>
                        </mml:mrow>
                     </mml:math> [<ce:cross-ref id="crosref0170" refid="bib46">46</ce:cross-ref>]. We plot in <ce:cross-refs id="crosrefs0035" refid="fig1 fig2">Figs. 1 and 2</ce:cross-refs>
                     <ce:float-anchor refid="fig1"/>
                     <ce:float-anchor refid="fig2"/> the variations of <mml:math altimg="si43.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">|</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>φ</mml:mi>
                                       <mml:mi>n</mml:mi>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mi>x</mml:mi>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mo stretchy="true">|</mml:mo>
                              </mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> (probability density function) for <mml:math altimg="si44.svg">
                        <mml:mrow>
                           <mml:mi>n</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0,1,2,3</mml:mn>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si45.svg">
                        <mml:mrow>
                           <mml:mi>α</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0.01</mml:mn>
                        </mml:mrow>
                     </mml:math>:</ce:para>
                  <ce:para id="p0075" view="all">The free particle system behaves as a harmonic oscillator due to the presence of a GMO and a slowly decaying PDM distribution function of exponential form which is mostly used in the study of semiconductors, quantum wells, quantum dots and impurities in crystals, among many others in particular in the presence of an exponential Morse potential [<ce:cross-refs id="crosrefs0040" refid="bib47 bib48 bib49 bib50 bib51">47–51</ce:cross-refs>]. Surprisingly, the quantum system is free from any potential and behaves as the free particle which is equally likely to be found everywhere on the position axis. Nevertheless, the energy levels are much lower than the conventional result:<mml:math altimg="si50.svg">
                        <mml:mrow>
                           <mml:mi>E</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>ℏ</mml:mi>
                           <mml:mi>ω</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>n</mml:mi>
                              <mml:mo linebreak="badbreak">+</mml:mo>
                              <mml:mfrac>
                                 <mml:mn>1</mml:mn>
                                 <mml:mn>2</mml:mn>
                              </mml:mfrac>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math>, <mml:math altimg="si51.svg">
                        <mml:mi>ω</mml:mi>
                     </mml:math> being the frequency of the oscillator. One more illustration corresponds to the particular case where <mml:math altimg="si52.svg">
                        <mml:mrow>
                           <mml:mi>m</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mi>exp</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mo linebreak="badbreak">−</mml:mo>
                              <mml:mfrac>
                                 <mml:mn>2</mml:mn>
                                 <mml:mn>3</mml:mn>
                              </mml:mfrac>
                              <mml:msup>
                                 <mml:mi>μ</mml:mi>
                                 <mml:mn>3</mml:mn>
                              </mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">(</mml:mo>
                                 <mml:mi>x</mml:mi>
                                 <mml:mo stretchy="true">)</mml:mo>
                              </mml:mrow>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math>, <mml:math altimg="si53.svg">
                        <mml:mrow>
                           <mml:mi>μ</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msqrt>
                              <mml:mrow>
                                 <mml:mi>α</mml:mi>
                                 <mml:mi>x</mml:mi>
                                 <mml:mo>+</mml:mo>
                                 <mml:mn>1</mml:mn>
                              </mml:mrow>
                           </mml:msqrt>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">−</mml:mo>
                           <mml:mn>1</mml:mn>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si31.svg">
                        <mml:mrow>
                           <mml:mi>V</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0</mml:mn>
                        </mml:mrow>
                     </mml:math>. It is easy to verify that equation [<ce:cross-ref id="crosref0175" refid="bib3">3</ce:cross-ref>] in that case is simplified up to the order <mml:math altimg="si54.svg">
                        <mml:mrow>
                           <mml:mtext>O</mml:mtext>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:msup>
                                 <mml:mi>α</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> to the following differential equation:<ce:display>
                        <ce:formula id="fd9">
                           <ce:label>(9)</ce:label>
                           <mml:math altimg="si55.svg" alttext="Equation 9.">
                              <mml:mrow>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>″</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">+</mml:mo>
                                 <mml:mi>α</mml:mi>
                                 <mml:msup>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mo>′</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="goodbreak">+</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:msub>
                                          <mml:mi>m</mml:mi>
                                          <mml:mn>0</mml:mn>
                                       </mml:msub>
                                       <mml:mi>E</mml:mi>
                                    </mml:mrow>
                                    <mml:msup>
                                       <mml:mi>ℏ</mml:mi>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mi>e</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mfrac>
                                          <mml:mn>2</mml:mn>
                                          <mml:mn>3</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mi>φ</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:mn>0</mml:mn>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>which corresponds to the case of forced oscillations. In fact, the quantum dynamics of the damped harmonic oscillator is of particular interest since it is connected to quantum mechanics with dissipation [<ce:cross-ref id="crosref0180" refid="bib52">52</ce:cross-ref>,<ce:cross-ref id="crosref0185" refid="bib53">53</ce:cross-ref>]. Following the arguments of [<ce:cross-ref id="crosref0190" refid="bib54">54</ce:cross-ref>,<ce:cross-ref id="crosref0195" refid="bib55">55</ce:cross-ref>], the expectation value of energy is given by:<ce:display>
                        <ce:formula id="fd10">
                           <ce:label>(10)</ce:label>
                           <mml:math altimg="si56.svg" alttext="Equation 10.">
                              <mml:mrow>
                                 <mml:mo>&lt;</mml:mo>
                                 <mml:mover accent="true">
                                    <mml:mi>E</mml:mi>
                                    <mml:mo>ˆ</mml:mo>
                                 </mml:mover>
                                 <mml:mo linebreak="badbreak">&gt;</mml:mo>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>n</mml:mi>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mi>ℏ</mml:mi>
                                 <mml:mfrac>
                                    <mml:msup>
                                       <mml:mi>ω</mml:mi>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mi>ω</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:msup>
                                             <mml:mi>α</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mi>e</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mn>2</mml:mn>
                                       <mml:mi>α</mml:mi>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>n</mml:mi>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:msub>
                                          <mml:mi>m</mml:mi>
                                          <mml:mn>0</mml:mn>
                                       </mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:msup>
                                          <mml:mi>e</mml:mi>
                                          <mml:mrow>
                                             <mml:mo>−</mml:mo>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                                <mml:mo linebreak="badbreak">/</mml:mo>
                                                <mml:mn>3</mml:mn>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:msup>
                                    </mml:mrow>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                          <mml:msub>
                                             <mml:mi>m</mml:mi>
                                             <mml:mn>0</mml:mn>
                                          </mml:msub>
                                          <mml:mi>E</mml:mi>
                                          <mml:msup>
                                             <mml:mi>e</mml:mi>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:mrow>
                                                   <mml:mn>2</mml:mn>
                                                   <mml:mo linebreak="badbreak">/</mml:mo>
                                                   <mml:mn>3</mml:mn>
                                                </mml:mrow>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:msup>
                                             <mml:mi>α</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:msup>
                                             <mml:mi>ℏ</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mi>e</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mn>2</mml:mn>
                                       <mml:mi>α</mml:mi>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si57.svg">
                        <mml:mrow>
                           <mml:mi>n</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>1,2,3</mml:mn>
                           <mml:mo>,</mml:mo>
                           <mml:mn>...</mml:mn>
                        </mml:mrow>
                     </mml:math>, <mml:math altimg="si58.svg">
                        <mml:mrow>
                           <mml:mi>ω</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">&gt;</mml:mo>
                           <mml:mi>α</mml:mi>
                        </mml:mrow>
                     </mml:math> and <mml:math altimg="si59.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mi>ω</mml:mi>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
                              <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:msub>
                                    <mml:mi>m</mml:mi>
                                    <mml:mn>0</mml:mn>
                                 </mml:msub>
                                 <mml:mi>E</mml:mi>
                              </mml:mrow>
                              <mml:msup>
                                 <mml:mi>ℏ</mml:mi>
                                 <mml:mn>2</mml:mn>
                              </mml:msup>
                           </mml:mfrac>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                    <mml:mo>/</mml:mo>
                                    <mml:mn>3</mml:mn>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math>. The corresponding wave function is given by:<ce:display>
                        <ce:formula id="fd11">
                           <ce:label>(11)</ce:label>
                           <mml:math altimg="si60.svg" alttext="Equation 11.">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>φ</mml:mi>
                                    <mml:mi>n</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>x</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mn>1</mml:mn>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mn>2</mml:mn>
                                             <mml:mi>n</mml:mi>
                                          </mml:msup>
                                          <mml:mi>n</mml:mi>
                                          <mml:mo>!</mml:mo>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mfrac>
                                          <mml:msqrt>
                                             <mml:mrow>
                                                <mml:msup>
                                                   <mml:mi>ω</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msup>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:msup>
                                                   <mml:mi>α</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msup>
                                             </mml:mrow>
                                          </mml:msqrt>
                                          <mml:mi>π</mml:mi>
                                       </mml:mfrac>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mn>4</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:msup>
                                    <mml:mi>e</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mi>i</mml:mi>
                                       <mml:msqrt>
                                          <mml:mrow>
                                             <mml:msup>
                                                <mml:mi>ω</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                             <mml:mo linebreak="badbreak">−</mml:mo>
                                             <mml:msup>
                                                <mml:mi>α</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msup>
                                          </mml:mrow>
                                       </mml:msqrt>
                                       <mml:mfrac>
                                          <mml:msup>
                                             <mml:mi>x</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mn>2</mml:mn>
                                       </mml:mfrac>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:msub>
                                    <mml:mi>H</mml:mi>
                                    <mml:mi>n</mml:mi>
                                 </mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:msup>
                                                   <mml:mi>ω</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msup>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:msup>
                                                   <mml:mi>α</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msup>
                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mfrac>
                                                <mml:mn>1</mml:mn>
                                                <mml:mn>4</mml:mn>
                                             </mml:mfrac>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mi>x</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0080" view="all">Equations [<ce:cross-ref id="crosref0200" refid="bib10">10</ce:cross-ref>,<ce:cross-ref id="crosref0205" refid="bib11">11</ce:cross-ref>] tend to their standard forms for <mml:math altimg="si61.svg">
                        <mml:mrow>
                           <mml:mi>α</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0</mml:mn>
                        </mml:mrow>
                     </mml:math>. We plot in <ce:cross-refs id="crosrefs0045" refid="fig3 fig4">Figs. 3 and 4</ce:cross-refs>
                     <ce:float-anchor refid="fig3"/>
                     <ce:float-anchor refid="fig4"/> the variations of <mml:math altimg="si43.svg">
                        <mml:mrow>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mo stretchy="true">|</mml:mo>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>φ</mml:mi>
                                       <mml:mi>n</mml:mi>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mi>x</mml:mi>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                                 <mml:mo stretchy="true">|</mml:mo>
                              </mml:mrow>
                              <mml:mn>2</mml:mn>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> for <mml:math altimg="si62.svg">
                        <mml:mrow>
                           <mml:mi>n</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>1,2,3,4</mml:mn>
                        </mml:mrow>
                     </mml:math>, <mml:math altimg="si45.svg">
                        <mml:mrow>
                           <mml:mi>α</mml:mi>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mn>0.01</mml:mn>
                        </mml:mrow>
                     </mml:math> and for normalized physical units:</ce:para>
                  <ce:para id="p0085" view="all">These numerical solutions differ from the standard oscillator free from damping. In fact, a variety of physical models may be derived from equation [<ce:cross-ref id="crosref0210" refid="bib3">3</ce:cross-ref>] which may describe dissimilar models in semiconductors and nanocrystals.</ce:para>
               </ce:section>
               <ce:section id="sec3" view="all">
                  <ce:label>3</ce:label>
                  <ce:section-title id="sectitle0035">Electrons in a periodic potential with PDM</ce:section-title>
                  <ce:para id="p0090" view="all">In this section, we discuss the dynamics of electrons moving in a weak periodic potential and to derive the corresponding energy levels. The wave function of an electron according to Bloch's theorem is given by <mml:math altimg="si64.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>ψ</mml:mi>
                              <mml:mi>k</mml:mi>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:msup>
                              <mml:mi>e</mml:mi>
                              <mml:mrow>
                                 <mml:mi>i</mml:mi>
                                 <mml:mi>k</mml:mi>
                                 <mml:mi>x</mml:mi>
                              </mml:mrow>
                           </mml:msup>
                           <mml:msub>
                              <mml:mi>u</mml:mi>
                              <mml:mi>p</mml:mi>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> where <mml:math altimg="si65.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>u</mml:mi>
                              <mml:mi>p</mml:mi>
                           </mml:msub>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> is a lattice periodic function of <mml:math altimg="si17.svg">
                        <mml:mi>x</mml:mi>
                     </mml:math> satisfying the relation <mml:math altimg="si66.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>u</mml:mi>
                              <mml:mi>p</mml:mi>
