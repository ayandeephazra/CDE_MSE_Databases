                                       <mml:msub>
                                          <mml:mi>m</mml:mi>
                                          <mml:mi>e</mml:mi>
                                       </mml:msub>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mo>,</mml:mo>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si77.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>m</mml:mi>
                              <mml:mi>e</mml:mi>
                           </mml:msub>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                           <mml:mfrac>
