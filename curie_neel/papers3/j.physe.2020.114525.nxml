                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>x</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math> or more explicitly using equation [<ce:cross-ref id="crosref0220" refid="bib1">1</ce:cross-ref>]:<ce:display>
                        <ce:formula id="fd13">
                           <ce:label>(13)</ce:label>
                           <mml:math altimg="si72.svg" alttext="Equation 13.">
                              <mml:mtable columnalign="left">
