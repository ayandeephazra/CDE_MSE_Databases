                                             </mml:mrow>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mi>k</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>cos</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                                <mml:mi>k</mml:mi>
