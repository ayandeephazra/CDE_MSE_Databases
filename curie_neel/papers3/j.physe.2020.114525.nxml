                                          <mml:mi>cos</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
