                                          <mml:mo linebreak="badbreak">→</mml:mo>
                                       </mml:mover>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:msup>
                                       <mml:mi>e</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mover accent="true">
                                             <mml:mi>k</mml:mi>
                                             <mml:mo linebreak="badbreak">→</mml:mo>
                                          </mml:mover>
                                          <mml:mo linebreak="badbreak">⋅</mml:mo>
