                                       <mml:mo>,</mml:mo>
                                       <mml:mi>t</mml:mi>
                                    </mml:mrow>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:munder>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mi>k</mml:mi>
                                 </mml:munder>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>c</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:msub>
                                       <mml:mi>u</mml:mi>
                                       <mml:mi>k</mml:mi>
                                    </mml:msub>
                                    <mml:mrow>
