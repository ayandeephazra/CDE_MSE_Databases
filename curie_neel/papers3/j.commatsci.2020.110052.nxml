                                                </mml:mrow>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>c</mml:mi>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi>∊</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>0</mml:mn>
                                                </mml:mrow>
