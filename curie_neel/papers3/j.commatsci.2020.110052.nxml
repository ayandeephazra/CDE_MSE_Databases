                                    <mml:mrow>
                                       <mml:mo>̂</mml:mo>
                                    </mml:mrow>
                                 </mml:mover>
                              </mml:mrow>
                           </mml:msubsup>
                           <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
