         <head>
            <ce:title id="title0010">Electronic and mechanical property of high electron mobility semiconductor Bi<ce:inf loc="post">2</ce:inf>O<ce:inf loc="post">2</ce:inf>Se</ce:title>
            <ce:author-group id="augrp0010">
               <ce:author id="au1" author-id="S0925838818322412-b08a86687e078e4401b354a9dbc2724b">
                  <ce:given-name>Jiang</ce:given-name>
                  <ce:surname>Liu</ce:surname>
               </ce:author>
               <ce:author id="au2" author-id="S0925838818322412-92ed0fbecde12730b33259692e1361be">
                  <ce:given-name>Liqiang</ce:given-name>
                  <ce:surname>Tian</ce:surname>
                  <ce:cross-ref refid="cor1" id="crosref0010">
                     <ce:sup loc="post">∗</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:author id="au3" author-id="S0925838818322412-69c1b28d80b1281f822378a5da458a38">
                  <ce:given-name>Yao</ce:given-name>
                  <ce:surname>Mou</ce:surname>
               </ce:author>
               <ce:author id="au4" author-id="S0925838818322412-57b1b73ee7e543cbb0fdfbb91777a574">
                  <ce:given-name>Wanli</ce:given-name>
                  <ce:surname>Jia</ce:surname>
               </ce:author>
               <ce:author id="au5" author-id="S0925838818322412-0cb3302ff356893f510d490f572920db">
                  <ce:given-name>Lin</ce:given-name>
                  <ce:surname>Zhang</ce:surname>
               </ce:author>
               <ce:author id="au6" author-id="S0925838818322412-7529044410c13d78854eacb9e6bd13dd">
                  <ce:given-name>Rujun</ce:given-name>
                  <ce:surname>Liu</ce:surname>
               </ce:author>
               <ce:affiliation id="aff1" affiliation-id="S0925838818322412-ee11a90856e39e7fcbd2e488e07b36ec">
                  <ce:textfn>Department of Applied Physics, Xi'an University of Technology, Xi'an 710054, China</ce:textfn>
                  <sa:affiliation>
                     <sa:organization>Department of Applied Physics</sa:organization>
                     <sa:organization>Xi'an University of Technology</sa:organization>
                     <sa:city>Xi'an</sa:city>
                     <sa:postal-code>710054</sa:postal-code>
