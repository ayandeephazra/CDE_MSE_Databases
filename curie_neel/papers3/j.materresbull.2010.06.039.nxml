                                 <mml:msub>
                                    <mml:mi>g</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msub>
                                 <mml:mo>â</mml:mo>
                                 <mml:msub>
                                    <mml:mi>g</mml:mi>
                                    <mml:mn>3</mml:mn>
                                 </mml:msub>
