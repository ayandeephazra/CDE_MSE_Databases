                                          <mml:mi>T</mml:mi>
                                          <mml:mo>â</mml:mo>
                                          <mml:mn>75.6</mml:mn>
                                       </mml:mrow>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
