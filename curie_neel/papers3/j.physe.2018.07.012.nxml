                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>â</mml:mo>
                                          <mml:mn>3</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:msup>
