                                    <mml:mi mathvariant="italic">GaAs</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                              <mml:mrow>
                                 <mml:mo>(</mml:mo>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:mi>T</mml:mi>
                                 </mml:mrow>
                                 <mml:mo>)</mml:mo>
                              </mml:mrow>
                              <mml:mo>=</mml:mo>
