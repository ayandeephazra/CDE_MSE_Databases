                                             <mml:mi>Î©</mml:mi>
                                             <mml:mi>s</mml:mi>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mi>Î</mml:mi>
                                          <mml:msubsup>
                                             <mml:mi>Î©</mml:mi>
                                             <mml:mi>s</mml:mi>
