                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mi>d</mml:mi>
                                             </mml:mtd>
                                          </mml:mtr>
                                          <mml:mtr columnalign="left">
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mi>d</mml:mi>
                                             </mml:mtd>
                                          </mml:mtr>
                                          <mml:mtr columnalign="left">
                                             <mml:mtd columnalign="left">
                                                <mml:mi>d</mml:mi>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mi>d</mml:mi>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                          </mml:mtr>
                                       </mml:mtable>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>;</mml:mo>
                                    <mml:mspace width="0.25em"/>
                                    <mml:msub>
                                       <mml:mtext>TO</mml:mtext>
