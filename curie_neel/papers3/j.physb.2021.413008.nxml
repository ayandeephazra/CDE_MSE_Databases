                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mi>Ï</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>L</mml:mi>
                                             <mml:mi>O</mml:mi>
