                                          <mml:mn>2</mml:mn>
                                       </mml:msqrt>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mtable columnalign="left">
                                          <mml:mtr columnalign="left">
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mrow>
                                                   <mml:mo>â</mml:mo>
                                                   <mml:mi>d</mml:mi>
                                                </mml:mrow>
                                             </mml:mtd>
                                          </mml:mtr>
                                          <mml:mtr columnalign="left">
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mn>0</mml:mn>
                                             </mml:mtd>
                                             <mml:mtd columnalign="left">
                                                <mml:mi>d</mml:mi>
                                             </mml:mtd>
