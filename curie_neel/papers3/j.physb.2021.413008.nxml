                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">)</mml:mo>
                                    </mml:mrow>
                                    <mml:msub>
                                       <mml:mi>Îµ</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>X</mml:mi>
                                          <mml:mi>X</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd4">
                              <ce:label>(4)</ce:label>
                              <mml:math altimg="si12.svg" alttext="Equation 4.">
                                 <mml:mrow>
                                    <mml:mi>Î</mml:mi>
                                    <mml:msub>
                                       <mml:mtext>Î©</mml:mtext>
                                       <mml:mtext>d</mml:mtext>
                                    </mml:msub>
                                    <mml:mo linebreak="goodbreak">â</mml:mo>
                                    <mml:mfrac>
                                       <mml:msub>
                                          <mml:mi>Ï</mml:mi>
                                          <mml:mn>0</mml:mn>
                                       </mml:msub>
                                       <mml:mn>2</mml:mn>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mi>p</mml:mi>
                                             <mml:msubsup>
                                                <mml:mi>Ï</mml:mi>
                                                <mml:mn>0</mml:mn>
                                                <mml:mn>2</mml:mn>
                                             </mml:msubsup>
                                          </mml:mfrac>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
