                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>b</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mtext>he</mml:mtext>
