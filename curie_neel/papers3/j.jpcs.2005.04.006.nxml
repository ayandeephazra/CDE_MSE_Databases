                                       </mml:msub>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mi>n</mml:mi>
                                       <mml:mo stretchy="false">)</mml:mo>
                                       <mml:msub>
                                          <mml:mi>r</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>he</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">[</mml:mo>
                                          <mml:mrow>
                                             <mml:msubsup>
                                                <mml:mi>b</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>he</mml:mtext>
