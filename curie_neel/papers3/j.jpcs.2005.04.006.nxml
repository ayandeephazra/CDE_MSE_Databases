                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mi>e</mml:mi>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>γ</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mo>+</mml:mo>
                                                      <mml:mo>−</mml:mo>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo stretchy="false">(</mml:mo>
                                                <mml:mn>1</mml:mn>
                                                <mml:mo>−</mml:mo>
                                                <mml:mi>r</mml:mi>
                                                <mml:mo>/</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>R</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mo>+</mml:mo>
                                                      <mml:mo>−</mml:mo>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo stretchy="false">)</mml:mo>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:mo>−</mml:mo>
                                          <mml:mn>2</mml:mn>
                                          <mml:msup>
                                             <mml:mi>e</mml:mi>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>γ</mml:mi>
