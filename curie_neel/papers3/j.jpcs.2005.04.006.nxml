                                          <mml:mtext>Ga</mml:mtext>
                                          <mml:mo>–</mml:mo>
                                          <mml:mtext>As</mml:mtext>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>SR</mml:mtext>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo stretchy="false">(</mml:mo>
                                    <mml:mi>r</mml:mi>
                                    <mml:mo stretchy="false">)</mml:mo>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>D</mml:mi>
