                                    <mml:mi>a</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>1</mml:mn>
                                    <mml:mo>−</mml:mo>
                                    <mml:mi>x</mml:mi>
                                 </mml:mrow>
