                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>C</mml:mi>
                                    <mml:mrow>
                                       <mml:mi mathvariant="normal">p</mml:mi>
                                       <mml:mi mathvariant="normal">o</mml:mi>
