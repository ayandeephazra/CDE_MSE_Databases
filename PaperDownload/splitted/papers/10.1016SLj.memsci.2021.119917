                  </ce:def-description>
                  <ce:def-term id="d0125">α</ce:def-term>
                  <ce:def-description id="defdis0125">
                     <ce:para id="p0150" view="all">Permselectivity</ce:para>
                  </ce:def-description>
                  <ce:def-term id="d0130">
                     <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>t</ce:italic>
                     </ce:inf>
                  </ce:def-term>
