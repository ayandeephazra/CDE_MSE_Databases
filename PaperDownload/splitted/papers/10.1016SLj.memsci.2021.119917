                        <ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si44.svg" alttext="Equation 8.">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>m</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>L</mml:mi>
                                             <mml:mi>J</mml:mi>
                                             <mml:mtext> </mml:mtext>
                                             <mml:mi>R</mml:mi>
