                              <mml:mi>t</mml:mi>
                              <mml:mi>M</mml:mi>
                              <mml:mi>s</mml:mi>
                           </mml:msubsup>
                        </mml:mrow>
                     </mml:math>
                  </ce:def-term>
                  <ce:def-description id="defdis0110">
                     <ce:para id="p0135" view="all">Transport number of counter-ions in solution phase</ce:para>
                  </ce:def-description>
                  <ce:def-term id="d0115">
                     <mml:math altimg="si11.svg">
                        <mml:mrow>
                           <mml:msubsup>
                              <mml:mi>t</mml:mi>
                              <mml:mi>X</mml:mi>
                              <mml:mi>s</mml:mi>
                           </mml:msubsup>
                        </mml:mrow>
                     </mml:math>
                  </ce:def-term>
