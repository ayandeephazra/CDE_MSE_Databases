                                          <mml:mi>m</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
                                             <mml:mi>w</mml:mi>
                                             <mml:mi>i</mml:mi>
                                             <mml:mi>r</mml:mi>
                                             <mml:mi>e</mml:mi>
                                             <mml:mtext> </mml:mtext>
                                             <mml:mi>R</mml:mi>
                                             <mml:mi>E</mml:mi>
                                             <mml:mo stretchy="true">)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:msubsup>
                                       <mml:mi>E</mml:mi>
                                       <mml:mi>x</mml:mi>
                                       <mml:mo>′</mml:mo>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak">−</mml:mo>
                                    <mml:msubsup>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
