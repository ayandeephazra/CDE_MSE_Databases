                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:mi>x</mml:mi>
                                    </mml:msub>
                                    <mml:mo linebreak="goodbreak">−</mml:mo>
                                    <mml:msub>
                                       <mml:mi>E</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>s</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
