                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>H</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mn>0.5</mml:mn>
                                             <mml:mi>M</mml:mi>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:msubsup>
                                          <mml:mi>a</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>C</mml:mi>
                                             <mml:mi>l</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>L</mml:mi>
                                             <mml:mo>,</mml:mo>
                                             <mml:mn>0.1</mml:mn>
                                             <mml:mi>M</mml:mi>
                                          </mml:mrow>
                                       </mml:msubsup>
                                    </mml:mfrac>
                                    <mml:mtext> </mml:mtext>
