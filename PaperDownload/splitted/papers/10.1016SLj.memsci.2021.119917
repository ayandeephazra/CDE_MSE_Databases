                  </ce:def-description>
               </ce:def-list>
               <ce:def-list id="dlist0015">
                  <ce:section-title id="sectitle0035">Greek letters</ce:section-title>
                  <ce:def-term id="d0120">
                     <mml:math altimg="si12.svg">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mi>Φ</mml:mi>
                              <mml:mi>v</mml:mi>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math>
                  </ce:def-term>
                  <ce:def-description id="defdis0120">
