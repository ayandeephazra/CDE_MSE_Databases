                  <ce:link locator="fx1"/>
               </ce:figure>
            </ce:abstract>
            <ce:keywords class="keyword">
               <ce:section-title>Keywords</ce:section-title>
               <ce:keyword>
                  <ce:text>Layered cobaltite oxide</ce:text>
               </ce:keyword>
