                  <ce:bib-reference id="b0040">
                     <ce:label>[8]</ce:label>
                     <sb:reference id="h0040">
                        <sb:contribution langtype="en">
                           <sb:authors>
                              <sb:author>
                                 <ce:given-name>C.</ce:given-name>
