                                                <mml:mi>o</mml:mi>
                                                <mml:mi>n</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                 </mml:mrow>
                                 <mml:mo>)</mml:mo>
                              </mml:mrow>
                           </mml:mrow>
                        </mml:math>, where Δ<ce:italic>c</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ion</ce:italic>
                        </ce:inf> (mol m<ce:sup loc="post">−3</ce:sup>) is the constant concentration difference across the oxide thickness <ce:italic>x</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf>. This flux is proportional to the rate of scale thickening <mml:math altimg="si4.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:mfrac>
                                 <mml:mrow>
                                    <mml:mtext>d</mml:mtext>
                                    <mml:msub>
                                       <mml:mi>x</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mtext>d</mml:mtext>
                                    <mml:mi>t</mml:mi>
                                 </mml:mrow>
                              </mml:mfrac>
                           </mml:mrow>
                        </mml:math> leading to:<ce:display>
                           <ce:formula id="fd2">
                              <ce:label>(2)</ce:label>
                              <mml:math altimg="si5.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>=</mml:mo>
                                    <mml:mi>C</mml:mi>
                                    <mml:mspace width="0.25em"/>
                                    <mml:mi>D</mml:mi>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:msub>
                                             <mml:mi>c</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>n</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>C</ce:italic> is a proportionality constant <ce:cross-ref refid="bib39" id="crosref0145">[39]</ce:cross-ref>. The integration yields the parabolic relationship Equation <ce:cross-ref refid="fd1" id="crosref0150">(1)</ce:cross-ref>, where <ce:italic>k</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>p</ce:italic>
                        </ce:inf> = <ce:italic>C</ce:italic> 
                        <ce:italic>D</ce:italic>Δ<ce:italic>c</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ion</ce:italic>
                        </ce:inf>, with <ce:italic>x</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf> = 0 at <ce:italic>t</ce:italic> = 0. Thus, the parabolic rate constant <ce:italic>k</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>p</ce:italic>
                        </ce:inf> is directly proportional to the diffusivity of the rate-controlling ionic species.</ce:para>
                     <ce:para id="p0125" view="all">Substituting the diffusivity by the parabolic rate constant in Equation <ce:cross-ref refid="fd2" id="crosref0155">(2)</ce:cross-ref> leads to the basic model for the corrosion algorithm:<ce:display>
                           <ce:formula id="fd3">
                              <ce:label>(3)</ce:label>
                              <mml:math altimg="si6.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>k</mml:mi>
                                             <mml:mi>p</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>.</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0130" view="all">Especially in the beginning of the corrosion, film thicknesses may be low enough to allow linear oxidation kinetics <ce:cross-ref refid="bib39" id="crosref0160">[39]</ce:cross-ref>. When the film is a continuous layer and thick enough, a transition to parabolic kinetics can be observed. This is taken into account by dividing the oxide growth process into a fast initial regime, and a subsequent slower parabolic regime. The oxidation in the initial regime is described by a linear growth law:<ce:display>
                           <ce:formula id="fd4">
                              <ce:label>(4)</ce:label>
                              <mml:math altimg="si7.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>k</mml:mi>
                                       <mml:mi>l</mml:mi>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>k</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>l</ce:italic>
                        </ce:inf> (m s<ce:sup loc="post">−1</ce:sup>) is the linear rate constant.</ce:para>
                     <ce:para id="p0135" view="all">The consumption of Cr<ce:sup loc="post">3+</ce:sup>-ions for the formation of the oxide (Cr<ce:inf loc="post">2</ce:inf>O<ce:inf loc="post">3</ce:inf>) results in a shrinkage of the alloy volume. With the knowledge of the oxide film thickness, the corresponding metal-retraction <ce:italic>r</ce:italic> (m) can be calculated. The procedure suggested in Ref. <ce:cross-ref refid="bib41" id="crosref0165">[41]</ce:cross-ref> is followed. By applying a simple mass balance, the flux <ce:italic>j</ce:italic>
                        <ce:sup loc="post">
                           <ce:italic>Cr</ce:italic>
                        </ce:sup> (mol m<ce:sup loc="post">−1</ce:sup> s<ce:sup loc="post">−1</ce:sup>) of the least noble element (here chromium) resulting from the growth of the oxide <mml:math altimg="si8.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:mo>Δ</mml:mo>
                              <mml:msubsup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo>→</mml:mo>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo>+</mml:mo>
                                    <mml:mo>Δ</mml:mo>
                                    <mml:mi>t</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> during a time step Δ<ce:italic>t</ce:italic> is defined by:<ce:display>
                           <ce:formula id="fd5">
                              <ce:label>(5)</ce:label>
                              <mml:math altimg="si9.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mi>j</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>C</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>z</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>x</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>ρ</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>x</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>M</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>x</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:msubsup>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>t</mml:mi>
                                                <mml:mo>→</mml:mo>
                                                <mml:mi>t</mml:mi>
                                                <mml:mo>+</mml:mo>
                                                <mml:mo>Δ</mml:mo>
                                                <mml:mi>t</mml:mi>
                                             </mml:mrow>
                                          </mml:msubsup>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>Δ</mml:mtext>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>z</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ox</ce:italic>
                        </ce:inf> is the number of cations per formula oxide, <ce:italic>ρ</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ox</ce:italic>
                        </ce:inf> (kg m<ce:sup loc="post">−3</ce:sup>) is the oxide density and <ce:italic>M</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ox</ce:italic>
                        </ce:inf> (kg mol<ce:sup loc="post">−1</ce:sup>) is the molar mass of the oxide.</ce:para>
                     <ce:para id="p0140" view="all">From the flux of chromium the metal-retraction Δ<ce:italic>r</ce:italic>
                        <ce:sup loc="post">
                           <ce:italic>t</ce:italic>→<ce:italic>t</ce:italic>+Δ<ce:italic>t</ce:italic>
                        </ce:sup> during one time step Δ<ce:italic>t</ce:italic> can be calculated:<ce:display>
                           <ce:formula id="fd6">
                              <ce:label>(6)</ce:label>
                              <mml:math altimg="si10.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mo>Δ</mml:mo>
                                    <mml:msup>
                                       <mml:mi>r</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>t</mml:mi>
                                          <mml:mo>→</mml:mo>
                                          <mml:mi>t</mml:mi>
                                          <mml:mo>+</mml:mo>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>=</mml:mo>
                                    <mml:mo>−</mml:mo>
                                    <mml:msub>
                                       <mml:mi>V</mml:mi>
                                       <mml:mi>M</mml:mi>
                                    </mml:msub>
                                    <mml:msup>
                                       <mml:mi>j</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>C</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>Δ</mml:mo>
                                    <mml:mi>t</mml:mi>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>V</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>M</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">3</ce:sup> mol<ce:sup loc="post">−1</ce:sup>) is the partial molar volume of the out-diffusing Cr-ions in the substrate.</ce:para>
                     <ce:para id="p0145" view="all">Thus, the weight gain relates to the binding of oxygen, so with a weight gain of Δ<ce:italic>W</ce:italic> (kg) due to the corrosion on the surface with area <ce:italic>A</ce:italic> (m<ce:sup loc="post">2</ce:sup>) the scale thickness can be deduced as<ce:display>
                           <ce:formula id="fd7">
                              <ce:label>(7)</ce:label>
                              <mml:math altimg="si11.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>x</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:mi>W</mml:mi>
                                          <mml:msub>
                                             <mml:mi>M</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>x</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>A</mml:mi>
                                          <mml:msub>
                                             <mml:mi>M</mml:mi>
                                             <mml:mi>O</mml:mi>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>z</mml:mi>
                                             <mml:mi>O</mml:mi>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>ρ</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>x</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>z</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>O</ce:italic>
                        </ce:inf> is the number of oxygen atoms per formula oxide, and <ce:italic>M</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>O</ce:italic>
                        </ce:inf> (kg mol<ce:sup loc="post">−1</ce:sup>) is the molar mass of oxygen.</ce:para>
                     <ce:para id="p0150" view="all">In the weight gain measurements of the porous metallic supports the initial surface area is estimated by BET and it is thus possible to calculate the average scale thickness according to Equation <ce:cross-ref refid="fd7" id="crosref0170">(7)</ce:cross-ref> and deduce the rate constants <ce:italic>k</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>l</ce:italic>
                        </ce:inf> and <ce:italic>k</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>p</ce:italic>
                        </ce:inf>.</ce:para>
                  </ce:section>
                  <ce:section id="sec3.2" view="all">
                     <ce:label>3.2</ce:label>
                     <ce:section-title id="sectitle0050">Computational geometry</ce:section-title>
                     <ce:para id="p0155" view="all">The sample measured with X-ray CT had not been exposed to any operation, and thus the sample was not corroded in its initial state. The measurement data was processed by an in-house algorithm to obtain the 3-D geometry of the metallic support. The resulting 3-D computational grid represents the pores of the real microstructure as a voxel mesh. Further details about the reconstruction are described in a previous study <ce:cross-ref refid="bib15" id="crosref0175">[15]</ce:cross-ref> and will thus not be presented here.</ce:para>
                     <ce:para id="p0160" view="all">The model equations are implemented in the open-source computational toolbox <ce:italic>OpenFOAM</ce:italic>. <ce:italic>OpenFOAM</ce:italic> is written in <ce:italic>C++</ce:italic>, an object-orientated language, that facilitates the development and inclusion of user-defined, specialised models in the existing software framework. The numerical techniques in <ce:italic>OpenFOAM</ce:italic> are based on the finite volume method, that subdivides the computational geometry into volume cells. Two kind of fields have to be distinguished: Firstly, volume fields, where the quantities (e.g. pressure, temperature, velocity, concentration, …) are stored at cell centres. Secondly, surface fields like e.g. mass fluxes are stored at face centres.</ce:para>
                     <ce:para id="p0165" view="all">To model the corrosion of the microstructure with a high accuracy, a fine mesh at the solid–fluid interface would be required, and re-meshing for each increment of the corrosive scale would be necessary. This will eventually make the computation of the entire 3-D microstructure impossible with current computational resources, as it would invoke an immense system of computational cells, and the solution procedure would be slowed heavily by re-meshing. Therefore, simplifications to both the corrosion model and the gas diffusion model have to be made.</ce:para>
                     <ce:para id="p0170" view="all">In the corrosion model the increment of the scale thickness and metal-retraction obtained during a time step is thus not resulting in re-meshing, but the thickness is rather stored as a parameter at each surface. The evolution of this parameter according to the growth model is described in Section <ce:cross-ref refid="sec3.3" id="crosref0180">3.3</ce:cross-ref>.</ce:para>
                     <ce:para id="p0175" view="all">In the gas diffusion model the influence of the corrosive scale and metal-retraction is accounted for by assuming that the cells, in which the corrosion is occurring, are a porous matter. This will limit the average diffusion rate through the cells along the corroding boundary of the metallic support. In <ce:italic>OpenFOAM</ce:italic> the porosity for each corroding cell is stored as a volume field.</ce:para>
                     <ce:para id="p0180" view="all">Due to the rather inhomogeneous microstructure, the computations were carried out on five independent sample positions in the same metallic support reconstruction to obtain a good statistical representation, where each geometry has a volume of 200 μm × 200 μm × 350 μm and the individual results were averaged. The lateral boundaries, parallel to the main diffusion path, are partly periodic and symmetry as described in Ref. <ce:cross-ref refid="bib15" id="crosref0185">[15]</ce:cross-ref>.</ce:para>
                  </ce:section>
                  <ce:section id="sec3.3" view="all">
                     <ce:label>3.3</ce:label>
                     <ce:section-title id="sectitle0055">Corrosion model</ce:section-title>
                     <ce:para id="p0185" view="all">In the corrosion model the oxidation process is described by employing first the linear and then the parabolic rate law. It is assumed that only the least noble element contributes to form the oxide layer, which is chromium in the case of this metallic support. Furthermore, it is assumed that the growing oxide film does not crack or spall. This is not fully consistent with reality, as compressive stresses might induce spallation with increasing scale thickness <ce:cross-ref refid="bib45" id="crosref0190">[45]</ce:cross-ref>. This phenomena is not fully understood and modelling of this is thus a separate study, and beyond the scope of this work.</ce:para>
                     <ce:para id="p0190" view="all">A principle sketch of the mechanism of the implemented corrosion model can be seen in <ce:cross-ref refid="fig1" id="crosref0195">Fig. 1</ce:cross-ref>
                        <ce:float-anchor refid="fig1"/>. In the initial step of the computation each corroding face retrieves its Cr-ions from one boundary face, which in turn retracts and the oxide fills the cell volume (see <ce:cross-ref refid="fig1" id="crosref0200">Fig. 1</ce:cross-ref>(b)). As the corrosion continues, the first cell layer will eventually be filled up with oxide scale (see <ce:cross-ref refid="fig1" id="crosref0205">Fig. 1</ce:cross-ref>(c)).</ce:para>
                     <ce:para id="p0195" view="all">After one cell is completely filled, neighbouring cells are determined and the connecting faces become the new surfaces, where the corrosion will progress from. The oxide layer thickness is determined as the distance between the centre point of the new corroding face and the corresponding source surface by vector analysis, taking into account the metal-retraction.</ce:para>
                     <ce:para id="p0200" view="all">Simultaneously, the number of corroding faces, which obtain their Cr-ions from one source face, is determined. In the case shown in <ce:cross-ref refid="fig1" id="crosref0210">Fig. 1</ce:cross-ref>(c), two corroding faces are coupled with one source face, after the first cell is filled. Thus, in this case the corrosion is limited by the diffusion through the source surface, and the growth rate of the corrosive scale on the two new corroding faces is halved (in order to conserve the mass of the Cr-ions). A correction factor <ce:italic>n</ce:italic> is thus introduced, which in the above mentioned example would equal two.</ce:para>
                     <ce:para id="p0205" view="all">The complementary case is a cell, which receives ions from two or more corroding surfaces. Thus, this cell is filled twice as fast (in case of two corroding surfaces) compared to a cell corroding only from one face.</ce:para>
                     <ce:para id="p0210" view="all">The stair-step representation of the surface, which is inherent from the mesh generation, leads to an overestimation of the initial surface area and to a high amount of cells that corrode from more than one side. In order to correct this unrealistic, faster growth the grid correction factor <ce:italic>c</ce:italic> is introduced. In a simple 2-D case with two corroding faces per cell <ce:italic>c</ce:italic> would equal <mml:math altimg="si12.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:mrow>
                                 <mml:mo>(</mml:mo>
                                 <mml:mrow>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mrow>
                                          <mml:msqrt>
                                             <mml:mn>8</mml:mn>
                                          </mml:msqrt>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>+</mml:mo>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mn>2</mml:mn>
                                    </mml:mfrac>
                                 </mml:mrow>
                                 <mml:mo>)</mml:mo>
                              </mml:mrow>
                           </mml:mrow>
                        </mml:math>, but can for the general case be written as<ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si13.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>c</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mi>G</mml:mi>
                                             <mml:mrow>
                                                <mml:mn>4</mml:mn>
                                                <mml:msubsup>
                                                   <mml:mi>F</mml:mi>
                                                   <mml:mi>c</mml:mi>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msubsup>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                    </mml:msqrt>
                                    <mml:mo>+</mml:mo>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mn>2</mml:mn>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>F</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>c</ce:italic>
                        </ce:inf> is the number of corroding faces per computational cell and <ce:italic>G</ce:italic> is a grid parameter. The derivation of this correction factor can be found in <ce:cross-ref refid="appsec1" id="crosref0215">Appendix A</ce:cross-ref>.</ce:para>
                     <ce:para id="p0215" view="all">The oxidation in the fast initial regime is described by a linear growth law in Equation <ce:cross-ref refid="fd4" id="crosref0220">(4)</ce:cross-ref>, which can be discretized in time by integration, yielding:<ce:display>
                           <ce:formula id="fd9a">
                              <ce:label>(9a)</ce:label>
                              <mml:math altimg="si14.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msubsup>
                                       <mml:mi>x</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>w</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>=</mml:mo>
                                    <mml:msubsup>
                                       <mml:mi>x</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>l</mml:mi>
                                          <mml:mi>d</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>+</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>k</mml:mi>
                                             <mml:mi>l</mml:mi>
                                          </mml:msub>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                       <mml:mi>n</mml:mi>
                                    </mml:mfrac>
                                    <mml:mi>c</mml:mi>
                                    <mml:mspace width="1em"/>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo>≤</mml:mo>
                                    <mml:msub>
                                       <mml:mi>t</mml:mi>
                                       <mml:mi>p</mml:mi>
                                    </mml:msub>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <mml:math altimg="si15.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>w</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> is the thickness of the oxide at the new time step <ce:italic>t</ce:italic> + Δ<ce:italic>t</ce:italic>, and <mml:math altimg="si16.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>x</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>c</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>r</mml:mi>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>o</mml:mi>
                                    <mml:mi>l</mml:mi>
                                    <mml:mi>d</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> is the current oxide thickness, <ce:italic>n</ce:italic> is the correction factor accounting for different constrictions to the diffusion and scale growth, and <ce:italic>c</ce:italic> is the grid correction factor correcting the overestimation of the surface due to the stair-step surface representation.</ce:para>
                     <ce:para id="p0220" view="all">In <ce:italic>OpenFOAM</ce:italic> a forward Euler time integration scheme of Equation <ce:cross-ref refid="fd3" id="crosref0225">(3)</ce:cross-ref> is implemented for the second, slower parabolic oxidation regime:<ce:display>
                           <ce:formula id="fd9b">
                              <ce:label>(9b)</ce:label>
                              <mml:math altimg="si17.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msubsup>
                                       <mml:mi>x</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>w</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>=</mml:mo>
                                    <mml:msqrt>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:msubsup>
                                                         <mml:mi>x</mml:mi>
                                                         <mml:mrow>
                                                            <mml:mi>c</mml:mi>
                                                            <mml:mi>o</mml:mi>
                                                            <mml:mi>r</mml:mi>
                                                            <mml:mi>r</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>o</mml:mi>
                                                            <mml:mi>l</mml:mi>
                                                            <mml:mi>d</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msubsup>
                                                   </mml:mrow>
                                                   <mml:mo>)</mml:mo>
                                                </mml:mrow>
                                             </mml:mrow>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                          <mml:mo>+</mml:mo>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>k</mml:mi>
                                                   <mml:mi>p</mml:mi>
                                                </mml:msub>
                                                <mml:mtext>Δ</mml:mtext>
                                                <mml:mi>t</mml:mi>
                                             </mml:mrow>
                                             <mml:mi>n</mml:mi>
                                          </mml:mfrac>
                                          <mml:mi>c</mml:mi>
                                       </mml:mrow>
                                    </mml:msqrt>
                                    <mml:mspace width="1em"/>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo>&gt;</mml:mo>
                                    <mml:msub>
                                       <mml:mi>t</mml:mi>
                                       <mml:mi>p</mml:mi>
                                    </mml:msub>
                                    <mml:mtext>.</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0225" view="all">Once the oxide growth is determined, the corresponding retraction of the metal-interface during this time-step can be calculated with Equations <ce:cross-refs refid="fd5 fdA_1 fdA_2 fdA_3 fdA_4 fdA_5 fd6" id="crosrefs0040">(5) and (6)</ce:cross-refs>. Thereafter, the change of the pore volume Δ<ce:italic>V</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>diff</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">3</ce:sup>) can be estimated by multiplying the base area <ce:italic>A</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>c</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">2</ce:sup>) with the difference between the oxide thickness and the metal-retraction:<ce:display>
                           <ce:formula id="fd10">
                              <ce:label>(10)</ce:label>
                              <mml:math altimg="si18.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mo>Δ</mml:mo>
                                    <mml:msub>
                                       <mml:mi>V</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>A</mml:mi>
                                       <mml:mi>c</mml:mi>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>x</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mo>−</mml:mo>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mtext>.</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0230" view="all">With the knowledge of the changed pore volume Δ<ce:italic>V</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>diff</ce:italic>
                        </ce:inf> from Equation <ce:cross-ref refid="fd10" id="crosref0230">(10)</ce:cross-ref>, the local porosity <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf> (−) of each computational cell can be calculated:<ce:display>
                           <ce:formula id="fd11">
                              <ce:label>(11)</ce:label>
                              <mml:math altimg="si19.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>ε</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>r</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:mn>1</mml:mn>
                                    <mml:mo>−</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>d</mml:mi>
                                                <mml:mi>i</mml:mi>
                                                <mml:mi>f</mml:mi>
                                                <mml:mi>f</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>l</mml:mi>
                                                <mml:mi>l</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where in <ce:italic>V</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>cell</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">3</ce:sup>) is the volume of the computational cell. The porosity is 1 in regions, where no oxide is prevailing, and 0 in regions that are fully corroded.</ce:para>
                     <ce:para id="p0235" view="all">The local porosity <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf> that is determined from the corrosion model and implemented in the conservation equations (described in Section <ce:cross-ref refid="sec3.4" id="crosref0235">3.4</ce:cross-ref>) will account for the changed open pore space.</ce:para>
                     <ce:para id="p0240" view="all">A summary of the main steps of the corrosion algorithm during one time step is as follows:<ce:list id="olist0010">
                           <ce:list-item id="o0010">
                              <ce:label>1.</ce:label>
                              <ce:para id="p0245" view="all">solve either the linear (Equation <ce:cross-ref refid="fd9a" id="crosref0240">(9a)</ce:cross-ref>) or parabolic rate law (Equation <ce:cross-ref refid="fd9b" id="crosref0245">(9b)</ce:cross-ref>);</ce:para>
                           </ce:list-item>
                           <ce:list-item id="o0015">
                              <ce:label>2.</ce:label>
                              <ce:para id="p0250" view="all">calculate the metal-retraction (Equation <ce:cross-ref refid="fd6" id="crosref0250">(6)</ce:cross-ref>);</ce:para>
                           </ce:list-item>
                           <ce:list-item id="o0020">
                              <ce:label>3.</ce:label>
                              <ce:para id="p0255" view="all">determine the local porosity <ce:italic>ε</ce:italic>
                                 <ce:inf loc="post">
                                    <ce:italic>corr</ce:italic>
                                 </ce:inf> (Equation <ce:cross-ref refid="fd11" id="crosref0255">(11)</ce:cross-ref>);</ce:para>
                           </ce:list-item>
                           <ce:list-item id="o0025">
                              <ce:label>4.</ce:label>
                              <ce:para id="p0260" view="all">if one cell is entirely filled with oxide (<ce:italic>ε</ce:italic>
                                 <ce:inf loc="post">
                                    <ce:italic>corr</ce:italic>
                                 </ce:inf> ≥ 1):<ce:list id="olist0015">
                                    <ce:list-item id="o0030">
                                       <ce:label>(a)</ce:label>
                                       <ce:para id="p0265" view="all">determine the neighbouring cells → joining surfaces are the new corroding surfaces;</ce:para>
                                    </ce:list-item>
                                    <ce:list-item id="o0035">
                                       <ce:label>(b)</ce:label>
                                       <ce:para id="p0270" view="all">calculation of the shortest distance to the source surface, taking the retraction into account;</ce:para>
                                    </ce:list-item>
                                    <ce:list-item id="o0040">
                                       <ce:label>(c)</ce:label>
                                       <ce:para id="p0275" view="all">check how many corroding surfaces are coupled with one source surface → limitation <ce:italic>n.</ce:italic>
                                       </ce:para>
                                    </ce:list-item>
                                 </ce:list>
                              </ce:para>
                           </ce:list-item>
                           <ce:list-item id="o0045">
                              <ce:label>5.</ce:label>
                              <ce:para id="p0280" view="all">if the diffusion through the corroded microstructure should be assessed, Equations <ce:cross-ref refid="fd12" id="crosref0260">(12)–(16)</ce:cross-ref> are solved.</ce:para>
                           </ce:list-item>
                        </ce:list>
                     </ce:para>
                  </ce:section>
                  <ce:section id="sec3.4" view="all">
                     <ce:label>3.4</ce:label>
                     <ce:section-title id="sectitle0060">Effective gas transport parameters</ce:section-title>
                     <ce:para id="p0285" view="all">To evaluate the influence of the corrosion, an effective transport parameter for the corroding microstructure is determined. Here an effective gas diffusion coefficient <mml:math altimg="si20.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>D</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>f</mml:mi>
                                    <mml:mi>f</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> is calculated, which would provide the same flux through a pseudo-homogeneous material, as the average flux through the considered porous structure.</ce:para>
                     <ce:para id="p0290" view="all">The average pore diameter of the support is 18 μm yielding a Knudsen number of ∼ 0.05 at 850 °C, why Knudsen diffusion can be neglected <ce:cross-ref refid="bib46" id="crosref0265">[46]</ce:cross-ref>, and the Maxwell–Stefan diffusion equations simplify for a binary system to a Laplacian equation. The effective diffusion coefficient is thus determined by solving a Laplacian equation on the reconstructed microstructure, computing the concentration distribution. Subsequently, the average mass or molar fluxes of the transported gas species <ce:italic>ϕ</ce:italic> are determined. The computation is carried out on a SERVE as defined in the introduction. Furthermore, the diffusion equation solved, is enhanced to take an additional porosity field into account:<ce:display>
                           <ce:formula id="fd12">
                              <ce:label>(12)</ce:label>
                              <mml:math altimg="si21.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi mathvariant="bold">∇</mml:mi>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:msub>
                                             <mml:mi>c</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>g</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>s</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>ε</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mi mathvariant="bold">∇</mml:mi>
                                          <mml:msub>
                                             <mml:mi>w</mml:mi>
                                             <mml:mi>a</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mn>0</mml:mn>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>c</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>gas</ce:italic>
                        </ce:inf> (mol m<ce:sup loc="post">−3</ce:sup>) is the mixture molar density, <ce:italic>D</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ab</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">−2</ce:sup> s<ce:sup loc="post">−1</ce:sup>) is the binary diffusion coefficient of species <ce:italic>a</ce:italic> in species <ce:italic>b</ce:italic>, <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf> is the local porosity field and <ce:italic>w</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>a</ce:italic>
                        </ce:inf> is the molar fraction of species <ce:italic>a</ce:italic>. The binary diffusion coefficients <ce:italic>D</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>ab</ce:italic>
                        </ce:inf> for different temperatures can be calculated according to the Chapman-Enskog theory <ce:cross-ref refid="bib47" id="crosref0270">[47]</ce:cross-ref>. As a reminder: The local porosity field <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf> is 1 in regions, where no oxide is prevailing, and 0 in regions that are fully corroded.</ce:para>
                     <ce:para id="p0295" view="all">The average flux <ce:italic>ϕ</ce:italic> over the surface <ce:italic>S</ce:italic> is determined by solving the surface integral over the surface <ce:italic>S</ce:italic>
                        <ce:display>
                           <ce:formula id="fd13">
                              <ce:label>(13)</ce:label>
                              <mml:math altimg="si22.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mi>A</mml:mi>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:munder>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mi>S</mml:mi>
                                       </mml:munder>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:msub>
                                             <mml:mi>c</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>g</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>s</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>ε</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>r</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mi mathvariant="bold">∇</mml:mi>
                                          <mml:msub>
                                             <mml:mi>w</mml:mi>
                                             <mml:mi>a</mml:mi>
                                          </mml:msub>
                                          <mml:mi mathvariant="bold-italic">n</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:mi>S</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>n</ce:italic> is the normal vector of the surface and <ce:italic>A</ce:italic> (m<ce:sup loc="post">2</ce:sup>) is the area of the surface <ce:italic>S</ce:italic>.</ce:para>
                     <ce:para id="p0300" view="all">For a pseudo-homogeneous media with an effective diffusion coefficient, the flux over the surface <ce:italic>S</ce:italic> is constant, such that<ce:display>
                           <ce:formula id="fd14">
                              <ce:label>(14)</ce:label>
                              <mml:math altimg="si23.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mi>A</mml:mi>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:munder>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mi>S</mml:mi>
                                       </mml:munder>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:msub>
                                             <mml:mi>c</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>g</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>s</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:msubsup>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>f</mml:mi>
                                                <mml:mi>f</mml:mi>
                                             </mml:mrow>
                                          </mml:msubsup>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi mathvariant="bold">∇</mml:mi>
                                          <mml:msub>
                                             <mml:mi>w</mml:mi>
                                             <mml:mi>a</mml:mi>
                                          </mml:msub>
                                          <mml:mi mathvariant="bold-italic">n</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mtext>d</mml:mtext>
                                          <mml:mi>S</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mo>−</mml:mo>
                                    <mml:msub>
                                       <mml:mi>c</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>g</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>s</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:msubsup>
                                       <mml:mi>D</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>b</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mo>Δ</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>w</mml:mi>
                                                   <mml:mi>a</mml:mi>
                                                </mml:msub>
                                             </mml:mrow>
                                             <mml:mi>L</mml:mi>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where the difference in the mole fraction Δ<ce:italic>w</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>a</ce:italic>
                        </ce:inf> is calculated as the difference between the weighted mole fraction at the surfaces through which the diffusion passes.</ce:para>
                     <ce:para id="p0305" view="all">By merging Equation <ce:cross-ref refid="fd13" id="crosref0275">(13)</ce:cross-ref> and (14) the effective diffusion coefficient <mml:math altimg="si20.gif" overflow="scroll">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>D</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>a</mml:mi>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>f</mml:mi>
                                    <mml:mi>f</mml:mi>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> can be determined:<ce:display>
                           <ce:formula id="fd15">
                              <ce:label>(15)</ce:label>
                              <mml:math altimg="si24.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msubsup>
                                       <mml:mi>D</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>b</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:munder>
                                                <mml:mo>∫</mml:mo>
                                                <mml:mi>S</mml:mi>
                                             </mml:munder>
                                             <mml:mrow>
                                                <mml:mo>−</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>D</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>a</mml:mi>
                                                      <mml:mi>b</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mspace width="0.25em"/>
                                                <mml:msub>
                                                   <mml:mi>ε</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>c</mml:mi>
                                                      <mml:mi>o</mml:mi>
                                                      <mml:mi>r</mml:mi>
                                                      <mml:mi>r</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mspace width="0.25em"/>
                                                <mml:mi mathvariant="bold">∇</mml:mi>
                                                <mml:msub>
                                                   <mml:mi>w</mml:mi>
                                                   <mml:mi>a</mml:mi>
                                                </mml:msub>
                                                <mml:mi mathvariant="bold-italic">n</mml:mi>
                                                <mml:mspace width="0.25em"/>
                                                <mml:mtext>d</mml:mtext>
                                                <mml:mi>S</mml:mi>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mo>Δ</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>w</mml:mi>
                                                         <mml:mi>a</mml:mi>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:mi>L</mml:mi>
                                                </mml:mfrac>
                                             </mml:mrow>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                          <mml:mi>A</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>.</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0310" view="all">The ratio between the binary and the effective diffusion coefficient can be defined as scaling factor <ce:cross-ref refid="bib15" id="crosref0280">[15]</ce:cross-ref>.<ce:display>
                           <ce:formula id="fd16">
                              <ce:label>(16)</ce:label>
                              <mml:math altimg="si25.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>f</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msubsup>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>f</mml:mi>
                                                <mml:mi>f</mml:mi>
                                             </mml:mrow>
                                          </mml:msubsup>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mi>ε</mml:mi>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mi>τ</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>and depends on the actual porosity <ce:italic>ε</ce:italic> and the tortuosity <ce:italic>τ</ce:italic>, but also on features of the microstructure such as bottle necks and dead-end pores. Thus, the scaling factor <ce:italic>f</ce:italic> is a measure for the mass-diffusion through the metallic support and an important input parameter for cell- or stack-simulations, as only continuum models and thus pseudo-homogeneous materials can currently be considered in such simulations. Note: the porosity in Equation <ce:cross-ref refid="fd16" id="crosref0285">(16)</ce:cross-ref> must not be confused with the local porosity <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>corr</ce:italic>
                        </ce:inf>, but is defined as<ce:display>
                           <ce:formula id="fd17">
                              <ce:label>(17)</ce:label>
                              <mml:math altimg="si26.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>ε</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>ε</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>n</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>−</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:munderover>
                                             <mml:mo>∑</mml:mo>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                                <mml:mo>=</mml:mo>
                                                <mml:mn>1</mml:mn>
                                             </mml:mrow>
                                             <mml:mi>n</mml:mi>
                                          </mml:munderover>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>c</mml:mi>
                                                   <mml:mi>e</mml:mi>
                                                   <mml:mi>l</mml:mi>
                                                   <mml:mi>l</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mrow>
                                                <mml:mo>(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mn>1</mml:mn>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:msub>
                                                      <mml:mi>ε</mml:mi>
                                                      <mml:mrow>
                                                         <mml:mi>c</mml:mi>
                                                         <mml:mi>o</mml:mi>
                                                         <mml:mi>r</mml:mi>
                                                         <mml:mi>r</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                                <mml:mo>)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mi>V</mml:mi>
                                    </mml:mfrac>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>ε</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>init</ce:italic>
                        </ce:inf> is the initial (non-corroded) porosity, <ce:italic>V</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>cell</ce:italic>
                        </ce:inf> (m<ce:sup loc="post">3</ce:sup>) is the volume of one computational cell and <ce:italic>V</ce:italic> (m<ce:sup loc="post">3</ce:sup>) is the volume of the entire metallic support.</ce:para>
                  </ce:section>
                  <ce:section id="sec3.5" view="all">
                     <ce:label>3.5</ce:label>
                     <ce:section-title id="sectitle0065">Evaluation of equivalent electrical resistance due to gas diffusion</ce:section-title>
                     <ce:para id="p0315" view="all">To evaluate the impact of corrosion on the electro-chemical performance of the SOFC, the equivalent electrical resistance due to gas diffusion <ce:italic>R</ce:italic>
                        <ce:inf loc="post">
                           <ce:italic>diff</ce:italic>
                        </ce:inf> (Ω m<ce:sup loc="post">2</ce:sup>) is calculated. A model adapted by Ref. <ce:cross-ref refid="bib48" id="crosref0290">[48]</ce:cross-ref> is applied:<ce:display>
                           <ce:formula id="fd18">
                              <ce:label>(18)</ce:label>
                              <mml:math altimg="si27.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi>R</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>f</mml:mi>
                                          <mml:mi>f</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mi>R</mml:mi>
                                                      <mml:mi>T</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>F</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                             </mml:mrow>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
                                    <mml:mfrac>
                                       <mml:mi>L</mml:mi>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>D</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>b</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mi>f</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mn>1</mml:mn>
                                             <mml:mrow>
                                                <mml:msubsup>
                                                   <mml:mi>p</mml:mi>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mi>H</mml:mi>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>A</mml:mi>
                                                      <mml:mi>F</mml:mi>
                                                      <mml:mi>L</mml:mi>
                                                   </mml:mrow>
                                                </mml:msubsup>
                                             </mml:mrow>
                                          </mml:mfrac>
                                          <mml:mo>+</mml:mo>
                                          <mml:mfrac>
                                             <mml:mn>1</mml:mn>
                                             <mml:mrow>
                                                <mml:msubsup>
                                                   <mml:mi>p</mml:mi>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mi>H</mml:mi>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msub>
                                                      <mml:mi>O</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>A</mml:mi>
                                                      <mml:mi>F</mml:mi>
                                                      <mml:mi>L</mml:mi>
                                                   </mml:mrow>
                                                </mml:msubsup>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mrow>
                                                <mml:mn>1.0133</mml:mn>
                                                <mml:mo>×</mml:mo>
                                                <mml:msup>
                                                   <mml:mrow>
                                                      <mml:mn>10</mml:mn>
                                                   </mml:mrow>
                                                   <mml:mn>5</mml:mn>
                                                </mml:msup>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mtext>Pa</mml:mtext>
