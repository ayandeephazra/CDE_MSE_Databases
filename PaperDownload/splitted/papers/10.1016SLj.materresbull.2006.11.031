            <ce:figure id="fig6">
               <ce:label>Fig. 6</ce:label>
               <ce:caption>
                  <ce:simple-para view="all">(a) SEI micrograph and corresponding distribution of elements: (b) Ce, (c) Gd and (d) Ni obtained by EDS mapping.</ce:simple-para>
               </ce:caption>
               <ce:link locator="gr6"/>
            </ce:figure>
