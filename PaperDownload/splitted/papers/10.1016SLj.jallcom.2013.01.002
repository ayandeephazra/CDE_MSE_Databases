            <ce:bibliography id="bi0005" view="all">
               <ce:section-title>References</ce:section-title>
               <ce:bibliography-sec id="bs0005">
                  <ce:bib-reference id="b0005">
                     <ce:label>[1]</ce:label>
                     <sb:reference>
