         <head>
            <ce:dochead id="dhead0010">
               <ce:textfn>Short Communication</ce:textfn>
            </ce:dochead>
            <ce:title id="title0010">The effect of membrane on an alkaline water electrolyzer</ce:title>
            <ce:author-group id="augrp0010">
               <ce:author id="au1" author-id="S0360319917339484-63e7aef10c40d1e23184936a856f9cbe">
                  <ce:given-name>Zengcai</ce:given-name>
                  <ce:surname>Liu</ce:surname>
               </ce:author>
               <ce:author id="au2" author-id="S0360319917339484-07ae7518539b04c6a75f5304aed9f05b">
                  <ce:given-name>Syed Dawar</ce:given-name>
                  <ce:surname>Sajjad</ce:surname>
               </ce:author>
               <ce:author id="au3" author-id="S0360319917339484-0a1e3015f0245ac1703dc884dab9892a">
