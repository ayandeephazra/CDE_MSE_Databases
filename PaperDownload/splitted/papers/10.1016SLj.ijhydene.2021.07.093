                                    <mml:mtext>e</mml:mtext>
                                    <mml:mo>−</mml:mo>
                                 </mml:msup>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> and ions <mml:math altimg="si23.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mover accent="true">
                                    <mml:mi>μ</mml:mi>
                                    <mml:mo>˜</mml:mo>
                                 </mml:mover>
