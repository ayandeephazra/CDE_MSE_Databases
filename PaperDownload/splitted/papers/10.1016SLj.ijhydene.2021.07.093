                                                      <mml:mn>2</mml:mn>
                                                   </mml:msub>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mtext>ct</mml:mtext>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>F</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mo>,</mml:mo>
                                                <mml:mspace width="0.25em"/>
                                                <mml:msub>
                                                   <mml:mover accent="true">
                                                      <mml:mi>s</mml:mi>
                                                      <mml:mo>˙</mml:mo>
                                                   </mml:mover>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mtext>H</mml:mtext>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msub>
                                                      <mml:mtext>O</mml:mtext>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mtext>ct</mml:mtext>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mn>2</mml:mn>
                                                      <mml:mi>F</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mo>,</mml:mo>
                                                <mml:mspace width="0.25em"/>
                                                <mml:msub>
                                                   <mml:mover accent="true">
                                                      <mml:mi>s</mml:mi>
                                                      <mml:mo>˙</mml:mo>
                                                   </mml:mover>
                                                   <mml:msub>
                                                      <mml:mtext>O</mml:mtext>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:msub>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mtext>ct</mml:mtext>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mn>4</mml:mn>
                                                      <mml:mi>F</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mo>,</mml:mo>
                                                <mml:mspace width="0.25em"/>
                                                <mml:msub>
                                                   <mml:mover accent="true">
                                                      <mml:mi>s</mml:mi>
                                                      <mml:mo>˙</mml:mo>
                                                   </mml:mover>
                                                   <mml:msub>
                                                      <mml:mtext>N</mml:mtext>
                                                      <mml:mn>2</mml:mn>
                                                   </mml:msub>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mn>0</mml:mn>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0160" view="all">The dusty-gas model (DGM) [<ce:cross-ref refid="bib26" id="crosref0205">26</ce:cross-ref>,<ce:cross-ref refid="bib27" id="crosref0210">27</ce:cross-ref>] was used to evaluate the molar flux in the porous electrodes.<ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si34.svg" alttext="Equation 8.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>N</mml:mi>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:msub>
                                                   <mml:msubsup>
                                                      <mml:mi>D</mml:mi>
                                                      <mml:mrow>
                                                         <mml:mi>i</mml:mi>
                                                         <mml:mo>,</mml:mo>
                                                         <mml:mtext>K</mml:mtext>
                                                      </mml:mrow>
                                                      <mml:mtext>eff</mml:mtext>
                                                   </mml:msubsup>
                                                </mml:mfrac>
                                                <mml:mo linebreak="badbreak">+</mml:mo>
                                                <mml:munderover>
                                                   <mml:mrow>
                                                      <mml:mo>∑</mml:mo>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>j</mml:mi>
                                                      <mml:mo linebreak="badbreak">=</mml:mo>
                                                      <mml:mn>1</mml:mn>
                                                      <mml:mo>,</mml:mo>
                                                      <mml:mi>j</mml:mi>
                                                      <mml:mo linebreak="badbreak">≠</mml:mo>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mi>n</mml:mi>
                                                </mml:munderover>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mi>X</mml:mi>
                                                         <mml:mi>j</mml:mi>
                                                      </mml:msub>
                                                      <mml:msub>
                                                         <mml:mi>N</mml:mi>
                                                         <mml:mi>i</mml:mi>
                                                      </mml:msub>
                                                      <mml:mo linebreak="badbreak">−</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>X</mml:mi>
                                                         <mml:mi>i</mml:mi>
                                                      </mml:msub>
                                                      <mml:msub>
                                                         <mml:mi>N</mml:mi>
                                                         <mml:mi>j</mml:mi>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:msubsup>
                                                      <mml:mi>D</mml:mi>
                                                      <mml:mrow>
                                                         <mml:mi>i</mml:mi>
                                                         <mml:mi>j</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mtext>eff</mml:mtext>
                                                   </mml:msubsup>
                                                </mml:mfrac>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>P</mml:mi>
                                                      <mml:mi>t</mml:mi>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>R</mml:mi>
                                                      <mml:mi>T</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:msub>
                                                         <mml:mi>X</mml:mi>
                                                         <mml:mi>i</mml:mi>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>X</mml:mi>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>R</mml:mi>
                                                      <mml:mi>T</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mn>1</mml:mn>
                                                      <mml:mo linebreak="badbreak">+</mml:mo>
                                                      <mml:mfrac>
                                                         <mml:mrow>
                                                            <mml:mi>K</mml:mi>
                                                            <mml:msub>
                                                               <mml:mi>P</mml:mi>
                                                               <mml:mi>t</mml:mi>
                                                            </mml:msub>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>μ</mml:mi>
                                                            <mml:msubsup>
                                                               <mml:mi>D</mml:mi>
                                                               <mml:mrow>
                                                                  <mml:mi>i</mml:mi>
                                                                  <mml:mo>,</mml:mo>
                                                                  <mml:mtext>K</mml:mtext>
                                                               </mml:mrow>
                                                               <mml:mtext>eff</mml:mtext>
                                                            </mml:msubsup>
                                                         </mml:mrow>
                                                      </mml:mfrac>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:msub>
                                                         <mml:mi>P</mml:mi>
                                                         <mml:mi>t</mml:mi>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <mml:math altimg="si35.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>X</mml:mi>
                                 <mml:mi>i</mml:mi>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> is the molar fraction, <mml:math altimg="si36.svg">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>D</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mo>,</mml:mo>
                                    <mml:mtext>K</mml:mtext>
                                 </mml:mrow>
                                 <mml:mtext>eff</mml:mtext>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> and <mml:math altimg="si37.svg">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mi>D</mml:mi>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>j</mml:mi>
                                 </mml:mrow>
                                 <mml:mtext>eff</mml:mtext>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math> are the effective Knudsen and molecular diffusivities, respectively, <mml:math altimg="si38.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>P</mml:mi>
                                 <mml:mi>t</mml:mi>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> is the total pressure, and <mml:math altimg="si39.svg">
                           <mml:mrow>
                              <mml:mi>μ</mml:mi>
                           </mml:mrow>
                        </mml:math> is the mixture viscosity [<ce:cross-ref refid="bib28" id="crosref0215">28</ce:cross-ref>]. <mml:math altimg="si40.svg">
                           <mml:mrow>
                              <mml:mi>K</mml:mi>
                           </mml:mrow>
                        </mml:math> is the permeability evaluated as follows [<ce:cross-ref refid="bib25" id="crosref0220">25</ce:cross-ref>].<ce:display>
                           <ce:formula id="fd9">
                              <ce:label>(9)</ce:label>
                              <mml:math altimg="si41.svg" alttext="Equation 9.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:mi>K</mml:mi>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>V</mml:mi>
                                                      <mml:mtext>Pore</mml:mtext>
                                                   </mml:msub>
                                                   <mml:mrow>
                                                      <mml:mn>6</mml:mn>
                                                      <mml:msub>
                                                         <mml:mi>τ</mml:mi>
                                                         <mml:mtext>Pore</mml:mtext>
                                                      </mml:msub>
                                                      <mml:msubsup>
                                                         <mml:mrow>
                                                            <mml:mo stretchy="true">(</mml:mo>
                                                            <mml:mrow>
                                                               <mml:mi>S</mml:mi>
                                                               <mml:mo linebreak="badbreak">/</mml:mo>
                                                               <mml:mi>V</mml:mi>
                                                            </mml:mrow>
                                                            <mml:mo stretchy="true">)</mml:mo>
                                                         </mml:mrow>
                                                         <mml:mtext>Pore</mml:mtext>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msubsup>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <mml:math altimg="si42.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>V</mml:mi>
                                 <mml:mtext>Pore</mml:mtext>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math>, <mml:math altimg="si43.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>τ</mml:mi>
                                 <mml:mtext>Pore</mml:mtext>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math>, and <mml:math altimg="si44.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>S</mml:mi>
                                    <mml:mo linebreak="badbreak">/</mml:mo>
                                    <mml:mi>V</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mtext>Pore</mml:mtext>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> are the volume fraction, tortuosity factor, and surface-to-volume ratio of the pore phase, respectively. By solving Eq. <ce:cross-ref refid="fd6" id="crosref0225">(6)</ce:cross-ref> in the electrodes, inhomogeneous distribution of total pressure in the electrodes is reproduced, which is one of the important factors explaining the asymmetric behavior of the cell and will be discussed later.</ce:para>
                     <ce:para id="p0165" view="all">The effective conductivities and gas diffusivities were evaluated by modifying the bulk transport coefficients by using the volume fraction <mml:math altimg="si45.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>V</mml:mi>
                                 <mml:mi>i</mml:mi>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> and the tortuosity factor <mml:math altimg="si46.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>τ</mml:mi>
                                 <mml:mi>i</mml:mi>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> of the corresponding phases.<ce:display>
                           <ce:formula id="fd10">
                              <ce:label>(10)</ce:label>
                              <mml:math altimg="si47.svg" alttext="Equation 10.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:msubsup>
                                                   <mml:mi>Γ</mml:mi>
                                                   <mml:mi>i</mml:mi>
                                                   <mml:mtext>eff</mml:mtext>
                                                </mml:msubsup>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mfrac>
                                                   <mml:msub>
                                                      <mml:mi>V</mml:mi>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:msub>
                                                   <mml:msub>
                                                      <mml:mi>τ</mml:mi>
                                                      <mml:mi>i</mml:mi>
                                                   </mml:msub>
                                                </mml:mfrac>
                                                <mml:msub>
                                                   <mml:mi>Γ</mml:mi>
                                                   <mml:mi>i</mml:mi>
                                                </mml:msub>
                                                <mml:mspace width="0.25em"/>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mi>Γ</mml:mi>
                                                         <mml:mi>i</mml:mi>
                                                      </mml:msub>
                                                      <mml:mo linebreak="badbreak">=</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>σ</mml:mi>
                                                         <mml:msup>
                                                            <mml:mtext>e</mml:mtext>
                                                            <mml:mo linebreak="badbreak">−</mml:mo>
                                                         </mml:msup>
                                                      </mml:msub>
                                                      <mml:mo>,</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>σ</mml:mi>
                                                         <mml:msup>
                                                            <mml:mtext>O</mml:mtext>
                                                            <mml:mrow>
                                                               <mml:mn>2</mml:mn>
                                                               <mml:mo linebreak="badbreak">−</mml:mo>
                                                            </mml:mrow>
                                                         </mml:msup>
                                                      </mml:msub>
                                                      <mml:mo>,</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>D</mml:mi>
                                                         <mml:mrow>
                                                            <mml:mi>i</mml:mi>
                                                            <mml:mi>j</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                      <mml:mo>,</mml:mo>
                                                      <mml:msub>
                                                         <mml:mi>D</mml:mi>
                                                         <mml:mrow>
                                                            <mml:mi>i</mml:mi>
                                                            <mml:mo>,</mml:mo>
                                                            <mml:mtext>K</mml:mtext>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0170" view="all">The bulk molecular diffusivity [<ce:cross-ref refid="bib29" id="crosref0230">29</ce:cross-ref>] and Knudsen diffusivity [<ce:cross-ref refid="bib30" id="crosref0235">30</ce:cross-ref>] were evaluated as follows.<ce:display>
                           <ce:formula id="fd11">
                              <ce:label>(11)</ce:label>
                              <mml:math altimg="si48.svg" alttext="Equation 11.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>D</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>i</mml:mi>
                                                      <mml:mi>j</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mfrac>
                                                   <mml:mrow>
                                                      <mml:mn>0.01013</mml:mn>
                                                      <mml:msup>
                                                         <mml:mi>T</mml:mi>
                                                         <mml:mn>1.75</mml:mn>
                                                      </mml:msup>
                                                      <mml:mrow>
                                                         <mml:mo stretchy="true">(</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mfrac>
                                                               <mml:mn>1</mml:mn>
                                                               <mml:mrow>
                                                                  <mml:msub>
                                                                     <mml:mi>M</mml:mi>
                                                                     <mml:mi>i</mml:mi>
                                                                  </mml:msub>
                                                                  <mml:mo linebreak="badbreak">×</mml:mo>
                                                                  <mml:msup>
                                                                     <mml:mn>10</mml:mn>
                                                                     <mml:mn>3</mml:mn>
                                                                  </mml:msup>
                                                               </mml:mrow>
                                                            </mml:mfrac>
                                                            <mml:mo linebreak="badbreak">+</mml:mo>
                                                            <mml:mfrac>
                                                               <mml:mn>1</mml:mn>
                                                               <mml:mrow>
                                                                  <mml:msub>
                                                                     <mml:mi>M</mml:mi>
                                                                     <mml:mi>j</mml:mi>
                                                                  </mml:msub>
                                                                  <mml:mo linebreak="badbreak">×</mml:mo>
                                                                  <mml:msup>
                                                                     <mml:mn>10</mml:mn>
                                                                     <mml:mn>3</mml:mn>
                                                                  </mml:msup>
                                                               </mml:mrow>
                                                            </mml:mfrac>
                                                         </mml:mrow>
                                                         <mml:mo stretchy="true">)</mml:mo>
                                                      </mml:mrow>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:msub>
                                                         <mml:mi>P</mml:mi>
                                                         <mml:mi>t</mml:mi>
                                                      </mml:msub>
                                                      <mml:msup>
                                                         <mml:mrow>
                                                            <mml:mo stretchy="true">[</mml:mo>
                                                            <mml:mrow>
                                                               <mml:msup>
                                                                  <mml:mrow>
                                                                     <mml:mo stretchy="true">(</mml:mo>
                                                                     <mml:mo>∑</mml:mo>
                                                                     <mml:msub>
                                                                        <mml:mi>v</mml:mi>
                                                                        <mml:mi>i</mml:mi>
                                                                     </mml:msub>
                                                                     <mml:mo stretchy="true">)</mml:mo>
                                                                  </mml:mrow>
                                                                  <mml:mfrac>
                                                                     <mml:mn>1</mml:mn>
                                                                     <mml:mn>3</mml:mn>
                                                                  </mml:mfrac>
                                                               </mml:msup>
                                                               <mml:mo linebreak="badbreak">+</mml:mo>
                                                               <mml:msup>
                                                                  <mml:mrow>
                                                                     <mml:mo stretchy="true">(</mml:mo>
                                                                     <mml:mo>∑</mml:mo>
                                                                     <mml:msub>
                                                                        <mml:mi>v</mml:mi>
                                                                        <mml:mi>j</mml:mi>
                                                                     </mml:msub>
                                                                     <mml:mo stretchy="true">)</mml:mo>
                                                                  </mml:mrow>
                                                                  <mml:mfrac>
                                                                     <mml:mn>1</mml:mn>
                                                                     <mml:mn>3</mml:mn>
                                                                  </mml:mfrac>
                                                               </mml:msup>
                                                            </mml:mrow>
                                                            <mml:mo stretchy="true">]</mml:mo>
                                                         </mml:mrow>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:msup>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
