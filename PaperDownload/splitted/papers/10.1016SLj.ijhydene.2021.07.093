                                                            <mml:mi>d</mml:mi>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                      </mml:mfrac>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>i</mml:mi>
