                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mo>−</mml:mo>
                                    </mml:mrow>
                                 </mml:msup>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math>:<ce:display>
                           <ce:formula id="fd4">
                              <ce:label>(4)</ce:label>
                              <mml:math altimg="si24.svg" alttext="Equation 4.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mi>d</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
                                                </mml:mfrac>
                                                <mml:mrow>
                                                   <mml:mo stretchy="true">(</mml:mo>
                                                   <mml:mrow>
                                                      <mml:mfrac>
