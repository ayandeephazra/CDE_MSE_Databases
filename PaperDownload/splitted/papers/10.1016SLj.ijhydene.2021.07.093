                                                      </mml:mfrac>
                                                      <mml:mfrac>
                                                         <mml:mrow>
                                                            <mml:mi>d</mml:mi>
                                                            <mml:msub>
                                                               <mml:mover accent="true">
                                                                  <mml:mi>μ</mml:mi>
                                                                  <mml:mo>˜</mml:mo>
                                                               </mml:mover>
                                                               <mml:msup>
                                                                  <mml:mtext>e</mml:mtext>
                                                                  <mml:mo linebreak="badbreak">−</mml:mo>
                                                               </mml:msup>
                                                            </mml:msub>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>d</mml:mi>
                                                            <mml:mi>x</mml:mi>
                                                         </mml:mrow>
                                                      </mml:mfrac>
                                                   </mml:mrow>
                                                   <mml:mo stretchy="true">)</mml:mo>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">=</mml:mo>
                                                <mml:mo linebreak="badbreak">−</mml:mo>
                                                <mml:msub>
                                                   <mml:mi>i</mml:mi>
                                                   <mml:mtext>ct</mml:mtext>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mtd>
                                       </mml:mtr>
                                    </mml:mtable>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd5">
                              <ce:label>(5)</ce:label>
                              <mml:math altimg="si25.svg" alttext="Equation 5.">
                                 <mml:mrow>
                                    <mml:mtable columnalign="left">
                                       <mml:mtr columnalign="left">
                                          <mml:mtd columnalign="left">
                                             <mml:mrow>
                                                <mml:mfrac>
                                                   <mml:mi>d</mml:mi>
                                                   <mml:mrow>
                                                      <mml:mi>d</mml:mi>
                                                      <mml:mi>x</mml:mi>
                                                   </mml:mrow>
