                                                            <mml:mi>σ</mml:mi>
                                                            <mml:msup>
                                                               <mml:mtext>O</mml:mtext>
                                                               <mml:mrow>
                                                                  <mml:mn>2</mml:mn>
                                                                  <mml:mo linebreak="badbreak">−</mml:mo>
                                                               </mml:mrow>
                                                            </mml:msup>
                                                            <mml:mtext>eff</mml:mtext>
                                                         </mml:msubsup>
                                                         <mml:mrow>
                                                            <mml:mn>2</mml:mn>
                                                            <mml:mi>F</mml:mi>
                                                         </mml:mrow>
                                                      </mml:mfrac>
                                                      <mml:mfrac>
                                                         <mml:mrow>
                                                            <mml:mi>d</mml:mi>
                                                            <mml:msub>
                                                               <mml:mover accent="true">
                                                                  <mml:mi>μ</mml:mi>
                                                                  <mml:mo>˜</mml:mo>
                                                               </mml:mover>
                                                               <mml:msup>
                                                                  <mml:mtext>O</mml:mtext>
                                                                  <mml:mrow>
                                                                     <mml:mn>2</mml:mn>
                                                                     <mml:mo linebreak="badbreak">−</mml:mo>
                                                                  </mml:mrow>
                                                               </mml:msup>
                                                            </mml:msub>
                                                         </mml:mrow>
