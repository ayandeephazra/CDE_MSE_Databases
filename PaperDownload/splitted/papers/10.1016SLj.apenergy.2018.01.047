                                       <mml:mo>+</mml:mo>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo>+</mml:mo>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mspace width="0.35em" height="0.8ex"/>
                                       <mml:mtext>e</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo>-</mml:mo>
                                    </mml:mrow>
                                 </mml:msup>
