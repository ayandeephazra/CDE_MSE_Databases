                                    </mml:mrow>
                                 </mml:munderover>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mtext>V</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mo>+</mml:mo>
                                    </mml:mrow>
                                 </mml:msup>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="e0010">
                              <ce:label>(2)</ce:label>
                              <mml:math altimg="si3.gif" overflow="scroll">
                                 <mml:mtext>At the positive electrode</mml:mtext>
                                 <mml:mo>:</mml:mo>
                                 <mml:mspace width="1em"/>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mtext>VO</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
