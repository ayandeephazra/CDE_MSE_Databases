                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mtext>H</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>O</mml:mtext>
                                 <mml:mo>-</mml:mo>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mtext>e</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mo>-</mml:mo>
                                    </mml:mrow>
                                 </mml:msup>
                                 <mml:munderover>
                                    <mml:mrow>
                                       <mml:mo>⇄</mml:mo>
                                    </mml:mrow>
