                     <ce:display>
                        <ce:formula id="fd3">
                           <ce:label>(3)</ce:label>
                           <mml:math altimg="si3.svg" alttext="Equation 3.">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mtext>H</mml:mtext>
                                    </mml:mrow>
                                    <mml:mn>2</mml:mn>
                                 </mml:msub>
