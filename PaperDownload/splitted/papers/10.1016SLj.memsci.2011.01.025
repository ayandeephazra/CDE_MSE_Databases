                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" rowsep="1" align="left">PPR/N117 (0.5<hsp sp="0.25"/>M, 6<hsp sp="0.25"/>min)</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" rowsep="1" align="left">PPR/N117 (1<hsp sp="0.25"/>M, 6<hsp sp="0.25"/>min)</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" rowsep="1" align="left">PPR/N117 (1<hsp sp="0.25"/>M, 24<hsp sp="0.25"/>h)</entry>
