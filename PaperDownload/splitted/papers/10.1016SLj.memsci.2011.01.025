            <ce:appendices view="all">
               <ce:section id="sec0080" view="compact-standard">
                  <ce:label>Appendix A</ce:label>
                  <ce:section-title>Supplementary data</ce:section-title>
                  <ce:para id="par0250" view="all">Supplementary data associated with this article can be found, in the online version, at <ce:inter-ref xlink:href="doi:10.1016/j.memsci.2011.01.025" xlink:type="simple">doi:10.1016/j.memsci.2011.01.025</ce:inter-ref>.</ce:para>
               </ce:section>
