               <ce:keyword>
                  <ce:text>Ca<ce:inf loc="post">2</ce:inf>Fe<ce:inf loc="post">2</ce:inf>O<ce:inf loc="post">5</ce:inf>
                  </ce:text>
               </ce:keyword>
