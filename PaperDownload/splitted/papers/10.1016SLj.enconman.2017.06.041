                                                </mml:msub>
                                                <mml:mo>+</mml:mo>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mi>r</mml:mi>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mi>k</mml:mi>
                                                   </mml:mrow>
                                                </mml:msub>
