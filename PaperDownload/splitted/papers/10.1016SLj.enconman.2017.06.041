                                       <mml:mrow>
                                          <mml:mi>k</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:mrow>
                                       <mml:mfenced open="[" close="]">
                                          <mml:mrow>
                                             <mml:mtable>
                                                <mml:mtr>
                                                   <mml:mtd columnalign="center">
