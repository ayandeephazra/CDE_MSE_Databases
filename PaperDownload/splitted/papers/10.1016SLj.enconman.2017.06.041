                                                         <mml:mrow>
                                                            <mml:mi>γ</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>k</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                   </mml:mtd>
                                                </mml:mtr>
                                                <mml:mtr>
                                                   <mml:mtd columnalign="center">
                                                      <mml:msub>
                                                         <mml:mrow>
                                                            <mml:mi>a</mml:mi>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mi>k</mml:mi>
                                                         </mml:mrow>
                                                      </mml:msub>
                                                   </mml:mtd>
                                                </mml:mtr>
                                             </mml:mtable>
                                          </mml:mrow>
                                       </mml:mfenced>
                                    </mml:mrow>
                                    <mml:mtext>,</mml:mtext>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>results into<ce:display>
                           <ce:formula id="e0080">
                              <ce:label>(15)</ce:label>
                              <mml:math altimg="si105.gif" overflow="scroll">
