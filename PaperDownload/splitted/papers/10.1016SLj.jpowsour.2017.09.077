                     <ce:sup loc="post">∗</ce:sup>
                  </ce:cross-ref>
                  
                  
               </ce:author>
               <ce:author id="au2" author-id="S0378775317313046-87dbfdc735a1a141097ad035f25c640d">
                  <ce:given-name>Yoon Seok</ce:given-name>
                  <ce:surname>Ko</ce:surname>
