                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> in atm):<ce:display>
                           <ce:formula id="fd10">
                              <ce:label>(10)</ce:label>
                              <mml:math altimg="si22.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mi>δ</mml:mi>
