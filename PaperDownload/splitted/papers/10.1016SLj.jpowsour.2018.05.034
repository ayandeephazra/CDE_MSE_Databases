                                          <mml:mi>l</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>m</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:msub>
                                             <mml:mi>r</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>p</mml:mi>
                                                <mml:mi>l</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>t</mml:mi>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>u</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mo>-</mml:mo>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>p</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>l</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>m</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>e</mml:mi>
