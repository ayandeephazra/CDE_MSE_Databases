                                                <mml:mi>i</mml:mi>
                                                <mml:mi>n</mml:mi>
                                                <mml:mi>i</mml:mi>
                                                <mml:mi>t</mml:mi>
                                                <mml:mi>i</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>l</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>p</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>r</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>c</mml:mi>
                                          <mml:mi>l</mml:mi>
                                          <mml:mi>e</mml:mi>
                                          <mml:mspace width="0.25em"/>
