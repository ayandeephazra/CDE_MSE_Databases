            <ce:abstract id="abs0005" class="graphical" view="all">
               <ce:section-title id="sect0005">Graphical abstract</ce:section-title>
               <ce:abstract-sec id="abst0005" view="all">
                  <ce:simple-para id="spar0050" view="all">
                     <ce:display>
                        <ce:figure id="fig0040">
                           <ce:link locator="fx1" xlink:type="simple" xlink:href="pii:S0013468615309543/fx1"/>
                        </ce:figure>
