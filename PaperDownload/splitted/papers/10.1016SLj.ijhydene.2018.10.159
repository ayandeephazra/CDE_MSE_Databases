         <ce:floats>
            <ce:figure id="fig1">
               <ce:label>Fig. 1</ce:label>
               <ce:caption id="cap0010">
                  <ce:simple-para id="fspara0010" view="all">Schematic illustration of component preparation and set-up for a planar SOFC whose cathode contact layer using MYC-LSCF composites:<ce:list id="olist0010">
                        <ce:list-item id="o0010">
                           <ce:label>(A)</ce:label>
                           <ce:para id="p0090" view="all">for micron-sized LSCF particles and nano-sized MnO, Co and Y<ce:inf loc="post">2</ce:inf>O<ce:inf loc="post">3</ce:inf> derived from the reduced MYC spinel powders.</ce:para>
                        </ce:list-item>
                        <ce:list-item id="o0015">
                           <ce:label>(B)</ce:label>
                           <ce:para id="p0095" view="all">for mixed powders slurry.</ce:para>
                        </ce:list-item>
                        <ce:list-item id="o0020">
                           <ce:label>(C)</ce:label>
                           <ce:para id="p0100" view="all">for sintering to dense MYC-LSCF ceramics.</ce:para>
                        </ce:list-item>
                        <ce:list-item id="o0025">
                           <ce:label>(D)</ce:label>
                           <ce:para id="p0105" view="all">for component assembly composed of green cathode contact layer.</ce:para>
                        </ce:list-item>
                        <ce:list-item id="o0030">
                           <ce:label>(E)</ce:label>
                           <ce:para id="p0110" view="all">for contact layer sintering.</ce:para>
                        </ce:list-item>
                     </ce:list>
