                  <ce:surname>Sun</ce:surname>
                  <ce:cross-ref refid="cor1">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:author>
                  <ce:given-name>N.Q.</ce:given-name>
                  <ce:surname>Zhang</ce:surname>
               </ce:author>
