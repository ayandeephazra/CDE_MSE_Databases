                  <ce:section id="sec3.2" view="all">
                     <ce:section-title id="sectitle0070">Discussion</ce:section-title>
                     <ce:section id="sec3.2.1" view="all">
                        <ce:section-title id="sectitle0075">Conductivity of each type of charge carrier</ce:section-title>
                        <ce:para id="p0100" view="all">In a B-site-doped Ba(Zr,Ce)O<ce:inf loc="post">3</ce:inf>
