                  <ce:text>Industrial coating process</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Single step co-firing of half-cells</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Solid oxide fuel cell</ce:text>
               </ce:keyword>
