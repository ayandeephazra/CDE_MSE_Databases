            <ce:keywords xml:lang="en" id="kwrds0010" view="all" class="keyword">
               <ce:section-title id="sectitle0025">Keywords</ce:section-title>
               <ce:keyword id="kwrd0010">
                  <ce:text>Solid-oxide fuel cells</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0015">
                  <ce:text>Dual-phase hetero-structure</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0020">
                  <ce:text>Composite cathode materials</ce:text>
               </ce:keyword>
