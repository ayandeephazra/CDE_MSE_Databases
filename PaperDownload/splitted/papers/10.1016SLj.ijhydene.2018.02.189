                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mtext>N</mml:mtext>
                                    </mml:mfrac>
                                    <mml:munderover>
                                       <mml:mo>∑</mml:mo>
                                       <mml:mrow>
                                          <mml:mtext>i</mml:mtext>
