                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mtext>ε</mml:mtext>
                                                <mml:mtext>p</mml:mtext>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mfrac>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mover accent="true">
                                                         <mml:mo>∇</mml:mo>
                                                         <mml:mo stretchy="true">⇀</mml:mo>
                                                      </mml:mover>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mover accent="true">
                                                         <mml:mi>v</mml:mi>
                                                         <mml:mo stretchy="true">⇀</mml:mo>
                                                      </mml:mover>
                                                   </mml:mrow>
                                                </mml:mrow>
                                                <mml:mo>)</mml:mo>
                                             </mml:mrow>
                                             <mml:mo>+</mml:mo>
                                             <mml:msup>
                                                <mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mo>(</mml:mo>
                                                      <mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mover accent="true">
                                                               <mml:mo>∇</mml:mo>
                                                               <mml:mo stretchy="true">⇀</mml:mo>
                                                            </mml:mover>
                                                         </mml:mrow>
                                                         <mml:mrow>
                                                            <mml:mover accent="true">
                                                               <mml:mi>v</mml:mi>
                                                               <mml:mo stretchy="true">⇀</mml:mo>
                                                            </mml:mover>
                                                         </mml:mrow>
                                                      </mml:mrow>
                                                      <mml:mo>)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                                <mml:mtext>T</mml:mtext>
                                             </mml:msup>
                                          </mml:mrow>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                                <mml:mtext>μ</mml:mtext>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mrow>
                                                      <mml:mn>3</mml:mn>
                                                      <mml:mtext>ε</mml:mtext>
                                                   </mml:mrow>
                                                   <mml:mtext>p</mml:mtext>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mover accent="true">
                                                   <mml:mo>∇</mml:mo>
                                                   <mml:mo stretchy="true">⇀</mml:mo>
                                                </mml:mover>
                                             </mml:mrow>
                                             <mml:mo>⋅</mml:mo>
                                             <mml:mrow>
                                                <mml:mover accent="true">
                                                   <mml:mi>v</mml:mi>
                                                   <mml:mo stretchy="true">⇀</mml:mo>
                                                </mml:mover>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                       <mml:mi>I</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>]</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mtext>μ</mml:mtext>
                                          <mml:mtext>κ</mml:mtext>
                                       </mml:mfrac>
                                       <mml:mo>+</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mo>∇</mml:mo>
                                             <mml:mo stretchy="true">⇀</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mo>·</mml:mo>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:mi>ρ</mml:mi>
                                             <mml:mrow>
                                                <mml:mover accent="true">
                                                   <mml:mi>v</mml:mi>
                                                   <mml:mo stretchy="true">⇀</mml:mo>
                                                </mml:mover>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mi>v</mml:mi>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where μ is viscosity of gases, ε<ce:inf loc="post">p</ce:inf> is porosity and κ is permeability of the electrode media.<ce:list id="ulist0025">
                        <ce:list-item id="u0040">
                           <ce:label>•</ce:label>
                           <ce:para id="p0305" view="all">Species conservation equation:</ce:para>
                        </ce:list-item>
                     </ce:list>
                     <ce:display>
                        <ce:formula id="fdA3">
                           <ce:label>A3</ce:label>
                           <mml:math altimg="si15.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:mi>ρ</mml:mi>
                                       <mml:mo>·</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>v</mml:mi>
                                             <mml:mo stretchy="true">⇀</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mo>·</mml:mo>
                                       <mml:msub>
                                          <mml:mtext>Y</mml:mtext>
                                          <mml:mtext>i</mml:mtext>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>=</mml:mo>
                                 <mml:mo>−</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>j</mml:mi>
                                             <mml:mtext>i</mml:mtext>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>S</mml:mi>
                                    <mml:mtext>i</mml:mtext>
                                 </mml:msub>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <ce:italic>j</ce:italic>
                     <ce:inf loc="post">i</ce:inf> is the multicomponent diffusive mass flux vector, Y<ce:inf loc="post">i</ce:inf> is mass fraction of a gas specie. Diffusion in porous media is commonly modelled by using effective diffusion coefficient <ce:italic>D</ce:italic>
                     <ce:inf loc="post">i,j,eff</ce:inf> = ε<ce:inf loc="post">p</ce:inf>
                     <ce:sup loc="post">3/2</ce:sup>·<ce:italic>D</ce:italic>
                     <ce:inf loc="post">i,j</ce:inf>, where <ce:italic>D</ce:italic>
                     <ce:inf loc="post">i,j</ce:inf> is binary diffusion coefficient of gas species i and j:<ce:display>
                        <ce:formula id="fdA4">
                           <ce:label>A4</ce:label>
                           <mml:math altimg="si16.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>j</mml:mi>
                                             <mml:mtext>i</mml:mtext>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>=</mml:mo>
                                 <mml:mo>−</mml:mo>
                                 <mml:munderover>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mrow>
                                       <mml:mtext>j</mml:mtext>
                                       <mml:mo>=</mml:mo>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mtext>N</mml:mtext>
                                       <mml:mo>-</mml:mo>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                 </mml:munderover>
                                 <mml:mrow>
                                    <mml:mi>ρ</mml:mi>
                                    <mml:mo>·</mml:mo>
                                    <mml:msub>
                                       <mml:mi>D</mml:mi>
                                       <mml:mrow>
                                          <mml:mtext>i,j,eff</mml:mtext>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mtext>Y</mml:mtext>
                                    <mml:mtext>j</mml:mtext>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:mo>−</mml:mo>
                                 <mml:munderover>
                                    <mml:mo>∑</mml:mo>
                                    <mml:mrow>
                                       <mml:mtext>j</mml:mtext>
                                       <mml:mo>=</mml:mo>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mtext>N</mml:mtext>
                                       <mml:mo>-</mml:mo>
                                       <mml:mn>1</mml:mn>
                                    </mml:mrow>
                                 </mml:munderover>
                                 <mml:mrow>
                                    <mml:mi>ρ</mml:mi>
                                    <mml:mo>·</mml:mo>
                                    <mml:msubsup>
                                       <mml:mtext>ε</mml:mtext>
                                       <mml:mtext>p</mml:mtext>
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mn>3</mml:mn>
                                             <mml:mn>2</mml:mn>
                                          </mml:mfrac>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>·</mml:mo>
                                    <mml:msub>
                                       <mml:mi>D</mml:mi>
                                       <mml:mrow>
                                          <mml:mtext>i,j</mml:mtext>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mtext>Y</mml:mtext>
                                    <mml:mtext>j</mml:mtext>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                     <ce:list id="ulist0030">
                        <ce:list-item id="u0045">
                           <ce:label>•</ce:label>
                           <ce:para id="p0310" view="all">Charge conservation equation:</ce:para>
                        </ce:list-item>
                     </ce:list>
                     <ce:display>
                        <ce:formula id="fdA5">
                           <ce:label>A5</ce:label>
                           <mml:math altimg="si17.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:mo>−</mml:mo>
                                       <mml:msub>
                                          <mml:mi>σ</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>s/el,eff</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>·</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mo>∇</mml:mo>
                                             <mml:mo stretchy="true">⇀</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:msub>
                                          <mml:mi>ϕ</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>s/el</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>S</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>s/el,</mml:mtext>
                                       <mml:mi>i</mml:mi>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0315" view="all">The electronic/ionic current transport in porous electrodes is modelled by considering conservation of electric charge, where <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">s/el,eff</ce:inf> is effective electronic/ionic conductivity of porous electrode and <ce:italic>ϕ</ce:italic>
                     <ce:inf loc="post">s/el</ce:inf> is electrostatic potential in the solid/electrolyte phase. It is assumed that no electrical current source (<ce:italic>S</ce:italic>
                     <ce:inf loc="post">s,<ce:italic>i</ce:italic>
                     </ce:inf> = 0) is present within the solid (metal) phase.<ce:list id="ulist0035">
                        <ce:list-item id="u0050">
                           <ce:label>•</ce:label>
                           <ce:para id="p0320" view="all">Energy conservation equation:</ce:para>
                        </ce:list-item>
                     </ce:list>
                     <ce:display>
                        <ce:formula id="fdA6">
                           <ce:label>A6</ce:label>
                           <mml:math altimg="si18.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mi>ρ</mml:mi>
                                                   <mml:mo>·</mml:mo>
                                                   <mml:msub>
                                                      <mml:mi>C</mml:mi>
                                                      <mml:mi>p</mml:mi>
                                                   </mml:msub>
                                                </mml:mrow>
                                                <mml:mo>)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mtext>eff</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>·</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>v</mml:mi>
                                             <mml:mo stretchy="true">⇀</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mo>·</mml:mo>
                                       <mml:mi>T</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>=</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>k</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>s,eff</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>·</mml:mo>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mo>∇</mml:mo>
                                             <mml:mo stretchy="true">⇀</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mi>T</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>S</mml:mi>
                                    <mml:mtext>e</mml:mtext>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0325" view="all">The energy transport in porous electrodes is modelled by using effective thermal conductivity (<ce:italic>k</ce:italic>
                     <ce:inf loc="post">s,eff</ce:inf>), and effective product of specific heat (<ce:italic>C</ce:italic>
                     <ce:inf loc="post">
                        <ce:italic>p</ce:italic>
                     </ce:inf>) and specific density (<ce:italic>ρ</ce:italic>) of the solid material. The heat source <ce:italic>S</ce:italic>
                     <ce:inf loc="post">e</ce:inf> depends on chemical (reversible), electrochemical (irreversible) and ohmic heat generation. The reversible heat term in SOEC cathode <ce:italic>q</ce:italic>
                     <ce:inf loc="post">rev,c</ce:inf> is:<ce:display>
                        <ce:formula id="fdA7">
                           <ce:label>A7</ce:label>
                           <mml:math altimg="si19.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>rev,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>i</mml:mi>
                                          <mml:mtext>c</mml:mtext>
                                       </mml:msub>
                                       <mml:mo>·</mml:mo>
                                       <mml:mo>Δ</mml:mo>
                                       <mml:msub>
                                          <mml:mtext>H</mml:mtext>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mtext>H</mml:mtext>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msub>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mo>·</mml:mo>
                                       <mml:mtext>F</mml:mtext>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <ce:italic>i</ce:italic>
                     <ce:inf loc="post">c</ce:inf> is cathodic current density, <mml:math altimg="si20.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:mo>Δ</mml:mo>
                           <mml:msub>
                              <mml:mtext>H</mml:mtext>
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mtext>H</mml:mtext>
                                    <mml:mn>2</mml:mn>
                                 </mml:msub>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo>=</mml:mo>
                           <mml:mo>−</mml:mo>
                           <mml:mn>241</mml:mn>
                           <mml:msup>
                              <mml:mrow>
                                 <mml:mtext>kJmol</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo>−</mml:mo>
                                 <mml:mn>1</mml:mn>
                              </mml:mrow>
                           </mml:msup>
                        </mml:mrow>
                     </mml:math> is the standard enthalpy change of formation, and F = 96485 As mol<ce:sup loc="post">−1</ce:sup> is Faraday constant. The irreversible heat term <ce:italic>q</ce:italic>
                     <ce:inf loc="post">irr,c</ce:inf> is:<ce:display>
                        <ce:formula id="fdA8">
                           <ce:label>A8</ce:label>
                           <mml:math altimg="si21.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>irr,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>i</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>c,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:msub>
                                    <mml:mi>η</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>act,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0330" view="all">The ohmic heat term <ce:italic>q</ce:italic>
                     <ce:inf loc="post">ohm,c</ce:inf>:<ce:display>
                        <ce:formula id="fdA9">
                           <ce:label>A9</ce:label>
                           <mml:math altimg="si22.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ohm,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>σ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>sc,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mtext>s</mml:mtext>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mtext>s</mml:mtext>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>σ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ec,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>el</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>el</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where the <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">sc,eff</ce:inf> and <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">ec,eff</ce:inf> are effective cathode electronic and ionic conductivity. The total heat generation term <ce:italic>S</ce:italic>
                     <ce:inf loc="post">e,c</ce:inf> is the sum of <ce:italic>q</ce:italic>
                     <ce:inf loc="post">rev,c</ce:inf>, <ce:italic>q</ce:italic>
                     <ce:inf loc="post">irr,c</ce:inf> and <ce:italic>q</ce:italic>
                     <ce:inf loc="post">ohm,c</ce:inf>:<ce:display>
                        <ce:formula id="fdA10">
                           <ce:label>A10</ce:label>
                           <mml:math altimg="si23.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>S</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>e,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>rev,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>irr,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ohm,c</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0335" view="all">The irreversible heat term <ce:italic>q</ce:italic>
                     <ce:inf loc="post">irr,a</ce:inf> in the anode is calculated similarly:<ce:display>
                        <ce:formula id="fdA11">
                           <ce:label>A11</ce:label>
                           <mml:math altimg="si24.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>irr,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>i</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>a,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:msub>
                                    <mml:mi>η</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>act,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0340" view="all">The ohmic heat term <ce:italic>q</ce:italic>
                     <ce:inf loc="post">ohm,a</ce:inf> is calculated as:<ce:display>
                        <ce:formula id="fdA12">
                           <ce:label>A12</ce:label>
                           <mml:math altimg="si25.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ohm,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>σ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>sa,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mtext>s</mml:mtext>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mtext>s</mml:mtext>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>σ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ea,eff</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>el</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>·</mml:mo>
                                 <mml:mrow>
                                    <mml:mover accent="true">
                                       <mml:mo>∇</mml:mo>
                                       <mml:mo stretchy="true">⇀</mml:mo>
                                    </mml:mover>
                                 </mml:mrow>
                                 <mml:msub>
                                    <mml:mi>ϕ</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>el</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>,</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where the <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">sa,eff</ce:inf> and <ce:italic>σ</ce:italic>
                     <ce:inf loc="post">ea,eff</ce:inf> are effective anode electronic and ionic conductivity. The total heat generation term <ce:italic>S</ce:italic>
                     <ce:inf loc="post">e,a</ce:inf> is the sum of <ce:italic>q</ce:italic>
                     <ce:inf loc="post">irr,a</ce:inf> and <ce:italic>q</ce:italic>
                     <ce:inf loc="post">ohm,a</ce:inf>:<ce:display>
                        <ce:formula id="fdA13">
                           <ce:label>A13</ce:label>
                           <mml:math altimg="si26.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:msub>
                                    <mml:mi>S</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>e,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>=</mml:mo>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>irr,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mo>+</mml:mo>
                                 <mml:msub>
                                    <mml:mi>q</mml:mi>
                                    <mml:mrow>
                                       <mml:mtext>ohm,a</mml:mtext>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:mtext>.</mml:mtext>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
