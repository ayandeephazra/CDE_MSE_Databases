                  <ce:textfn>School of Material Science and Engineering, Jingdezhen Ceramic Institute, Jingdezhen 333001, People's Republic of China</ce:textfn>
               </ce:affiliation>
               <ce:correspondence id="cr0005">
                  <ce:label>⁎</ce:label>
                  <ce:text>Corresponding author. Tel./fax: +<ce:hsp sp="0.10"/>86 798 8499328.</ce:text>
               </ce:correspondence>
