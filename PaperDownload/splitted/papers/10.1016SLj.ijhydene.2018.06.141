                                       <mml:mrow>
                                          <mml:mtext>Cr</mml:mtext>
                                       </mml:mrow>
                                       <mml:mn>2</mml:mn>
                                    </mml:msub>
                                    <mml:msub>
                                       <mml:mtext>O</mml:mtext>
                                       <mml:mn>3</mml:mn>
                                    </mml:msub>
                                    <mml:mrow>
