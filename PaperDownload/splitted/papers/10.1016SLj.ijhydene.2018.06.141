                                       <mml:mtext>O</mml:mtext>
                                       <mml:mn>2</mml:mn>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mtext>g</mml:mtext>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>→</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mtext>CrO</mml:mtext>
                                       </mml:mrow>
                                       <mml:mn>3</mml:mn>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mo>(</mml:mo>
                                       <mml:mtext>g</mml:mtext>
