                                    <mml:mi>H</mml:mi>
                                    <mml:mn>2</mml:mn>
                                 </mml:msub>
                                 <mml:mo linebreak="goodbreak">+</mml:mo>
                                 <mml:mfrac>
                                    <mml:mn>1</mml:mn>
                                    <mml:mn>2</mml:mn>
                                 </mml:mfrac>
