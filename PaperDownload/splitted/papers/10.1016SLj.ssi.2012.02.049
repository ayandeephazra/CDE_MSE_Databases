            <ce:title>Novel Ba<ce:inf loc="post">0.5</ce:inf>Sr<ce:inf loc="post">0.5</ce:inf>(Co<ce:inf loc="post">0.8</ce:inf>Fe<ce:inf loc="post">0.2</ce:inf>)<ce:inf loc="post">1<ce:hsp sp="0.12"/>−<ce:hsp sp="0.12"/>x</ce:inf>Ti<ce:inf loc="post">x</ce:inf>O<ce:inf loc="post">3<ce:hsp sp="0.12"/>−<ce:hsp sp="0.12"/>
