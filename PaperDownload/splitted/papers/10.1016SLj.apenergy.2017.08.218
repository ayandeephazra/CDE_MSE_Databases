                     <sa:organization>Changwon National University</sa:organization>
                     <sa:country>Republic of Korea</sa:country>
                  </sa:affiliation>
               </ce:affiliation>
               <ce:correspondence id="cor1">
                  <ce:label>⁎</ce:label>
                  <ce:text>Corresponding author at: Mechanical Engineering Department of Changwon National University, 20 Changwondaehak-ro, Changwon City 51140, Republic of Korea.</ce:text>
                  <sa:affiliation>
                     <sa:organization>Mechanical Engineering Department of Changwon National University</sa:organization>
                     <sa:address-line>20 Changwondaehak-ro</sa:address-line>
                     <sa:city>Changwon City</sa:city>
