                                                <mml:mrow>
                                                   <mml:mtext>Co</mml:mtext>
                                                </mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                             <mml:msub>
                                                <mml:mtext>O</mml:mtext>
                                                <mml:mn>6</mml:mn>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mover>
                                    </mml:mrow>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                          <mml:mtext>Co</mml:mtext>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>Co</mml:mtext>
