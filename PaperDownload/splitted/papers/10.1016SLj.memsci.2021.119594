                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mspace width="0.25em"/>
                                          <mml:msub>
                                             <mml:mi>δ</mml:mi>
                                             <mml:mi>t</mml:mi>
                                          </mml:msub>
                                       </mml:mrow>
