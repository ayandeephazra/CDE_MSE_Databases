                                             </mml:mrow>
                                             <mml:mn>0</mml:mn>
                                             <mml:mi>t</mml:mi>
                                          </mml:msubsup>
                                          <mml:mi>I</mml:mi>
                                          <mml:mrow>
                                             <mml:mo stretchy="true">(</mml:mo>
