                  <ce:text>Electrical conductivity</ce:text>
               </ce:keyword>
               <ce:keyword id="k0015">
                  <ce:text>Cathode</ce:text>
               </ce:keyword>
               <ce:keyword id="k0020">
                  <ce:text>BaBi<ce:inf loc="post">0.05</ce:inf>Co<ce:inf loc="post">0.8</ce:inf>Nb<ce:inf loc="post">0.15</ce:inf>O<ce:inf loc="post">3−</ce:inf>
                     <ce:italic>
                        <ce:inf loc="post">δ</ce:inf>
                     </ce:italic>
                  </ce:text>
               </ce:keyword>
            </ce:keywords>
