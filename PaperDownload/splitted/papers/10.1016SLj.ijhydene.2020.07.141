                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:mn>1</mml:mn>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">/</mml:mo>
                                                <mml:mn>4</mml:mn>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:msubsup>
                                          <mml:mi>P</mml:mi>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mtext>O</mml:mtext>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                             <mml:mo>,</mml:mo>
                                             <mml:mn>0</mml:mn>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>−</mml:mo>
                                                   <mml:mn>1</mml:mn>
                                                </mml:mrow>
                                                <mml:mo linebreak="badbreak">/</mml:mo>
                                                <mml:mn>4</mml:mn>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:msubsup>
                                    </mml:mrow>
                                 </mml:mfrac>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>
                  </ce:para>
                  <ce:para id="p0110" view="all">According to Eqs. <ce:cross-refs id="crosrefs0030" refid="fd13 fd14">(13) and (14)</ce:cross-refs>, the overpotential, <mml:math altimg="si46.svg">
                        <mml:mi>η</mml:mi>
                     </mml:math>, can be expressed by the normalized conductivity <mml:math altimg="si8.svg">
                        <mml:mrow>
                           <mml:mi>g</mml:mi>
                           <mml:mrow>
                              <mml:mo stretchy="true">(</mml:mo>
                              <mml:mi>t</mml:mi>
                              <mml:mo stretchy="true">)</mml:mo>
                           </mml:mrow>
                        </mml:mrow>
                     </mml:math>,<ce:display>
                        <ce:formula id="fd15">
                           <ce:label>(15)</ce:label>
                           <mml:math altimg="si47.svg" alttext="Equation 15.">
                              <mml:mrow>
                                 <mml:mi>η</mml:mi>
                                 <mml:mrow>
                                    <mml:mo stretchy="true">(</mml:mo>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi>R</mml:mi>
                                       <mml:mi>T</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mi>z</mml:mi>
                                       <mml:mi>F</mml:mi>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mi>ln</mml:mi>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>P</mml:mi>
                                                            <mml:mrow>
                                                               <mml:msub>
                                                                  <mml:mtext>CO</mml:mtext>
                                                                  <mml:mn>2</mml:mn>
                                                               </mml:msub>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mi>∞</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>P</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mtext>CO</mml:mtext>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mi>∞</mml:mi>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>P</mml:mi>
                                                            <mml:mrow>
                                                               <mml:msub>
                                                                  <mml:mtext>CO</mml:mtext>
                                                                  <mml:mn>2</mml:mn>
                                                               </mml:msub>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mn>0</mml:mn>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>P</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mtext>CO</mml:mtext>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mn>0</mml:mn>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                </mml:mrow>
                                                <mml:mo stretchy="true">)</mml:mo>
                                             </mml:mrow>
                                          </mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:msup>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">[</mml:mo>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msup>
                                                            <mml:mrow>
                                                               <mml:mrow>
                                                                  <mml:mo stretchy="true">(</mml:mo>
                                                                  <mml:mrow>
                                                                     <mml:mfrac>
                                                                        <mml:mrow>
                                                                           <mml:msub>
                                                                              <mml:mi>P</mml:mi>
                                                                              <mml:mrow>
                                                                                 <mml:msub>
                                                                                    <mml:mtext>CO</mml:mtext>
                                                                                    <mml:mn>2</mml:mn>
                                                                                 </mml:msub>
                                                                                 <mml:mo>,</mml:mo>
                                                                                 <mml:mi>∞</mml:mi>
                                                                              </mml:mrow>
                                                                           </mml:msub>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:msub>
                                                                              <mml:mi>P</mml:mi>
                                                                              <mml:mrow>
                                                                                 <mml:mtext>CO</mml:mtext>
                                                                                 <mml:mo>,</mml:mo>
                                                                                 <mml:mi>∞</mml:mi>
                                                                              </mml:mrow>
                                                                           </mml:msub>
                                                                        </mml:mrow>
                                                                     </mml:mfrac>
                                                                  </mml:mrow>
                                                                  <mml:mo stretchy="true">)</mml:mo>
                                                               </mml:mrow>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mrow>
                                                                  <mml:mrow>
                                                                     <mml:mo>−</mml:mo>
                                                                     <mml:mn>1</mml:mn>
                                                                  </mml:mrow>
                                                                  <mml:mo linebreak="badbreak">/</mml:mo>
                                                                  <mml:mn>2</mml:mn>
                                                               </mml:mrow>
                                                            </mml:mrow>
                                                         </mml:msup>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msup>
                                                            <mml:mrow>
                                                               <mml:mrow>
                                                                  <mml:mo stretchy="true">(</mml:mo>
                                                                  <mml:mrow>
                                                                     <mml:mfrac>
                                                                        <mml:mrow>
                                                                           <mml:msub>
                                                                              <mml:mi>P</mml:mi>
                                                                              <mml:mrow>
                                                                                 <mml:msub>
                                                                                    <mml:mtext>CO</mml:mtext>
                                                                                    <mml:mn>2</mml:mn>
                                                                                 </mml:msub>
                                                                                 <mml:mo>,</mml:mo>
                                                                                 <mml:mn>0</mml:mn>
                                                                              </mml:mrow>
                                                                           </mml:msub>
                                                                        </mml:mrow>
                                                                        <mml:mrow>
                                                                           <mml:msub>
                                                                              <mml:mi>P</mml:mi>
                                                                              <mml:mrow>
                                                                                 <mml:mtext>CO</mml:mtext>
                                                                                 <mml:mo>,</mml:mo>
                                                                                 <mml:mn>0</mml:mn>
                                                                              </mml:mrow>
                                                                           </mml:msub>
                                                                        </mml:mrow>
                                                                     </mml:mfrac>
                                                                  </mml:mrow>
                                                                  <mml:mo stretchy="true">)</mml:mo>
                                                               </mml:mrow>
                                                            </mml:mrow>
                                                            <mml:mrow>
                                                               <mml:mrow>
                                                                  <mml:mrow>
                                                                     <mml:mo>−</mml:mo>
                                                                     <mml:mn>1</mml:mn>
                                                                  </mml:mrow>
                                                                  <mml:mo linebreak="badbreak">/</mml:mo>
                                                                  <mml:mn>2</mml:mn>
                                                               </mml:mrow>
                                                            </mml:mrow>
                                                         </mml:msup>
                                                      </mml:mrow>
                                                   </mml:mfrac>
                                                   <mml:mo linebreak="badbreak">−</mml:mo>
