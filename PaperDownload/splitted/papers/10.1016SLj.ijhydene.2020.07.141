                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>c</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
