                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mrow>
                                                <mml:mo stretchy="true">(</mml:mo>
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mi>K</mml:mi>
                                                      <mml:mrow>
                                                         <mml:mi>e</mml:mi>
                                                         <mml:mi>q</mml:mi>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>P</mml:mi>
                                                            <mml:mrow>
                                                               <mml:msub>
                                                                  <mml:mtext>CO</mml:mtext>
