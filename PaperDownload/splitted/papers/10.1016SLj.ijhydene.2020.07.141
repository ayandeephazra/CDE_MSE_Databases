                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:mfrac>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mo linebreak="newline">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi>R</mml:mi>
