                                                                        <mml:mrow>
                                                                           <mml:mfrac>
                                                                              <mml:mrow>
                                                                                 <mml:msub>
                                                                                    <mml:mi>P</mml:mi>
                                                                                    <mml:mrow>
                                                                                       <mml:msub>
                                                                                          <mml:mtext>CO</mml:mtext>
