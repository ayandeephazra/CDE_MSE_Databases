                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>σ</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi>σ</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>∞</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>σ</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
