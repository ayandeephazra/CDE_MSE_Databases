                                    <mml:mi>t</mml:mi>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:mi>c</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
                                          <mml:mi>t</mml:mi>
                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                       <mml:mo linebreak="badbreak">−</mml:mo>
                                       <mml:mi>c</mml:mi>
                                       <mml:mrow>
                                          <mml:mo stretchy="true">(</mml:mo>
