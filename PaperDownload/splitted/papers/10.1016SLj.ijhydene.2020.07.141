                                             </mml:mfrac>
                                             <mml:msub>
                                                <mml:mi>k</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>c</mml:mi>
                                                   <mml:mi>h</mml:mi>
                                                   <mml:mi>e</mml:mi>
                                                   <mml:mi>m</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
