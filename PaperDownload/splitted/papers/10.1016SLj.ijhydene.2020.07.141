                                          <mml:mo stretchy="true">)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:mfrac>
                              </mml:mrow>
                           </mml:math>
                        </ce:formula>
                     </ce:display>where <mml:math altimg="si8.svg">
                        <mml:mrow>
