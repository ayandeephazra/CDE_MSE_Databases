                                 <mml:mfrac>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mi>M</mml:mi>
                                          <mml:mi>b</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msubsup>
                                       <mml:mtext> </mml:mtext>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:mn>1</mml:mn>
                                       <mml:mo>-</mml:mo>
                                       <mml:msubsup>
                                          <mml:mi>ν</mml:mi>
                                          <mml:mn>2</mml:mn>
                                          <mml:mn>2</mml:mn>
                                       </mml:msubsup>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:mtext> </mml:mtext>
                                       <mml:msub>
                                          <mml:mi>E</mml:mi>
                                          <mml:mn>2</mml:mn>
                                       </mml:msub>
                                    </mml:mrow>
                                 </mml:mfrac>
                                 <mml:mfenced open="(" close=")">
                                    <mml:mrow>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>I</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mfrac>
                                       <mml:mo>-</mml:mo>
                                       <mml:mfrac>
                                          <mml:mn>1</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>I</mml:mi>
                                                <mml:mi>c</mml:mi>
                                             </mml:msub>
