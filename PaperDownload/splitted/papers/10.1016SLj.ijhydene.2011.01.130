               <ce:link locator="gr5"/>
            </ce:figure>
            <ce:figure id="fig6">
               <ce:label>Fig. 6</ce:label>
               <ce:caption>
                  <ce:simple-para id="fspara0035" view="all">Formation model of the sub-scale.</ce:simple-para>
