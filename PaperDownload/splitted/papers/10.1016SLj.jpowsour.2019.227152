                                             <mml:mi>o</mml:mi>
                                             <mml:mi>h</mml:mi>
                                             <mml:mi>m</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="goodbreak">=</mml:mo>
                                       <mml:munderover>
                                          <mml:mrow>
                                             <mml:mo>∫</mml:mo>
                                          </mml:mrow>
