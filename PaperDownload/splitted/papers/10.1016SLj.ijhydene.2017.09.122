                                    <mml:mn>2</mml:mn>
                                    <mml:mrow>
                                       <mml:mo>[</mml:mo>
                                       <mml:mrow>
                                          <mml:msubsup>
                                             <mml:mi>V</mml:mi>
                                             <mml:mi>O</mml:mi>
                                             <mml:mrow>
                                                <mml:mo>⋅</mml:mo>
