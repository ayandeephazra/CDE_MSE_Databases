                                             </mml:mrow>
                                             <mml:mo>′</mml:mo>
                                          </mml:msubsup>
                                       </mml:mrow>
                                       <mml:mo>]</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mrow>
                                       <mml:mo>[</mml:mo>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mi>h</mml:mi>
                                             <mml:mo>⋅</mml:mo>
                                          </mml:msup>
                                       </mml:mrow>
                                       <mml:mo>]</mml:mo>
                                    </mml:mrow>
