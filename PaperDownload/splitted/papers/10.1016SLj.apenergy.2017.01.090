                              <mml:mrow>
                                 <mml:mn>1</mml:mn>
                                 <mml:mo>-</mml:mo>
                                 <mml:mtext>x</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>Sr</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mtext>x</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>MnO</mml:mtext>
                              </mml:mrow>
