                                 <mml:mi mathvariant="normal">δ</mml:mi>
                              </mml:mrow>
                           </mml:msub>
                        </mml:mrow>
                     </mml:math>) and <mml:math altimg="si2.gif" overflow="scroll">
                        <mml:mrow>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>La</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mtext>x</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>Sr</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mn>1</mml:mn>
                                 <mml:mo>-</mml:mo>
                                 <mml:mtext>x</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>Co</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mtext>y</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
                           <mml:msub>
                              <mml:mrow>
                                 <mml:mtext>Fe</mml:mtext>
                              </mml:mrow>
                              <mml:mrow>
                                 <mml:mn>1</mml:mn>
                                 <mml:mo>-</mml:mo>
                                 <mml:mtext>y</mml:mtext>
                              </mml:mrow>
                           </mml:msub>
