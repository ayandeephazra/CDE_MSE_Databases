                                             <mml:mn>0.2</mml:mn>
                                          </mml:msub>
                                          <mml:msub>
                                             <mml:mi>Fe</mml:mi>
                                             <mml:mn>0.8</mml:mn>
                                          </mml:msub>
                                          <mml:mo>+</mml:mo>
                                          <mml:msub>
                                             <mml:mtext>6CO</mml:mtext>
                                             <mml:mn>2</mml:mn>
                                          </mml:msub>
                                          <mml:mo>+</mml:mo>
                                          <mml:mn>5</mml:mn>
                                          <mml:msub>
                                             <mml:mi>N</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:msub>
                                          <mml:mo>+</mml:mo>
                                          <mml:msub>
                                             <mml:mtext>20H</mml:mtext>
                                             <mml:mn>2</mml:mn>
                                          </mml:msub>
