            <ce:title>Evaluation of Laves-phase forming Fe–Cr alloy for SOFC interconnects in reducing atmosphere</ce:title>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Teruhisa</ce:given-name>
                  <ce:surname>Horita</ce:surname>
                  <ce:cross-ref refid="cor1">
