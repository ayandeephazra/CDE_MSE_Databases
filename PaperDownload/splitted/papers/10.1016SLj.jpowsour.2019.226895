                                       <mml:mrow>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
