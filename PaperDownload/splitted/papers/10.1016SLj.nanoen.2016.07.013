                                       <mml:mrow>
                                          <mml:mo stretchy="true">→</mml:mo>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>900</mml:mn>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mo>°</mml:mo>
