                                             <mml:mi mathvariant="normal">C</mml:mi>
                                          </mml:mrow>
                                       </mml:mrow>
                                    </mml:mover>
                                    <mml:mn>0.9</mml:mn>
                                    <mml:mrow>
                                       <mml:msub subscriptshift="65%">
                                          <mml:mrow>
                                             <mml:mi mathvariant="normal">La</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mn>0.44</mml:mn>
