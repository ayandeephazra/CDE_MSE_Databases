                  <ce:text>Thin functional layers</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0010">
                  <ce:text>Solid oxide fuel cells</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0015">
                  <ce:text>Ceria</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0020">
