                                          <mml:mi>M</mml:mi>
                                          <mml:mi>S</mml:mi>
                                       </mml:msub>
                                       <mml:mo>→</mml:mo>
                                       <mml:msub>
                                          <mml:mi>H</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>ads</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>−</mml:mo>
                                       <mml:mtext>O</mml:mtext>
                                       <mml:mo>−</mml:mo>
