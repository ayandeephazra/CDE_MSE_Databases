                           <ce:display>
                              <ce:formula id="fd11">
                                 <ce:label>(11)</ce:label>
                                 <mml:math altimg="si12.gif" overflow="scroll">
                                    <mml:mrow>
                                       <mml:msub>
                                          <mml:mi>H</mml:mi>
                                          <mml:mrow>
                                             <mml:mtext>ads</mml:mtext>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>−</mml:mo>
                                       <mml:mtext>O</mml:mtext>
                                       <mml:mo>−</mml:mo>
