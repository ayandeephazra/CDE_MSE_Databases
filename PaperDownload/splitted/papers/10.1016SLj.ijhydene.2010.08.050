                                 <ce:label>(5)</ce:label>
                                 <mml:math altimg="si5.gif" overflow="scroll">
                                    <mml:mrow>
                                       <mml:mn>2</mml:mn>
                                       <mml:msub>
                                          <mml:mi>e</mml:mi>
