            <ce:abstract class="graphical" xml:lang="en" id="ab005" view="all">
               <ce:abstract-sec id="as005" view="all">
                  <ce:section-title id="st035">Graphical abstract</ce:section-title>
                  <ce:simple-para id="sp0005" view="all">SEM and TEM images of the synthesized NiO–YSZ nanocomposite powder fired at 1000<ce:hsp sp="0.25"/>°C.<ce:display>
                        <ce:figure id="f0035">
