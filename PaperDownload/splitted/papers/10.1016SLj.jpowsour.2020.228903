                                                   <mml:mi>C</mml:mi>
                                                   <mml:mi>E</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:mfrac>
                                       <mml:mo linebreak="goodbreak">×</mml:mo>
