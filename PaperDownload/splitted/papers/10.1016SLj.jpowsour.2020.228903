                                    <mml:mrow>
                                       <mml:mi>V</mml:mi>
                                       <mml:mi>E</mml:mi>
                                       <mml:mo linebreak="badbreak">=</mml:mo>
                                       <mml:mfrac>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>η</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>E</mml:mi>
                                                   <mml:mi>E</mml:mi>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
