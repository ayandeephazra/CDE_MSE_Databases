                                                <mml:mi mathvariant="italic">x</mml:mi>
                                             </mml:mrow>
                                          </mml:mrow>
                                       </mml:mrow>
                                    </mml:msqrt>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd2">
                              <ce:label>(2)</ce:label>
                              <mml:math altimg="si2.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mi mathvariant="italic">R</mml:mi>
                                       <mml:mtext>a</mml:mtext>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mn>1</mml:mn>
                                       <mml:mi mathvariant="italic">L</mml:mi>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:munderover>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mi mathvariant="italic">L</mml:mi>
                                       </mml:munderover>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>|</mml:mo>
                                             <mml:mrow>
                                                <mml:mi mathvariant="italic">z</mml:mi>
                                                <mml:mrow>
