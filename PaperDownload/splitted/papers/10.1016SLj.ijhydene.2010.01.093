                                                      <mml:mo>(</mml:mo>
                                                      <mml:mi mathvariant="italic">x</mml:mi>
                                                      <mml:mo>)</mml:mo>
                                                   </mml:mrow>
                                                </mml:mrow>
                                             </mml:mrow>
                                             <mml:mo>|</mml:mo>
                                          </mml:mrow>
                                          <mml:mo>ⅆ</mml:mo>
                                          <mml:mi mathvariant="italic">x</mml:mi>
