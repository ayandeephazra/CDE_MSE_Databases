                  <ce:surname>Bing</ce:surname>
                  <ce:cross-ref refid="aff1">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Guangyuan</ce:given-name>
                  <ce:surname>Xie</ce:surname>
                  <ce:cross-ref refid="aff2">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
               </ce:author>
