                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mrow>
                                       <mml:mo stretchy="true">(</mml:mo>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:mtext>M</mml:mtext>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:msub>
                                             <mml:mi>V</mml:mi>
                                             <mml:msup>
                                                <mml:mtext>M</mml:mtext>
                                                <mml:mrow>
