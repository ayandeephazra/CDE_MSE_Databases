                                       <mml:mrow>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:msup>
                                             <mml:mi>p</mml:mi>
                                             <mml:mtext>e</mml:mtext>
                                          </mml:msup>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:mo>Δ</mml:mo>
                                          <mml:msup>
