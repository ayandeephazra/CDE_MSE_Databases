                              </mml:math>
                           </ce:formula>
                        </ce:display>where <mml:math altimg="si8.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>V</mml:mi>
                                 <mml:mtext>M</mml:mtext>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> and <mml:math altimg="si9.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mi>V</mml:mi>
                                 <mml:msup>
                                    <mml:mtext>M</mml:mtext>
