                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mrow>
                                                <mml:mrow>
                                                   <mml:mo>Δ</mml:mo>
                                                   <mml:mi>m</mml:mi>
