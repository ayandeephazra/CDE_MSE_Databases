                              <mml:math altimg="si1.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mi>X</mml:mi>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mi>k</mml:mi>
                                       <mml:mi>p</mml:mi>
                                    </mml:msub>
                                    <mml:mi>t</mml:mi>
                                    <mml:mo>+</mml:mo>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd2">
                              <ce:label>(2)</ce:label>
                              <mml:math altimg="si2.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mi>X</mml:mi>
                                       <mml:mn>2</mml:mn>
                                    </mml:msup>
