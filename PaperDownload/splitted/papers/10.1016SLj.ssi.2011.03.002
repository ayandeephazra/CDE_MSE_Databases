               </ce:author>
               <ce:author>
                  <ce:given-name>Jie-Wei</ce:given-name>
                  <ce:surname>Yin</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>Ming-Wen</ce:given-name>
                  <ce:surname>Xiong</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>Zi-Feng</ce:given-name>
                  <ce:surname>Ma</ce:surname>
                  <ce:cross-ref refid="cr0005">
