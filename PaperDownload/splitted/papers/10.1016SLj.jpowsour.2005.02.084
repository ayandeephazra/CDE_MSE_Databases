               <ce:keyword>
                  <ce:text>ASR</ce:text>
                  <ce:keyword>
                     <ce:text>area specific resistance</ce:text>
                  </ce:keyword>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>EIS</ce:text>
                  <ce:keyword>
                     <ce:text>electrochemical impedance spectroscopy</ce:text>
                  </ce:keyword>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>FSS</ce:text>
                  <ce:keyword>
                     <ce:text>ferritic stainless steel</ce:text>
                  </ce:keyword>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>IT-SOFCs</ce:text>
                  <ce:keyword>
                     <ce:text>intermediate temperature solid oxide fuel cells</ce:text>
                  </ce:keyword>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>TEC</ce:text>
                  <ce:keyword>
                     <ce:text>thermal expansion coefficient</ce:text>
                  </ce:keyword>
               </ce:keyword>
            </ce:keywords>
            <ce:keywords class="keyword">
               <ce:section-title>Keywords</ce:section-title>
               <ce:keyword>
                  <ce:text>SOFC</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Ferritic stainless steel</ce:text>
               </ce:keyword>
