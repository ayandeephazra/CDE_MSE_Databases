                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                              <mml:mn>7.02</mml:mn>
                              <mml:mo linebreak="goodbreak" linebreakstyle="after">×</mml:mo>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mn>10</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>6</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, <mml:math altimg="si38.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                              <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                              <mml:mn>6.79</mml:mn>
                              <mml:mo linebreak="goodbreak" linebreakstyle="after">×</mml:mo>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mn>10</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>−</mml:mo>
                                    <mml:mn>6</mml:mn>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math> and <mml:math altimg="si39.svg">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
