                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi mathvariant="bold-italic">O</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="bold-italic">I</mml:mi>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="bold-italic">P</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>0</mml:mn>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo stretchy="false">)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
