                              <mml:mn>5.41</mml:mn>
                              <mml:mo linebreak="goodbreak" linebreakstyle="after">×</mml:mo>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mn>10</mml:mn>
                                 </mml:mrow>
