                                             <mml:mo>'</mml:mo>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd6">
                              <ce:label>(6)</ce:label>
                              <mml:math altimg="si20.svg" alttext="Equation 6.">
                                 <mml:mrow>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">r</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">tot</mml:mi>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:mo linebreak="badbreak">=</mml:mo>
                                          <mml:msubsup>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">r</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>0</mml:mn>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mo>'</mml:mo>
                                                <mml:mo>'</mml:mo>
                                             </mml:mrow>
                                          </mml:msubsup>
                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:msubsup>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="bold-italic">P</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:msub>
                                                      <mml:mrow>
                                                         <mml:mi mathvariant="bold-italic">O</mml:mi>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:mn>2</mml:mn>
                                                      </mml:mrow>
                                                   </mml:msub>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="bold-italic">II</mml:mi>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:mo linebreak="badbreak">/</mml:mo>
                                             <mml:msub>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="bold-italic">P</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mn>0</mml:mn>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo stretchy="false">)</mml:mo>
                                          </mml:mrow>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>−</mml:mo>
                                          <mml:mn>0.5</mml:mn>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo linebreak="goodbreak">+</mml:mo>
                                    <mml:mrow>
                                       <mml:mo stretchy="false">(</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi mathvariant="bold-italic">r</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi mathvariant="bold-italic">b</mml:mi>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mo linebreak="badbreak">+</mml:mo>
                                       <mml:msup>
                                          <mml:mrow>
                                             <mml:mi mathvariant="bold-italic">r</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mo>'</mml:mo>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mo stretchy="false">)</mml:mo>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0070" view="all">
                        <ce:cross-ref refid="fig3" id="crosref0200">Fig. 3</ce:cross-ref>
                        <ce:float-anchor refid="fig3"/> shows the oxygen permeation flux under air/He gradient to illustrate the effect of the surface modification for the zirconia-based membrane with the coating layer on surface and without the NNO coating layer. The flux of the coated membrane on both sides corresponds to approximately 1.6 mL cm<ce:sup loc="post">−2</ce:sup>·min<ce:sup loc="post">−1</ce:sup> at 900 °C. Conversely, the oxygen permeation flux of the bare membrane corresponds to 0.05 mL cm<ce:sup loc="post">−2</ce:sup>·min<ce:sup loc="post">−1</ce:sup> at 900 °C. Thus, it is concluded that the permeation flux of membrane is significantly enhanced by the NNO coating layer. Additionally, the oxygen fluxes of membrane with coating layer on only feed side or permeate side are measured to investigate the mechanism of enhanced oxygen permeability. The oxygen permeation flux increases when the membrane surface is coated on the permeate side. The results demonstrate that the oxygen permeation flux is more affected by the oxygen excorporation reaction at the permeate side than the incorporation reaction at the feed side in the coated membrane.</ce:para>
                     <ce:para id="p0075" view="all">In order to investigate more precise surface exchange kinetics of the coated composite membrane, the oxygen permeation fluxes are investigated from a resistance view point. The permeation resistance is obtained from oxygen fluxes that are measured as a function of <mml:math altimg="si1.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> and temperature. <ce:cross-ref refid="fig4" id="crosref0205">Fig. 4</ce:cross-ref>
                        <ce:float-anchor refid="fig4"/>a and b shows the permeation resistances of the feed and permeation sides as a function of <mml:math altimg="si1.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> at 800 °C, 850 °C and 900 °C. The <mml:math altimg="si1.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> of a side is maintained as constant while the other side of <mml:math altimg="si1.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> is controlled to evaluate <mml:math altimg="si17.svg">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>0</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math>, <mml:math altimg="si10.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, and <mml:math altimg="si18.svg">
                           <mml:mrow>
                              <mml:msubsup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mn>0</mml:mn>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msubsup>
                           </mml:mrow>
                        </mml:math>. The total permeation resistance is calculated from the oxygen permeation fluxes by using Equation <ce:cross-ref refid="fd1" id="crosref0210">(1)</ce:cross-ref> and corresponds to the sum of <mml:math altimg="si11.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, <mml:math altimg="si10.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, and <mml:math altimg="si12.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>. The surface resistances of the feed and permeate sides are given in Equations <ce:cross-refs refid="fd3 fd4" id="crosrefs0045">(3) and (4)</ce:cross-refs>, respectively. Based on extant studies, the value of n changes based on the R.D.S of the surface exchange reaction [<ce:cross-refs refid="bib36 bib37 bib38" id="crosrefs0050">36–38</ce:cross-refs>].</ce:para>
                     <ce:para id="p0080" view="all">The total permeation resistance is well fitted by a power law of <mml:math altimg="si1.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>P</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>2</mml:mn>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math>, wherein n = 0.5 as shown in <ce:cross-ref refid="fig4" id="crosref0215">Fig. 4</ce:cross-ref>a and b; thus, it is concluded that the adsorbed oxygen atoms are mainly related to the R.D.S of the surface reaction. The y-intercept denotes the sum of the <mml:math altimg="si10.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, and interface resistance of the other side. Based on Equations <ce:cross-refs refid="fd5 fd6" id="crosrefs0055">(5) and (6)</ce:cross-refs>, the three resistances, namely <mml:math altimg="si11.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, <mml:math altimg="si10.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>b</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, and <mml:math altimg="si12.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mo>'</mml:mo>
                                    <mml:mo>'</mml:mo>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math>, are calculated by linear regression.</ce:para>
                     <ce:para id="p0085" view="all">The kinetic parameters of the membrane coated with the active layer is calculated from permeation resistances as follows:<ce:display>
                           <ce:formula id="fd7">
                              <ce:label>(7)</ce:label>
                              <mml:math altimg="si21.svg" alttext="Equation 7.">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">k</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">ex</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">RT</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>4</mml:mn>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">F</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msup>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">C</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">O</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mn>1</mml:mn>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msup>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">r</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mi mathvariant="bold-italic">int</mml:mi>
                                             </mml:mrow>
                                          </mml:msup>
                                       </mml:mrow>
                                    </mml:mfrac>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <mml:math altimg="si22.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>k</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>e</mml:mi>
                                    <mml:mi>x</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> denotes the surface exchange coefficient, <mml:math altimg="si23.svg">
                           <mml:mrow>
                              <mml:msub>
                                 <mml:mrow>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>O</mml:mi>
                                 </mml:mrow>
                              </mml:msub>
                           </mml:mrow>
                        </mml:math> denotes the lattice oxygen concentration, <mml:math altimg="si24.svg">
                           <mml:mrow>
                              <mml:msup>
                                 <mml:mrow>
                                    <mml:mi>r</mml:mi>
                                 </mml:mrow>
                                 <mml:mrow>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>n</mml:mi>
                                    <mml:mi>t</mml:mi>
                                 </mml:mrow>
                              </mml:msup>
                           </mml:mrow>
                        </mml:math> denotes the interface resistance of the feed or permeate side, R denotes the gas constant, T denotes the temperature, and F denotes Faraday constant. The lattice oxygen concentration of the LSM30 membrane is calculated by Equation <ce:cross-ref refid="fd8" id="crosref0220">(8)</ce:cross-ref>.<ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si25.svg" alttext="Equation 8.">
                                 <mml:mrow>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">C</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>0</mml:mn>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">LSM</mml:mi>
                                          <mml:mn>30</mml:mn>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="badbreak">=</mml:mo>
                                    <mml:mn>0.7</mml:mn>
                                    <mml:mo linebreak="goodbreak">×</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">C</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">ScSZ</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak">+</mml:mo>
                                    <mml:mn>0.3</mml:mn>
                                    <mml:mo linebreak="goodbreak">×</mml:mo>
                                    <mml:msubsup>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">C</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">O</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">LSM</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                     </ce:para>
                     <ce:para id="p0090" view="all">The D values of the membrane are also calculated by the following equation originated from the Nernst -Einstein relationship:<ce:display>
                           <ce:formula id="fd9">
                              <ce:label>(9)</ce:label>
                              <mml:math altimg="si26.svg" alttext="Equation 9.">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">D</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">S</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo linebreak="badbreak">=</mml:mo>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi mathvariant="bold-italic">D</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>∗</mml:mo>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:mfrac>
