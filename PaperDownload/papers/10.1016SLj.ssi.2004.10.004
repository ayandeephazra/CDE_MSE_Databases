                                          <mml:msub>
                                             <mml:mi>ρ</mml:mi>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mtext>Cr</mml:mtext>
                                                   <mml:mn>2</mml:mn>
                                                </mml:msub>
                                                <mml:msub>
                                                   <mml:mi mathvariant="normal">O</mml:mi>
                                                   <mml:mn>3</mml:mn>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:mo stretchy="true">)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>√</mml:mo>
                                 <mml:msub>
                                    <mml:mi>k</mml:mi>
                                    <mml:mi mathvariant="normal">g</mml:mi>
                                 </mml:msub>
