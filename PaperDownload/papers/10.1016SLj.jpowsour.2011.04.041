                                 <mml:msub>
                                    <mml:mtext>NiO</mml:mtext>
                                    <mml:mn>4</mml:mn>
                                 </mml:msub>
                              </mml:mrow>
                           </mml:msub>
                           <mml:mo>=</mml:mo>
                           <mml:mn>1</mml:mn>
                           <mml:mtext>   </mml:mtext>
                           <mml:mi>Ω</mml:mi>
                           <mml:mtext>   </mml:mtext>
                           <mml:mtext>c</mml:mtext>
