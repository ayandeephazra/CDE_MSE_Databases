                                                <mml:mn>2</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mo linebreak="badbreak">+</mml:mo>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
