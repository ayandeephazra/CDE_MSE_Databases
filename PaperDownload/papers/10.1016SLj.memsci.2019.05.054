                        <ce:display>
                           <ce:formula id="fd2">
                              <ce:label>(2)</ce:label>
                              <mml:math altimg="si2.svg" alttext="Equation 2.">
                                 <mml:mrow>
                                    <mml:mi>S</mml:mi>
                                    <mml:mi>o</mml:mi>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mi>C</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mtext>PE</mml:mtext>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mrow>
                                                <mml:mi>c</mml:mi>
                                             </mml:mrow>
                                             <mml:mrow>
                                                <mml:mtext>V</mml:mtext>
                                                <mml:mn>5</mml:mn>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mrow>
