                                       <mml:mn>5.0</mml:mn>
                                       <mml:msup>
                                          <mml:mtext>E</mml:mtext>
                                          <mml:mrow>
                                             <mml:mo>−</mml:mo>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msup>
                                       <mml:mi> </mml:mi>
