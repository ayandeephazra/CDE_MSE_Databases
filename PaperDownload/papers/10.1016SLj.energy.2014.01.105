                  <ce:text>DCFC (direct carbon fuel cell)</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0020">
                  <ce:text>Tubular fuel cell</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0025">
                  <ce:text>Carbon bed</ce:text>
               </ce:keyword>
               <ce:keyword id="kwrd0030">
                  <ce:text>MIEC (Mixed ion electronic conducting) anode</ce:text>
               </ce:keyword>
