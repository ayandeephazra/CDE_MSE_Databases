         <head>
            <ce:title>Electrochemical performance of LSM–SDC electrodes prepared with ion-impregnated LSM</ce:title>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Xingyan</ce:given-name>
                  <ce:surname>Xu</ce:surname>
                  <ce:cross-ref refid="aff1">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Chuanbao</ce:given-name>
                  <ce:surname>Cao</ce:surname>
                  <ce:cross-ref refid="aff1">
                     <ce:sup loc="post">a</ce:sup>
                  </ce:cross-ref>
                  <ce:cross-ref refid="cor1">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:author>
                  <ce:given-name>Changrong</ce:given-name>
                  <ce:surname>Xia</ce:surname>
                  <ce:cross-ref refid="aff2">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Dingkun</ce:given-name>
                  <ce:surname>Peng</ce:surname>
                  <ce:cross-ref refid="aff2">
                     <ce:sup loc="post">b</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:affiliation id="aff1">
                  <ce:label>a</ce:label>
