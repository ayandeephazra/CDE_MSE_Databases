                  <ce:text>Cermet</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Fuel cells</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Anode</ce:text>
               </ce:keyword>
