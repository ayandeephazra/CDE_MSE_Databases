                                       <mml:mi>t</mml:mi>
                                       <mml:mrow>
                                          <mml:mtext>SOEC</mml:mtext>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd9">
                              <ce:label>(9)</ce:label>
                              <mml:math altimg="si9.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mover accent="true">
                                          <mml:mi>m</mml:mi>
                                          <mml:mo>˙</mml:mo>
                                       </mml:mover>
                                       <mml:mrow>
                                          <mml:mtext>SOFC</mml:mtext>
