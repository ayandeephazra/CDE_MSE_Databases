                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>t</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>SOEC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
