                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>t</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>SOFC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>m</mml:mi>
                                                <mml:mo>˙</mml:mo>
                                             </mml:mover>
                                             <mml:mrow>
                                                <mml:mtext>SOFC</mml:mtext>
                                                <mml:mo>,</mml:mo>
                                                <mml:mtext>in</mml:mtext>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mi>t</mml:mi>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>t</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>SOEC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>m</mml:mi>
                                                <mml:mo>˙</mml:mo>
                                             </mml:mover>
                                             <mml:mrow>
                                                <mml:mtext>SOEC</mml:mtext>
                                                <mml:mo>,</mml:mo>
                                                <mml:mtext>out</mml:mtext>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mi>t</mml:mi>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd6">
                              <ce:label>(6)</ce:label>
                              <mml:math altimg="si6.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>t</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>SOFC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mover accent="true">
                                                <mml:mi>m</mml:mi>
                                                <mml:mo>˙</mml:mo>
                                             </mml:mover>
                                             <mml:mrow>
                                                <mml:mtext>SOFC</mml:mtext>
                                                <mml:mo>,</mml:mo>
                                                <mml:mtext>out</mml:mtext>
                                             </mml:mrow>
