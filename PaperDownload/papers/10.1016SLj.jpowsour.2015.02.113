                                                   <mml:mtext>SOEC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>j</mml:mi>
                                             <mml:mrow>
                                                <mml:mtext>SOEC</mml:mtext>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mi>t</mml:mi>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mo>−</mml:mo>
                                    <mml:mrow>
                                       <mml:msubsup>
                                          <mml:mo>∫</mml:mo>
                                          <mml:mn>0</mml:mn>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>t</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>SOFC</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                          </mml:mrow>
                                       </mml:msubsup>
                                       <mml:mrow>
                                          <mml:msub>
                                             <mml:mi>j</mml:mi>
                                             <mml:mrow>
                                                <mml:mtext>SOFC</mml:mtext>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mi>t</mml:mi>
                                             <mml:mo>)</mml:mo>
                                          </mml:mrow>
                                          <mml:mi>d</mml:mi>
                                          <mml:mi>t</mml:mi>
                                       </mml:mrow>
                                    </mml:mrow>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>where <ce:italic>j</ce:italic>
                        <ce:inf loc="post">SOFC</ce:inf> and <ce:italic>j</ce:italic>
                        <ce:inf loc="post">SOEC</ce:inf> are the operating current densities in each mode. For steady state simulation, as in this study, Eqs. <ce:cross-refs refid="fd5 fd6 fd7" id="crosrefs0055">(5)–(7)</ce:cross-refs> simplify to:<ce:display>
                           <ce:formula id="fd8">
                              <ce:label>(8)</ce:label>
                              <mml:math altimg="si8.gif" overflow="scroll">
                                 <mml:mrow>
