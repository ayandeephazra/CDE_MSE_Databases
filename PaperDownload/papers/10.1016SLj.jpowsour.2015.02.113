                                                <mml:mo>˙</mml:mo>
                                             </mml:mover>
                                             <mml:mrow>
                                                <mml:mtext>SOEC</mml:mtext>
                                                <mml:mo>,</mml:mo>
                                                <mml:mtext>in</mml:mtext>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mrow>
                                             <mml:mo>(</mml:mo>
                                             <mml:mi>t</mml:mi>
                                             <mml:mo>)</mml:mo>
