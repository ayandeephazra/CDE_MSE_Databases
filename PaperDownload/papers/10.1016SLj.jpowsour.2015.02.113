                                          <mml:mo>˙</mml:mo>
                                       </mml:mover>
                                       <mml:mrow>
                                          <mml:mtext>SOFC</mml:mtext>
                                          <mml:mo>,</mml:mo>
                                          <mml:mtext>in</mml:mtext>
                                       </mml:mrow>
