                     <row valign="top">
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="center">SOFC unit (SOFC)<br/>
                           <inline-figure baseline="0.0">
                              <link id="lk00050" locator="fx2" xlink:href="pii:S0306261920309648/fx2" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:type="simple"/>
                           </inline-figure>
