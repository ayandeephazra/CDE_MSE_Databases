                                          <mml:mi mathvariant="normal">Φ</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>H</mml:mi>
                                             <mml:mn>2</mml:mn>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo>+</mml:mo>
