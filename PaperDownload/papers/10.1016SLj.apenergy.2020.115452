                                    </mml:msub>
                                    <mml:mfenced close="]" open="[">
                                       <mml:mrow>
                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:msubsup>
                                                <mml:mi>h</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>k</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="italic">IS</mml:mi>
                                                </mml:mrow>
                                             </mml:msubsup>
                                             <mml:mo>-</mml:mo>
                                             <mml:msubsup>
                                                <mml:mi>h</mml:mi>
                                                <mml:mrow>
                                                   <mml:mi>k</mml:mi>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mi mathvariant="italic">ES</mml:mi>
