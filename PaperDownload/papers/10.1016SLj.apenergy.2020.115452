                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi mathvariant="italic">perm</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:mo linebreak="goodbreak" linebreakstyle="after">=</mml:mo>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>N</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>m</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mi>N</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi>t</mml:mi>
                                          </mml:mrow>
                                       </mml:msub>
                                       <mml:msub>
                                          <mml:mrow>
                                             <mml:mo>∙</mml:mo>
                                             <mml:mi>A</mml:mi>
                                          </mml:mrow>
                                          <mml:mrow>
                                             <mml:mi mathvariant="italic">perm</mml:mi>
                                             <mml:mo>,</mml:mo>
