                                          <mml:mi mathvariant="italic">ES</mml:mi>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo linebreak="goodbreak">=</mml:mo>
                                    <mml:munder>
                                       <mml:mo>∑</mml:mo>
                                       <mml:mi>k</mml:mi>
                                    </mml:munder>
