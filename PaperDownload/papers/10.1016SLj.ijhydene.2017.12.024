                        <ce:figure id="undfig1">
                           <ce:link locator="fx1" xlink:type="simple" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:href="pii:S0360319917346608/fx1"/>
                        </ce:figure>
