                  <ce:text>Solid oxide fuel cell</ce:text>
               </ce:keyword>
               <ce:keyword id="key0010">
                  <ce:text>Metallic interconnect</ce:text>
               </ce:keyword>
               <ce:keyword id="key0015">
                  <ce:text>Electroplated Ni-Co-Cu coating</ce:text>
               </ce:keyword>
