               <ce:keyword id="kwrd0035">
                  <ce:text>LaNi<ce:inf loc="post">0.6</ce:inf>Co<ce:inf loc="post">0.4</ce:inf>O<ce:inf loc="post">3−<ce:italic>δ</ce:italic>
