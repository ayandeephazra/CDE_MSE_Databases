                                                <mml:mi>P</mml:mi>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>d</mml:mi>
                                                <mml:mi>u</mml:mi>
                                                <mml:mi>c</mml:mi>
                                                <mml:mi>e</mml:mi>
                                                <mml:mi>d</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="fd20">
                              <ce:label>(20)</ce:label>
                              <mml:math altimg="si25.gif" overflow="scroll">
                                 <mml:mrow>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>n</mml:mi>
