                                                <mml:mi>t</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>n</mml:mi>
                                             <mml:mo>˙</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>H</mml:mi>
                                          <mml:msub>
                                             <mml:mn>2</mml:mn>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
                                                <mml:mi>n</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>+</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>n</mml:mi>
                                             <mml:mo>˙</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>H</mml:mi>
                                          <mml:msub>
                                             <mml:mn>2</mml:mn>
