                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mo>=</mml:mo>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mover accent="true">
                                             <mml:mi>n</mml:mi>
                                             <mml:mo>˙</mml:mo>
                                          </mml:mover>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>C</mml:mi>
                                          <mml:mi>a</mml:mi>
                                          <mml:mi>t</mml:mi>
                                          <mml:mi>h</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>d</mml:mi>
                                          <mml:msub>
                                             <mml:mi>e</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>i</mml:mi>
