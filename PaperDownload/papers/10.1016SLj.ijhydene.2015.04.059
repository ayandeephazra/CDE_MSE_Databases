                           <ce:display>
                              <ce:formula id="fd39">
                                 <ce:label>(39)</ce:label>
                                 <mml:math altimg="si44.gif" overflow="scroll">
                                    <mml:mrow>
                                       <mml:mi>A</mml:mi>
                                       <mml:mi>r</mml:mi>
                                       <mml:mi>e</mml:mi>
                                       <mml:msub>
                                          <mml:mi>a</mml:mi>
                                          <mml:mrow>
                                             <mml:mi>H</mml:mi>
