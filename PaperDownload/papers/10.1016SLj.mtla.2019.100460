                           </ce:formula>
                        </ce:display>
                        <ce:display>
                           <ce:formula id="eqn0003">
                              <ce:label>(3)</ce:label>
                              <mml:math altimg="si4.svg">
                                 <mml:mrow>
                                    <mml:mn>2</mml:mn>
                                    <mml:mtext>MO</mml:mtext>
