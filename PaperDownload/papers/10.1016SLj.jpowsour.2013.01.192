                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>→</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msub>
                                       <mml:mrow>
                                          <mml:mtext>CrO</mml:mtext>
