                                       <mml:mo>(</mml:mo>
                                       <mml:mtext>g</mml:mtext>
                                       <mml:mo>)</mml:mo>
                                    </mml:mrow>
                                    <mml:mo>+</mml:mo>
                                    <mml:mn>3</mml:mn>
                                    <mml:msub>
                                       <mml:mtext>H</mml:mtext>
