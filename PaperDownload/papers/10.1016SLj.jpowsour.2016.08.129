                                                <mml:mi>q</mml:mi>
                                                <mml:mo>,</mml:mo>
                                                <mml:mi>r</mml:mi>
                                                <mml:mi>t</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mi>F</mml:mi>
                                          <mml:msub>
                                             <mml:mi>Îµ</mml:mi>
                                             <mml:mrow>
                                                <mml:mi>v</mml:mi>
