                                                         <mml:mtext>CO</mml:mtext>
                                                      </mml:mrow>
                                                      <mml:mo>]</mml:mo>
                                                   </mml:mrow>
                                                   <mml:mrow>
                                                      <mml:mo>[</mml:mo>
                                                      <mml:mrow>
