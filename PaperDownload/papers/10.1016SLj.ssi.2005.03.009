            <ce:title>Ferrite-based perovskites as cathode materials for anode-supported solid oxide fuel cells</ce:title>
            <ce:subtitle>Part I. Variation of composition</ce:subtitle>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Andreas</ce:given-name>
                  <ce:surname>Mai</ce:surname>
               </ce:author>
