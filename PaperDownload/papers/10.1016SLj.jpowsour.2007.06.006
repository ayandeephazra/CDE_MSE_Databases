                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char="±">750<hsp sp="0.25"/>±<hsp sp="0.25"/>21</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char="±">834<hsp sp="0.25"/>±<hsp sp="0.25"/>22</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char="±">804<hsp sp="0.25"/>±<hsp sp="0.25"/>17</entry>
                        <entry xmlns="http://www.elsevier.com/xml/common/dtd" align="char" char="±">760<hsp sp="0.25"/>±<hsp sp="0.25"/>19</entry>
                     </row>
                  </tbody>
               </tgroup>
            </ce:table>
