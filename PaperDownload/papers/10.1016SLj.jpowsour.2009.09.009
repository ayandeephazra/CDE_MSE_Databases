         <head>
            <ce:dochead>
               <ce:textfn>Review</ce:textfn>
            </ce:dochead>
            <ce:title>Performance of anode-supported solid oxide fuel cell using novel ceria electrolyte</ce:title>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Jin Soo</ce:given-name>
                  <ce:surname>Ahn</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>Shobit</ce:given-name>
                  <ce:surname>Omar</ce:surname>
                  <ce:cross-ref refid="fn1">
                     <ce:sup loc="post">1</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Heesung</ce:given-name>
                  <ce:surname>Yoon</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>Juan C.</ce:given-name>
                  <ce:surname>Nino</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>Eric D.</ce:given-name>
                  <ce:surname>Wachsman</ce:surname>
                  <ce:cross-ref refid="cor1">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
