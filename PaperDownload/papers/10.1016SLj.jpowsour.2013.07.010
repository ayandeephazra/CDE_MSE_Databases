                                    <mml:mrow>
                                       <mml:mtext>Ti</mml:mtext>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mtext>Ti</mml:mtext>
                                    </mml:mrow>
                                    <mml:mo>′</mml:mo>
                                 </mml:msubsup>
                                 <mml:mo>+</mml:mo>
                                 <mml:msubsup>
