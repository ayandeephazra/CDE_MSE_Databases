                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:msubsup>
                                                <mml:mrow>
                                                   <mml:mtext>Ti</mml:mtext>
                                                </mml:mrow>
                                                <mml:mrow>
                                                   <mml:mtext>Ti</mml:mtext>
                                                </mml:mrow>
                                                <mml:mtext>X</mml:mtext>
                                             </mml:msubsup>
                                          </mml:mrow>
                                          <mml:mo>)</mml:mo>
                                       </mml:mrow>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mn>0.6</mml:mn>
                                    </mml:mrow>
                                 </mml:msub>
                                 <mml:msub>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:msubsup>
                                                <mml:mtext>O</mml:mtext>
                                                <mml:mtext>O</mml:mtext>
                                                <mml:mrow>
                                                   <mml:mtext>
                                                      <ce:glyph name="rad"/>
                                                   </mml:mtext>
