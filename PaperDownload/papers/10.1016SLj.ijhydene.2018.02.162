                                          <mml:mo>″″</mml:mo>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>+</mml:mo>
                                    <mml:mi>T</mml:mi>
                                    <mml:msubsup>
                                       <mml:mi>i</mml:mi>
                                       <mml:mi>i</mml:mi>
