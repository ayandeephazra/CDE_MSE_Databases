               <ce:correspondence id="cor1">
                  <ce:label>∗</ce:label>
                  <ce:text>
                     <ce:italic>Corresponding author</ce:italic>. Tel.: +86 41184708380; fax: +86 41184708389.</ce:text>
               </ce:correspondence>
