                                       <mml:mrow>
                                          <mml:mo>[</mml:mo>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>rev</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo>+</mml:mo>
                                             <mml:mi>s</mml:mi>
                                             <mml:mspace width="0.5em"/>
