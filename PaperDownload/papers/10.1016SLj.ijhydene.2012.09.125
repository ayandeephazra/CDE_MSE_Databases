                                                                     <mml:mi>R</mml:mi>
                                                                     <mml:mrow>
                                                                        <mml:mtext>act</mml:mtext>
                                                                        <mml:mo>,</mml:mo>
                                                                        <mml:mtext>c</mml:mtext>
                                                                        <mml:mo>,</mml:mo>
                                                                        <mml:mtext>E</mml:mtext>
                                                                     </mml:mrow>
                                                                  </mml:msub>
                                                                  <mml:msub>
                                                                     <mml:mi>C</mml:mi>
                                                                     <mml:mrow>
                                                                        <mml:mtext>dl</mml:mtext>
                                                                        <mml:mo>,</mml:mo>
                                                                        <mml:mtext>c</mml:mtext>
                                                                        <mml:mo>,</mml:mo>
                                                                        <mml:mtext>E</mml:mtext>
                                                                     </mml:mrow>
                                                                  </mml:msub>
