                                    <mml:msub>
                                       <mml:mi>N</mml:mi>
                                       <mml:mtext>S</mml:mtext>
                                    </mml:msub>
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>(</mml:mo>
                                          <mml:mrow>
                                             <mml:msub>
                                                <mml:mi>V</mml:mi>
                                                <mml:mrow>
                                                   <mml:mtext>rev</mml:mtext>
                                                </mml:mrow>
                                             </mml:msub>
                                             <mml:mo>+</mml:mo>
