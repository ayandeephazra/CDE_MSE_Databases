                        </ce:formula>
                     </ce:display>
                     <ce:display>
                        <ce:formula id="fd3">
                           <ce:label>(3)</ce:label>
                           <mml:math altimg="si3.gif" overflow="scroll">
                              <mml:mrow>
                                 <mml:mn>2</mml:mn>
                                 <mml:msup>
                                    <mml:mrow>
                                       <mml:mtext>OH</mml:mtext>
                                    </mml:mrow>
                                    <mml:mo>−</mml:mo>
                                 </mml:msup>
                                 <mml:mrow>
                                    <mml:mo>(</mml:mo>
                                    <mml:mtext>aq</mml:mtext>
                                    <mml:mtext>.</mml:mtext>
                                    <mml:mo>)</mml:mo>
                                 </mml:mrow>
                                 <mml:mo>→</mml:mo>
                                 <mml:mfrac>
                                    <mml:mn>1</mml:mn>
                                    <mml:mn>2</mml:mn>
                                 </mml:mfrac>
                                 <mml:msub>
                                    <mml:mtext>O</mml:mtext>
                                    <mml:mn>2</mml:mn>
                                 </mml:msub>
                                 <mml:mrow>
