                                             <mml:msup>
                                                <mml:mi>e</mml:mi>
                                                <mml:mrow>
                                                   <mml:mfrac>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>v</mml:mi>
                                                            <mml:mrow>
                                                               <mml:mtext>act</mml:mtext>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mtext>c</mml:mtext>
                                                               <mml:mo>,</mml:mo>
                                                               <mml:mtext>E</mml:mtext>
                                                            </mml:mrow>
                                                         </mml:msub>
                                                      </mml:mrow>
                                                      <mml:mrow>
                                                         <mml:msub>
                                                            <mml:mi>N</mml:mi>
