                     <ce:hsp sp="0.12"/>
                     <ce:inf loc="post">+</ce:inf>
                     <ce:hsp sp="0.12"/>
                     <ce:inf loc="post">
                        <ce:italic>δ</ce:italic>
                     </ce:inf>
                  </ce:text>
               </ce:keyword>
