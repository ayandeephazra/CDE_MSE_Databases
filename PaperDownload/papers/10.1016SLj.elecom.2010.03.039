         <head>
            <ce:title>Composite cathode for IT-SOFC: Sr-doped lanthanum cuprate and Gd-doped ceria</ce:title>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Seung Jun</ce:given-name>
                  <ce:surname>Lee</ce:surname>
               </ce:author>
               <ce:author>
                  <ce:given-name>P.</ce:given-name>
                  <ce:surname>Muralidharan</ce:surname>
                  <ce:cross-ref refid="fn1">
                     <ce:sup loc="post">1</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Seung Hwan</ce:given-name>
                  <ce:surname>Jo</ce:surname>
                  <ce:cross-ref refid="fn1">
                     <ce:sup loc="post">1</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author>
                  <ce:given-name>Do Kyung</ce:given-name>
                  <ce:surname>Kim</ce:surname>
                  <ce:cross-ref refid="cor1">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:affiliation>
                  <ce:textfn>Department of Materials Science and Engineering, Korea Advanced Institute of Science and Technology (KAIST), 335 Gwahangno, Yuseong-gu, Daejeon 305-701, Republic of Korea</ce:textfn>
               </ce:affiliation>
               <ce:correspondence id="cor1">
                  <ce:label>⁎</ce:label>
                  <ce:text>Corresponding author. Tel.: +82 42 350 4118; fax: +82 42 350 3310.</ce:text>
               </ce:correspondence>
               <ce:footnote id="fn1">
                  <ce:label>1</ce:label>
                  <ce:note-para>Equal contribution of work.</ce:note-para>
               </ce:footnote>
