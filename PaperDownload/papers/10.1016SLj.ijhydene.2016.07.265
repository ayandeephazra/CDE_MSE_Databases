                                          <mml:mspace width="0.25em"/>
                                          <mml:msubsup>
                                             <mml:mi>P</mml:mi>
                                             <mml:msub>
                                                <mml:mi>O</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                             <mml:mrow>
                                                <mml:mi>C</mml:mi>
                                                <mml:mi>a</mml:mi>
                                                <mml:mi>t</mml:mi>
                                                <mml:mi>h</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>d</mml:mi>
                                                <mml:mi>e</mml:mi>
