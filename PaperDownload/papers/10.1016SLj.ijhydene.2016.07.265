                                       <mml:mrow>
                                          <mml:mi>r</mml:mi>
                                          <mml:msub>
                                             <mml:mi>σ</mml:mi>
                                             <mml:msup>
                                                <mml:mi>h</mml:mi>
                                                <mml:mo>•</mml:mo>
                                             </mml:msup>
                                          </mml:msub>
                                          <mml:mo>−</mml:mo>
                                          <mml:msub>
                                             <mml:mi>σ</mml:mi>
