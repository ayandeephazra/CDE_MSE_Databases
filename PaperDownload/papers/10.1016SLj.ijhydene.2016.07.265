                                 <mml:math altimg="si65.gif" overflow="scroll">
                                    <mml:msub>
                                       <mml:mi>J</mml:mi>
                                       <mml:msup>
                                          <mml:mi>h</mml:mi>
                                          <mml:mo>•</mml:mo>
                                       </mml:msup>
                                    </mml:msub>
