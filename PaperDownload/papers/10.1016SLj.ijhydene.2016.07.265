                                 </mml:math>
                              </ce:formula>
                           </ce:display>
                           <ce:display>
                              <ce:formula id="fd14">
                                 <ce:label>(14)</ce:label>
                                 <mml:math altimg="si16.gif" overflow="scroll">
                                    <mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>[</mml:mo>
