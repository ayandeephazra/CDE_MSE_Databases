                                                      <mml:mrow>
                                                         <mml:mn>6</mml:mn>
                                                         <mml:mo>−</mml:mo>
                                                         <mml:mrow>
                                                            <mml:mo>[</mml:mo>
                                                            <mml:msubsup>
                                                               <mml:mi>X</mml:mi>
                                                               <mml:mrow>
                                                                  <mml:mi>C</mml:mi>
                                                                  <mml:mi>e</mml:mi>
                                                               </mml:mrow>
                                                               <mml:mi>′</mml:mi>
                                                            </mml:msubsup>
                                                            <mml:mo>]</mml:mo>
