                                          <mml:mi>o</mml:mi>
                                          <mml:mi>n</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                 </mml:math>
                              </ce:formula>
                           </ce:display>
                           <ce:display>
                              <ce:formula id="fd49">
                                 <ce:label>(49)</ce:label>
                                 <mml:math altimg="si66.gif" overflow="scroll">
                                    <mml:msub>
                                       <mml:mi>J</mml:mi>
                                       <mml:mrow>
                                          <mml:mi>i</mml:mi>
                                          <mml:mi>o</mml:mi>
                                          <mml:mi>n</mml:mi>
                                       </mml:mrow>
                                    </mml:msub>
                                    <mml:mi>L</mml:mi>
                                    <mml:mo>=</mml:mo>
                                    <mml:mfrac>
                                       <mml:mrow>
                                          <mml:mi>R</mml:mi>
                                          <mml:mi>T</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mn>4</mml:mn>
                                          <mml:mi>F</mml:mi>
                                       </mml:mrow>
                                    </mml:mfrac>
                                    <mml:msubsup>
                                       <mml:mo>∫</mml:mo>
                                       <mml:mrow>
                                          <mml:mi>ln</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:msubsup>
                                             <mml:mi>P</mml:mi>
                                             <mml:msub>
                                                <mml:mi>O</mml:mi>
                                                <mml:mn>2</mml:mn>
                                             </mml:msub>
                                             <mml:mrow>
                                                <mml:mi>A</mml:mi>
                                                <mml:mi>n</mml:mi>
                                                <mml:mi>o</mml:mi>
                                                <mml:mi>d</mml:mi>
                                                <mml:mi>e</mml:mi>
                                             </mml:mrow>
                                          </mml:msubsup>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mi>ln</mml:mi>
                                          <mml:mspace width="0.25em"/>
                                          <mml:msubsup>
