                  <ce:text>Interconnect/cathode contact</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Reaction sintering</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Area specific resistance</ce:text>
               </ce:keyword>
