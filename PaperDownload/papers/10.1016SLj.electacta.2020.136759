         <head>
            <ce:title id="tte0002">High-performance SrFe<ce:inf loc="post">0.1</ce:inf>Mo<ce:inf loc="post">0.9</ce:inf>O<ce:inf loc="post">3-</ce:inf>
               <ce:italic>
                  <ce:inf loc="post">δ</ce:inf>
               </ce:italic> -based composites for the anode application in solid oxide fuel cells</ce:title>
            <ce:author-group id="aut0001">
               <ce:author id="au0001" author-id="S001346862031152X-e4ac2f378b50530cbc976be4c16b103b">
                  <ce:given-name>Amit</ce:given-name>
                  <ce:surname>Das</ce:surname>
               </ce:author>
               <ce:author id="au0002" author-id="S001346862031152X-a20404a4e990695ac54f350af8f09e44">
                  <ce:given-name>Sunil</ce:given-name>
                  <ce:surname>Kumar</ce:surname>
               </ce:author>
               <ce:author id="au0003" author-id="S001346862031152X-db0feb0b29c9c4ddc5c398923c1521af">
                  <ce:given-name>Sandeep</ce:given-name>
                  <ce:surname>Kumar</ce:surname>
               </ce:author>
               <ce:author id="au0004" author-id="S001346862031152X-933a7cdda48c2381b0f2467e16636f71">
                  <ce:given-name>Shobit</ce:given-name>
                  <ce:surname>Omar</ce:surname>
                  <ce:cross-ref id="crf0001" refid="cor0001">
