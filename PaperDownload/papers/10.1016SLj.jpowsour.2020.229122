                                       <mml:mn>2</mml:mn>
                                    </mml:msub>
                                    <mml:mo linebreak="goodbreak">+</mml:mo>
                                    <mml:mspace width="0.25em"/>
                                    <mml:mi>C</mml:mi>
                                 </mml:mrow>
                              </mml:math>
                           </ce:formula>
                        </ce:display>
                        <ce:display>
