                                          <mml:mi>o</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>×</mml:mo>
                                       </mml:mrow>
                                    </mml:msubsup>
                                    <mml:mo>+</mml:mo>
                                    <mml:mn>2</mml:mn>
                                    <mml:msup>
                                       <mml:mrow>
                                          <mml:mi>h</mml:mi>
                                       </mml:mrow>
                                       <mml:mrow>
                                          <mml:mo>•</mml:mo>
                                       </mml:mrow>
                                    </mml:msup>
                                    <mml:mo>=</mml:mo>
