                                       <mml:mi>z</mml:mi>
                                       <mml:mtext>d</mml:mtext>
                                       <mml:mi>y</mml:mi>
                                    </mml:mrow>
                                    <mml:mo>=</mml:mo>
                                    <mml:mo>−</mml:mo>
                                    <mml:mfenced open="(" close=")">
                                       <mml:mrow>
                                          <mml:mfrac>
                                             <mml:mi>w</mml:mi>
                                             <mml:mrow>
                                                <mml:msub>
                                                   <mml:mi>ρ</mml:mi>
                                                   <mml:mtext>S</mml:mtext>
                                                </mml:msub>
                                             </mml:mrow>
                                          </mml:mfrac>
                                       </mml:mrow>
                                    </mml:mfenced>
                                    <mml:munder>
                                       <mml:mo>∫</mml:mo>
                                       <mml:mi>y</mml:mi>
                                    </mml:munder>
