                                          <mml:mrow>
                                             <mml:mo stretchy="false">(</mml:mo>
                                             <mml:mo>%</mml:mo>
                                             <mml:msup>
                                                <mml:mi>h</mml:mi>
                                                <mml:mrow>
                                                   <mml:mo>−</mml:mo>
