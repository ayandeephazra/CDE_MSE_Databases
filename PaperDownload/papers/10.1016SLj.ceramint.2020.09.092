                                             </mml:mrow>
                                          </mml:msub>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mo linebreak="badbreak">−</mml:mo>
                                          <mml:mspace width="0.25em"/>
                                          <mml:mi>A</mml:mi>
                                          <mml:mi>S</mml:mi>
                                          <mml:msub>
                                             <mml:mi>R</mml:mi>
                                             <mml:mrow>
                                                <mml:mn>0</mml:mn>
                                                <mml:mi>h</mml:mi>
                                             </mml:mrow>
                                          </mml:msub>
                                       </mml:mrow>
