                  <ce:text>Thin-film-processed cathode</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Solid-oxide fuel cell</ce:text>
               </ce:keyword>
               <ce:keyword>
                  <ce:text>Lateral conduction loss</ce:text>
               </ce:keyword>
               <ce:keyword>
