                                 <mml:msubsup>
                                    <mml:mrow>
                                       <mml:mi mathvariant="normal">O</mml:mi>
                                    </mml:mrow>
                                    <mml:mrow>
                                       <mml:mi mathvariant="normal">O</mml:mi>
                                    </mml:mrow>
