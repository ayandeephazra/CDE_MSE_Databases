         <head>
            <ce:title>Improving La<ce:inf loc="post">0.6</ce:inf>Sr<ce:inf loc="post">0.4</ce:inf>Co<ce:inf loc="post">0.2</ce:inf>Fe<ce:inf loc="post">0.8</ce:inf>O<ce:inf loc="post">3<ce:hsp sp="0.12"/>−<ce:hsp sp="0.12"/>
                  <ce:italic>δ</ce:italic>
               </ce:inf> cathode performance by infiltration of a Sm<ce:inf loc="post">0.5</ce:inf>Sr<ce:inf loc="post">0.5</ce:inf>CoO<ce:inf loc="post">3<ce:hsp sp="0.12"/>−<ce:hsp sp="0.12"/>
                  <ce:italic>δ</ce:italic>
               </ce:inf> coating</ce:title>
            <ce:author-group>
               <ce:author>
                  <ce:given-name>Xiaoyuan</ce:given-name>
