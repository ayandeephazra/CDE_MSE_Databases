               <ce:author orcid="0000-0001-5780-1966" id="au0030" author-id="S0167273818301267-dba61e27cce1d366493b6e23935cefea">
                  <ce:given-name>Tayfur</ce:given-name>
                  <ce:surname>Ozturk</ce:surname>
                  <ce:cross-ref refid="cr0005" id="cf0005">
                     <ce:sup loc="post">⁎</ce:sup>
                  </ce:cross-ref>
                  
               </ce:author>
               <ce:affiliation id="af0005" affiliation-id="S0167273818301267-5e70117857a1a34917e60133c4bfb667">
