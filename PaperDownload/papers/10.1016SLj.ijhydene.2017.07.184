               </ce:caption>
               <ce:alt-text role="short" id="alttext0030">Fig. 4</ce:alt-text>
               <ce:link locator="gr4" xlink:type="simple" xlink:role="http://data.elsevier.com/vocabulary/ElsevierContentTypes/23.4" xlink:href="pii:S0360319917330203/gr4"/>
