            <ce:appendices view="all">
               <ce:section id="appsec1" view="extended">
                  <ce:label>Appendix A</ce:label>
                  <ce:section-title id="sectitle0125">Supplementary data</ce:section-title>
                  <ce:para id="p0190" view="all">The following is the Supplementary data to this article:<ce:display>
                        <ce:e-component id="mmc1">
