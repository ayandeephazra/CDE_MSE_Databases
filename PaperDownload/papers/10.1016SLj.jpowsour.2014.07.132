            <ce:keywords xml:lang="en" id="kwrds0010" view="all" class="keyword">
               <ce:section-title id="sectitle0025">Keywords</ce:section-title>
               <ce:keyword id="kwrd0010">
                  <ce:text>La<ce:inf loc="post">0.4</ce:inf>Sr<ce:inf loc="post">0.6</ce:inf>Co<ce:inf loc="post">0.8</ce:inf>Fe<ce:inf loc="post">0.2</ce:inf>O<ce:inf loc="post">3</ce:inf>
