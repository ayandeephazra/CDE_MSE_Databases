                        <ce:figure id="undfig1">
                           <ce:link locator="fx1"/>
                        </ce:figure>
                     </ce:display>
                  </ce:simple-para>
               </ce:abstract-sec>
            </ce:abstract>
            
            <ce:keywords xml:lang="en" id="kwrds0010" view="all" class="keyword">
               <ce:section-title id="sectitle0025">Keywords</ce:section-title>
