               </ce:keyword>
               <ce:keyword id="kw0010">
                  <ce:text>Inorganic–organic composite membrane</ce:text>
               </ce:keyword>
               <ce:keyword id="kw0015">
                  <ce:text>High temperature and low humidity</ce:text>
               </ce:keyword>
