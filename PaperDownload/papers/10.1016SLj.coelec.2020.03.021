                                 </mml:mfrac>
                                 <mml:mfrac>
                                    <mml:mi>i</mml:mi>
                                    <mml:mi>b</mml:mi>
                                 </mml:mfrac>
                                 <mml:mo linebreak="badbreak">=</mml:mo>
                                 <mml:mi>λ</mml:mi>
                                 <mml:mspace width="0.25em"/>
