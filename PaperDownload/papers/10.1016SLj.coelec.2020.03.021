                     <ce:sup loc="post">1</ce:sup>
                  </ce:cross-ref>
               </ce:author>
               <ce:author id="au2" author-id="S2451910320300764-d17e23546134ad37de39c0ba75a248b5">
                  <ce:given-name>Brian P.</ce:given-name>
